{
  "name": "spy-client",
  "version": "2.0.3",
  "description": "spy client",
  "main": "dist/spy-client.js",
  "module": "dist/spy-client.esm.js",
  "typings": "dist/spy-client.d.ts",
  "scripts": {
    "w_dev": "rm -fr dist && NODE_ENV=development webpack --config webpack.dev.config.ts",
    "w_watch": "rm -fr dist && NODE_ENV=development webpack -w --config webpack.dev.config.ts",
    "w_build": "rm -fr dist && NODE_ENV=production webpack --config webpack.prod.config.ts",
    "dev": "rollup -c --environment NODE_ENV:development",
    "watch": "NODE_OPTIONS='--max-old-space-size=4096' rollup -cw --environment NODE_ENV:development",
    "build": "rollup -c --environment NODE_ENV:production",
    "lint": "eslint src/**/*.ts",
    "example": "npm run dev && echo 'open url http://localhost:8000/example' && python -m SimpleHTTPServer 8000 ",
    "test": "karma start karma.conf.ts",
    "release_pre": "npm version patch",
    "release": "rm -fr dist && npm run build && npm run lint && npm run test && npm publish",
    "release_post": "git push origin master && git push origin --tags"
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "dependencies": {
    "core-js": "^3.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@ecomfe/eslint-config": "^3.2.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^4.0.0",
    "@types/glob": "^7.1.1",
    "@types/jasmine": "^3.5.10",
    "@types/node": "^12.6.8",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.4.35",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "cache-loader": "^4.1.0",
    "css-loader": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.1",
    "fecs": "^1.6.4",
    "file-loader": "^4.1.0",
    "jasmine-core": "^3.4.0",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-webpack": "^4.0.2",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "rollup": "^2.2.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "rollup-plugin-uglify": "^6.0.4",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1"
  },
  "keywords": [
    "spy",
    "client",
    "Longtask",
    "frontend monitor",
    "performance monitor",
    "exception monitor",
    "jsError",
    "whiteScreenError",
    "resourceError",
    "performance timing",
    "Largest Contentful Paint",
    "FID",
    "TTI",
    "Cumulative Layout Shift",
    "Memory",
    "Resource Timing"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "kaivean",
  "license": "ISC",
  "babel": {
    "comments": false,
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 8",
    "android >= 2.3",
    "ios >= 7"
  ]
}
