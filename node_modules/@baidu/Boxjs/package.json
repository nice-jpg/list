{

  "name": "@baidu/Boxjs",
  "version": "1.2.91",
  "description": "手百端能力sdk",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "apidoc": "^0.17.6",
    "eslint": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-jasmine": "^2.2.0",
    "fs-then-native": "^2.0.0",
    "gitbook-cli": "^2.3.0",
    "gulp": "^3.9.1",
    "jasmine": "^2.6.0",
    "jsdoc-to-markdown": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "baidu-invoke": "git+ssh://git@icode.baidu.com:8235/baidu/reactnative/baidu-invoke#v0.5.1",
    "fis3-deploy-replace": "^1.0.2",
    "fis-postprocessor-jswrapperwithdeps": "0.0.14",
    "fis3-packager-deps-pack": "^0.1.2"
  },
  "scripts": {
    "docshook": "cp .git/hooks/pre-push.sample .git/hooks/pre-push && echo '#!/bin/sh\nsh buildDocs.sh' > ./.git/hooks/pre-push",
    "start": "gulp start",
    "gulp": "gulp",
    "test": "jasmine",
    "docs:prepare": "gitbook install",
    "docs:api": "jsdoc2md --configure tooling/docs/jsdoc.json --files src/components/**/*.js > docs/api.md",
    "docs:build": "npm run docs:prepare && gitbook build ./docs ./_book/dist/jssdk",
    "docs:serve": "npm run docs:prepare && gitbook serve ./docs ./_book/dist/jssdk",
    "docs:clean": "rimraf _book",
    "apiDoc": "apidoc -i src/components/ -o _book/dist/jssdk/apidoc/ -t third_party/template/",
    "lint": "eslint src build test"
  },
  "repository": {
    "type": "git",
    "url": "ssh://gaojiexuan@icode.baidu.com:8235/baidu/search-fe/jssdk"
  },
  "keywords": [
    "openAPI",
    "jssdk",
    "baidumobile"
  ],
  "author": "renzhonghua",
  "license": "ISC",
  "directories": {
    "doc": "docs",
    "test": "test"
  }
}
