{
  "name": "@baidu/spy",
  "version": "1.0.2",
  "description": "spy log",
  "main": "dist/spy.js",
  "module": "dist/spy.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "dev": "rm -fr dist && NODE_ENV=development webpack --config webpack.dev.config.ts",
    "watch": "rm -fr dist && NODE_ENV=development webpack -w --config webpack.dev.config.ts",
    "build": "rm -fr dist && NODE_ENV=production webpack --config webpack.prod.config.ts",
    "lint": "tslint --project . && fecs",
    "example": "npm run dev && echo 'open url http://localhost:8000/example' && python -m SimpleHTTPServer 8000 ",
    "test": "karma start karma.conf.ts",
    "release_pre": "npm version patch",
    "release": "npm run build &&  npm publish --registry=http://registry.npm.baidu-int.com",
    "release_post": "git push origin --tags  && git push origin HEAD:refs/for/master"
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@types/glob": "^7.1.1",
    "@types/jasmine": "^3.4.0",
    "@types/mini-css-extract-plugin": "^0.2.1",
    "@types/node": "^12.6.8",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.4",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.4.35",
    "@types/webpack-merge": "^4.1.5",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-replace-object-assign": "^2.0.0",
    "cache-loader": "^4.1.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.1.0",
    "eslint": "^6.0.1",
    "fecs": "^1.6.4",
    "file-loader": "^4.1.0",
    "jasmine-core": "^3.4.0",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-typescript": "^4.1.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.6.2",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1"
  },
  "keywords": [
    "spy",
    "log"
  ],
  "author": "kaivean",
  "license": "ISC",
  "babel": {
    "plugins": [
      "transform-replace-object-assign"
    ],
    "comments": false,
    "presets": [
      [
        "@babel/preset-env"
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  }
}
