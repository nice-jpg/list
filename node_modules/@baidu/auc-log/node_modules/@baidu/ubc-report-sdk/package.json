{
    "name": "@baidu/ubc-report-sdk",
    "version": "2.1.1",
    "description": "A sdk for reporting use bahavior based on UBC system",
    "module": "dist/ubc-web-sdk.esm.js",
    "main": "dist/ubc-web-sdk.esm.js",
    "scripts": {
        "build": "rollup --config ./tool/rollup.config.prod.js",
        "dev": "npm-run-all --parallel dev-example dev-server",
        "dev-example": "rollup --config ./tool/rollup.config.dev.js -w",
        "dev-build": "rollup --config ./tool/rollup.config.dev.js",
        "dev-server": "http-server -p 8081",
        "runJest": "jest --config ./jest.config.js  --watchAll",
        "testAll": "npm-run-all build dev-build --parallel dev-server runJest",
        "test": "cross-env NODE_ENV=test:unit  jest  --coverage"
    },
    "author": "wangbing11@baidu.com",
    "license": "ISC",
    "dependencies": {
        "babel-polyfill": "^6.26.0"
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^23.6.0",
        "cross-env": "^7.0.1",
        "express": "^4.17.1",
        "glob": "^7.1.6",
        "http-server": "^0.11.1",
        "jest": "^23.6.0",
        "npm-run-all": "^4.1.5",
        "regenerator-runtime": "^0.12.1",
        "rollup": "^0.65.0",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-commonjs": "^9.3.4",
        "rollup-plugin-eslint": "^5.1.0",
        "rollup-plugin-node-resolve": "^3.4.0",
        "rollup-plugin-terser": "^2.0.2"
    },
    "files": [
        "dist",
        "src"
    ]
}
