{
  "name": "@baidu/ubc-report-hn-sdk",
  "version": "1.0.4",
  "description": "h5日志打点库-hn版",
  "main": "dist/ubc-report-sdk.esm.js",
  "files": [
    "src",
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "ubc",
    "hn"
  ],
  "scripts": {
    "build": "rollup -c ./scripts/rollup.config.prod.js",
    "watch": "rollup -c ./scripts/rollup.config.prod.js -w",
    "dev": "npm-run-all --parallel start watch example",
    "start": "serve  -l 8081 -c ./scripts/serve.json",
    "example": "rollup -c ./scripts/rollup.config.dev.js -w",
    "test": "cross-env NODE_ENV=test:unit  jest  --coverage",
    "docs": "jsdoc2md -t ./docs/README.hbs src/UbcLogger.js > README.md",
    "lint": "eslint --ext .js src/  --ignore-path .gitignore",
    "lint:fix": "eslint --fix --ext .js src/  --ignore-path .gitignore",
    "pretest": "npm run build"
  },
  "lint-staged": {
    "linters": {
      "*.{js}": [
        "eslint"
      ]
    },
    "ignore": [
      "dist/**/*",
      "example/**/*"
    ]
  },
  "repository": {
    "type": "git"
  },
  "author": "changyuqing@baidu.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/eslint-plugin": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.12.11",
    "@ecomfe/eslint-config": "^7.0.0",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "jsdoc-to-markdown": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.34.2",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^11.3.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "lodash.merge": "^4.6.2"
  }
}
