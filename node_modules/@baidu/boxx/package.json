        "vuepress": "^1.0.2"
    },
    "files": [
        "dist/*",
        "*.md"
    ],
    "scripts": {
        "start": "npm run dev",
        "dev": "rollup --config build/rollup.config.dev.js -w --environment NODE_ENV:development ",
        "dev:mock": "rollup --config build/rollup.config.dev.js -w --environment NODE_ENV:development-mock ",
        "build": "node build/build.js",
        "deploy": "npm run build && fis3 release dev",
        "deploy:test": "npm run build && fis3 release test",
        "deploy:doc": "vuepress build doc && fis3 release doc",
        "doc": "vuepress dev doc",
        "doc:build": "vuepress build doc",
        "upload": "npm run build && node scripts/upload.js",
        "test": "NODE_ENV=test jest --ci",
        "test:dev": "NODE_ENV=test jest --watchAll",
        "test:update-snapshot": "NODE_ENV=test jest --updateSnapshot",
        "prepublishOnly": "sfe-publish-check && npm run build",
        "sfepublish": "sfe-publish",
        "lint-staged": "lint-staged",
        "updateAdapteInfo": "node build/getAdapteInfo.js"
    },
    "repository": {
        "type": "git",
