{
    "name": "@baidu/mcp-sdk",
    "version": "1.15.2",
    "description": "UG invoke",
    "main": "dist/index.js",
    "files": [
        "src",
        "dist",
        "README.md"
    ],
    "scripts": {
        "lint": "eslint  test/**/**.js src/** --ignore-path .gitignore",
        "build": "rollup -c",
        "build:apm": "cross-env NODE_ENV=build:apm rollup -c rollup.apm.config.js",
        "prepublishOnly": "npm run build && npm run build:apm",
        "test:e2e": "rollup -c rollup.e2e.config.js -w",
        "test:apm": "cross-env NODE_ENV=build:apm rollup -c rollup.apm.test.config.js",
        "test": "cross-env NODE_ENV=test:unit  jest  --coverage",
        "commit": "git-cz",
        "version": "standard-version",
        "release": "git push --follow-tags && npm publish"
    },
    "repository": {
        "type": "git"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-conventional-changelog"
        }
    },
    "lint-staged": {
        "*.(js)": "npm run lint"
    },
    "author": "zhangsiyuan",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@babel/plugin-proposal-async-generator-functions": "^7.2.0",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
        "@babel/plugin-proposal-optional-chaining": "^7.2.0",
        "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
        "@babel/plugin-transform-async-to-generator": "^7.8.3",
        "@babel/plugin-transform-modules-commonjs": "^7.8.3",
        "@babel/preset-env": "^7.9.5",
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "@ecomfe/eslint-config": "^2.1.0",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^17.1.0",
        "@rollup/plugin-node-resolve": "^11.2.0",
        "@rollup/plugin-replace": "^2.3.2",
        "@types/jest": "^25.2.3",
        "babel-eslint": "^11.0.0-beta.0",
        "commitizen": "^4.0.3",
        "core-js": "^3.6.5",
        "cross-env": "^7.0.1",
        "cz-conventional-changelog": "^3.1.0",
        "eslint": "^6.1.0",
        "eslint-plugin-babel": "^5.3.0",
        "husky": "^1.3.1",
        "internal-ip": "^3.0.1",
        "jest": "^25.1.0",
        "lint-staged": "^9.2.1",
        "lodash": "^4.17.15",
        "rollup": "^2.41.2",
        "rollup-plugin-serve": "^1.0.1",
        "rollup-plugin-terser": "^5.3.0",
        "san": "^3.8.1",
        "standard-version": "^7.1.0"
    },
    "dependencies": {
        "@baidu/ubc-report-sdk": "^3.0.3",
        "@baidu/ug-invoke-app": "2.3.5",
        "expired-storage": "^1.0.2",
        "flyio": "^0.6.14",
        "lodash.isstring": "^4.0.1"
    }
