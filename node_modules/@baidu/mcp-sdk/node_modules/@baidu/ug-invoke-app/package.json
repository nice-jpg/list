{
    "name": "@baidu/ug-invoke-app",
    "version": "2.3.5",
    "description": "UG invoke",
    "main": "dist/index.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "src",
        "dist",
        "README.md"
    ],
    "scripts": {
        "lint": " tsc && eslint ./*.js test/unit/**/*.js test/e2e/**/*.js src/** --ignore-path .gitignore",
        "build": "tsc && rollup -c && npm run build:es5 && npm run build:single",
        "build:es5": "cross-env NODE_ENV=build:es5 rollup -c rollup.es5.config.js",
        "build:single": "rollup -c rollup.singleFile.config.js",
        "prepublishOnly": "npm run build",
        "test:e2e": "rollup -c rollup.e2e.config.js -w",
        "test": "cross-env NODE_ENV=test:unit  jest  --coverage"
    },
    "repository": {
        "type": "git"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.(js|ts)": "npm run lint"
    },
    "author": "zhangsiyuan",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/eslint-parser": "^7.12.1",
        "@babel/eslint-plugin": "^7.12.1",
        "@babel/plugin-proposal-async-generator-functions": "^7.12.12",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
        "@babel/plugin-proposal-optional-chaining": "^7.12.7",
        "@babel/plugin-proposal-pipeline-operator": "^7.12.1",
        "@babel/plugin-transform-modules-commonjs": "^7.12.1",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-typescript": "^7.12.7",
        "@ecomfe/eslint-config": "^7.0.0",
        "@rollup/plugin-babel": "^5.2.2",
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.0.1",
        "@rollup/plugin-replace": "^2.3.4",
        "@rollup/plugin-strip": "^2.0.0",
        "@types/jest": "^25.2.3",
        "@typescript-eslint/eslint-plugin": "^4.12.0",
        "@typescript-eslint/parser": "^4.12.0",
        "babel-eslint": "^11.0.0-beta.2",
        "babel-plugin-const-enum": "^1.0.1",
        "core-js": "^3.8.2",
        "cross-env": "^7.0.3",
        "eslint": "^7.17.0",
        "eslint-plugin-babel": "^5.3.1",
        "husky": "^1.3.1",
        "internal-ip": "^3.0.1",
        "jest": "^25.5.4",
        "lint-staged": "^9.5.0",
        "rollup": "^2.36.1",
        "rollup-plugin-serve": "^1.1.0",
        "rollup-plugin-terser": "^7.0.2",
        "san": "^3.10.1",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "@baidu/eop-utils": "0.1.2",
        "@baidu/ug-error-log": "^2.2.1",
        "clipboard": "2.0.8",
        "query-string": "^6.13.8",
        "url-parse": "^1.4.7"
    }
}
