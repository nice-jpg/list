import styles from './app.module.less';\nimport TopCity from '../components/topCity'\nimport MapChart from '../components/mapChart';\nimport axios from 'axios';\n// const mock = require('./mock.json')\nimport ubc from '@baidu/ubc-report-sdk';\n\nexport default class App extends Component {\n    static components = {\n        'map-chart': MapChart,\n        'top-city': TopCity\n    };\n    /* eslint-disable max-len */\n    static template = `\n        <div\n            id=\"appContain\"\n            class=\"{{styles['app']}}\"\n        >\n            <div class=\"{{styles['app-container']}}\">\n                <div class=\"{{styles['title']}}\">疫情指数分布</div>\n                <div class=\"{{styles['tab-level1']}}\">\n                    <div\n                        s-for=\"name, index in tabNames\"\n                        class=\"{{tabSelected === index ? styles['select'] : ''}}\"\n                        on-click=\"tabClick(index)\"\n                    >{{name}}</div>\n                </div>\n                <div class=\"{{styles['tab-level2']}}\">\n                    <div\n                        s-for=\"subName, index in subTabNames\"\n                        class=\"{{subTabSelected === index ? styles['select'] : ''}}\"\n                        on-click=\"subTabClick(index)\"\n                    >{{subName}}</div>\n                </div>\n                <div\n                    s-if=\"{{topCityData}}\"\n                >\n                    <top-city\n                        topCityData=\"{{topCityData}}\"\n                        subTabSelected=\"{{subTabSelected}}\"\n                        updateTime=\"{{updateTime}}\"\n                        isIOS=\"{{IS_IOS}}\"\n                    />\n                </div>\n                <map-chart\n                    s-if=\"tabSelected === 0 && mapData\"\n                    subTabSelected=\"{{subTabSelected}}\"\n                    mapData=\"{{mapData}}\"\n                />\n            </div>\n        </div>\n    `;\n    /* eslint-enable max-len */\n    static computed = {\n        tabNames() {\n            const itemData = this.data.get('itemData');\n            return (itemData || this.data.get('names')).map(item => {\n                return item.name || '';\n            });\n        },\n        mapData() {\n            const itemData = this.data.get('itemData');\n            return itemData && itemData[0] && itemData[0]['data'];\n        },\n        topCityData() {\n            const tabSelected = this.data.get('tabSelected');\n            const topCity = this.data.get('itemData[1].data');\n            if (tabSelected === 0 || !topCity) {\n                return '';\n            }\n            const subTabSelected = this.data.get('subTabSelected');\n            const props = subTabSelected === 0 ? 'search' : 'inquiry';\n            const currentCity = topCity[`${props}_list`] || [];\n            let result = [];\n            let count = 5;\n            const resultLen = Math.ceil(currentCity.length / count);\n            const maxValue = currentCity[0][`${props}_dau`];\n            for (let i = 0; i < resultLen; i++) {\n                let resultItem = [];\n                for (let j = 0; j < count; j++) {\n                    let data = currentCity[i * 5 + j];\n                    if (!data) {\n                        continue;\n                    }\n                    const trend = 'fair';\n                    const rate = data[`${props}_rate`] || '';\n                    rate.indexOf('+') !== -1 && (trend = 'up');\n                    rate.indexOf('-') !== -1 && (trend = 'down');\n                    resultItem.push({\n                        area: data.area,\n                        num: data[`${props}_dau`],\n                        rate: `${rate}`.slice(1),\n                        trend,\n                        precent: (data[`${props}_dau`] / maxValue * 100).toFixed(1) + '%',\n                    })\n                }\n                result.push(resultItem);\n            }\n            return result;\n        },\n        updateTime() {\n            const topCity = this.data.get('itemData[1].data');\n            if (!topCity) {\n                return '';\n            }\n            const subTabSelected = this.data.get('subTabSelected');\n            const props = subTabSelected === 0 ? 'search' : 'inquiry';\n            const currentCity = topCity[`${props}_list`] || [];\n            return currentCity[0] && currentCity[0]['update_time'] || ''\n        }\n    }\n    initLog() {\n        ubc('init', '4172', {\n            version: '2.0',\n            timingId: '11656',\n            page: 'COVID_map_card',\n            from: 'feed',\n            source: 'feed',\n            queryParams: {\n                appname: 'baiduboxapp'\n            }\n        });\n    }\n\n    inited() {\n        this.initLog();\n    }\n\n    created() {\n        // 创建时从url上读取主题并设置html的样式\n        let paramsFromUrl = window.location.href;\n        let themeIndex = paramsFromUrl.indexOf('&theme=');\n        let paramEnd = paramsFromUrl.indexOf('&timeStamp') === -1\n            ? paramsFromUrl.length : paramsFromUrl.indexOf('&timeStamp');\n        let theme = paramsFromUrl.slice(themeIndex + 1, paramEnd).split('=')[1];\n        const $root = document.getElementsByTagName('html')[0];\n        $root.classList.add(theme);\n        this.data.set('theme', theme);\n        // 设置字体\n        if (/iphone|ipad|ipod|ios/ig.test(paramsFromUrl)) {\n            $root.classList.add('iphone');\n            this.data.set('IS_IOS', true);\n        } else {\n            $root.classList.add('android');\n            this.data.set('IS_IOS', false);\n        }\n        if (window.sortsData.error !== 0) {\n            window.ReactNativeWebView && window.ReactNativeWebView.postMessage(0);\n        } else {\n            this.data.set('itemData', window.sortsData.data);\n        }\n        // this.data.set('itemData', mock);\n    }\n    attached() {\n        // iOS\n        window.addEventListener('message', e => {\n            if (!e.data || typeof e.data !== 'string' || typeof e.data === 'string' && e.data.indexOf('HN') === -1) {\n                return;\n            }\n            let dataFromHN = JSON.parse(e.data);\n            if (dataFromHN.from !== 'HN') {\n                return;\n            }\n            this.setTheme(dataFromHN);\n            this.refresh(dataFromHN);\n        });\n        // android\n        document.addEventListener('message', e => {\n            let dataFromHN = JSON.parse(e.data);\n            if (dataFromHN.from !== 'HN') {\n                return;\n            }\n            this.setTheme(dataFromHN);\n            this.refresh(dataFromHN);\n        });\n        // 设置webview高度\n        this.nextTick(() => {\n            let height = document.getElementById('appContain').scrollHeight;\n            window.ReactNativeWebView && window.ReactNativeWebView.postMessage(height);\n        });\n        // 展现打点\n        ubc('event', {\n            type: 'show',\n            value: 'COVID_map_card'\n        });\n    }\n\n    initData() {\n        return {\n            styles,\n            tabSelected: 0,\n            subTabSelected: 0,\n            names: [{name: '地图'}, {name: 'TOP城市'}],\n            subTabNames: ['疫情搜索比例', '健康问诊比例'],\n            pageSelected: 0\n        };\n    }\n    setTheme(dataFromChannel) {\n        if (dataFromChannel.theme) {\n            // 频道改变主题\n            const theme = `virusmap-${dataFromChannel.theme}`;\n            const IS_IOS = this.data.get('IS_IOS');\n            const $root = document.getElementsByTagName('html')[0];\n            $root.className = `${theme} ${IS_IOS ? 'iphone' : 'android'}`;\n            this.data.set('theme', theme);\n        }\n    }\n    refresh(dataFromChannel) {\n        if (dataFromChannel.operate === 'refresh') {\n            axios.get('/newspage/data/virusmap', {\n                params: {\n                    _format: 'json'\n                }\n            }).then(res => {\n                if (res.status === 200 && res.data && res.data.error === 0) {\n                    const newData = res.data.data;\n                    this.data.set('itemData', newData);\n                    this.nextTick(() => {\n                        let height = document.getElementById('appContain').scrollHeight;\n                        window.ReactNativeWebView && window.ReactNativeWebView.postMessage(height - 1);\n                    });\n                }\n            }).catch(() => {\n            });\n        }\n    }\n    tabClick(index) {\n        this.data.set('tabSelected', index);\n        this.nextTick(() => {\n            // 加一下延时，不然算出来的高度是上一个tab的\n            let height = document.getElementById('appContain').scrollHeight;\n            // 这里减一是因为ios底部发现会有1px的线\n            window.ReactNativeWebView && window.ReactNativeWebView.postMessage(height - 1);\n        });\n        ubc('event', {\n            type: 'clk',\n            value: index === 0 ? 'map_tab': 'top_city_tab',\n        });\n    }\n    subTabClick(index) {\n        this.data.set('subTabSelected', index);\n        const tabSelected = this.data.get('tabSelected');\n        const valueConfig = [\n            ['COVID_search_map', 'health_ask_map'],\n            ['COVID_search_city', 'health_ask_city'],\n        ]\n        ubc('event', {\n            type: 'clk',\n            value: valueConfig[tabSelected][index],\n        });\n    }\n}\n"]}]}