AACwC,cAAL,CAAoBC,OADX,IAETzC,IAAI,CAACwC,cAAL,CAAoBC,OAApB,CAA4BC,MAA5B,GAAqC,CAF5C;AAGA,aAAO1C,IAAP;AACH,KALM,CAAP;AAMH,GAda;AAedD,EAAAA,WAfc,yBAeA;AACV,QAAMoB,SAAS,GAAG,KAAKf,IAAL,CAAUC,GAAV,CAAc,YAAd,KAA+B,EAAjD;AACA,QAAMsC,kBAAkB,GAAG,KAAKvC,IAAL,CAAUC,GAAV,CAAc,yBAAd,CAA3B;;AACA,QAAIc,SAAS,CAACuB,MAAV,GAAmB,CAAnB,IAAwB,CAACC,kBAA7B,EAAiD;AAC7C,aAAO,IAAP;AACH;;AACD,WAAO,KAAP;AACH,GAtBa;AAuBdxC,EAAAA,QAvBc,sBAuBH;AACP,QAAMgB,SAAS,GAAG,KAAKf,IAAL,CAAUC,GAAV,CAAc,WAAd,CAAlB;AACA,QAAMuC,iBAAiB,GAAGzB,SAAS,CAAC0B,SAAV,CAAoB,UAAA7C,IAAI;AAAA,aAAI,CAAC,CAACA,IAAI,CAACE,QAAX;AAAA,KAAxB,CAA1B,CAFO,CAGP;;AACA,QAAI0C,iBAAiB,GAAG,CAAxB,EAA2B;AACvB,aAAO,EAAP;AACH;;AACD,6CACKA,iBADL,EACyB,IADzB;AAGH;AAjCa,C;;AAjFlB,IAAIE,MAAM,CAACC,GAAX,EAAgB;AACZ,MAAIC,WAAW,GAAGC,OAAO,CAAC,+HAAD,CAAzB;;AACA,MAAIC,aAAa,GAAGD,OAAO,CAAC,gHAAD,CAA3B;;AAEA,MAAIE,iBAAiB,GAAGD,aAAa,CAACE,UAAd,CAAyBN,MAAzB,CAAxB;;AACA,MAAIK,iBAAiB,CAACE,QAAlB,IAA8BF,iBAAiB,CAACG,SAAlB,CAA4BD,QAA9D,EAAwE;AACpEP,IAAAA,MAAM,CAACC,GAAP,CAAWQ,MAAX;;AACAP,IAAAA,WAAW,CAACQ,OAAZ,CAAoBP,OAAO,CAAC,KAAD,CAA3B;;AAEA,QAAIQ,UAAU,GAAG,UAAjB;;AACA,QAAI,CAACX,MAAM,CAACC,GAAP,CAAW3C,IAAhB,EAAsB;AAClB4C,MAAAA,WAAW,CAACU,YAAZ,CAAyBD,UAAzB,EAAqCN,iBAArC;AACH,KAFD,MAGK;AACDH,MAAAA,WAAW,CAACW,SAAZ,CAAsBF,UAAtB,EAAkCN,iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * @file 城市疫情速报组件\n * @author shiqinqin@baidu.com\n */\n\nimport {Component} from 'san';\nimport ubc from '@baidu/ubc-report-sdk';\nimport s from './reportTable.module.less';\nimport invokeScheme from '../../utils/invokeScheme';\nimport DangerousArea from '../dangerousArea';\nexport default class ReportTable extends Component {\n    static template = `\n        <div class=\"{{s['reportTable']}}\" s-if=\"bodyItems.length\">\n            <div>\n                <span class=\"{{s['reportTable-title']}}\">疫情速报</span>\n                <div class=\"{{s['reportTable-tips']}}\">\n                    <span>该数据为31省（自治区、直辖市）本土新增，及港澳台新增确诊数据</span>\n                </div>\n            </div>\n            <div class=\"{{s['reportTable-tablebox']}}\" style=\"{{isRowHidden ? '' : 'padding-bottom: 0.25rem;'}}\">\n                <table class=\"{{s['reportTable-table']}}\" border=\"0\" cellspacing=\"0\" cellpadding=\"0\">\n                    <thead class=\"{{s['reportTable-thead']}}\">\n                        <tr>\n                            <th s-for=\"headItem, index in headItems\"\n                                style=\"{{index > 0\n                                    ? index === 2 ? 'width: 30%;': 'width: 19%;' \n                                    : ''}}\">{{headItem}}</th>\n                        </tr>\n                    </thead>\n                    <tbody class=\"{{s['reportTable-tbody']}}\">\n                        <fragment s-for=\"bodyRow, index in bodyItems\">\n                            <tr\n                                class=\"{{(isRowHidden && index === 4)||(index === bodyItems.length - 1) || showArea[index]\n                                    ? '' : s['reportTable-row']}}\">\n                                <td class=\"{{s['reportTable-tbody-bodyRowItem']}} {{s['reportTable-tbody-firstRowItem']}}\"\n                                    on-click=\"handleFirstRowItem($event, index, bodyRow)\">\n                                    <div class=\"{{s['reportTable-citybox']}}\">\n                                        <span class=\"{{s['reportTable-city']}}\">{{bodyRow.city}}</span>\n                                    </div>\n                                    <div s-if=\"bodyRow.province\" class=\"{{s['reportTable-provincebox']}}\">\n                                        <div class=\"{{s['reportTable-provinceinnerbox']}}\">\n                                            <span class=\"{{s['reportTable-province']}}\">{{bodyRow.province}}</span>\n                                        </div>\n                                    </div>\n                                    <div class=\"{{s['reportTable-arrowbox']}}\">\n                                        <span class=\"{{s['reportTable-arrow']}}\"></span>\n                                    </div>\n                                </td>\n                                <td class=\"{{s['reportTable-tbody-bodyRowItem']}}\">{{bodyRow.nativeRelative || '-'}}</td>\n                                <td class=\"{{s['reportTable-tbody-bodyRowItem']}}\">{{bodyRow.asymptomaticRelative || '-'}}</td>\n                                <td class=\"{{s['reportTable-tbody-bodyRowItem']}}\"\n                                    style=\"position: relative;\"\n                                    on-click=\"areaClick(bodyRow, index)\"\n                                >\n                                    <span>{{bodyRow.isDanger ? bodyRow.dangerousAreas.totalNum : 0}}</span>\n                                    <div s-if=\"{{bodyRow.isDanger}}\"\n                                        class=\"{{s['reportTable-triangle']}} {{showArea[index] ? s['rotate'] : ''}}\"\n                                    ></div>\n                                </td>\n                            </tr>\n                            <tr s-if=\"{{bodyRow.isDanger && showArea[index]}}\">\n                                <td style=\"padding: 0;\" colSpan=\"{{4}}\">\n                                    <dangerous-area areaData=\"{{bodyRow.dangerousAreas}}\"/>\n                                </td>\n                            </tr>\n                        </fragment>\n                    </tbody>\n                </table>\n                <div\n                    s-if=\"{{isRowHidden}}\"\n                    class=\"{{s['reportTable-collapse-button']}}\"\n                    on-click=\"onButtonClick\">\n                    <span class=\"{{s['reportTable-collapse-buttontext']}}\">展开全部</span>\n                </div>\n            </div>\n        </div>\n    `;\n\n    static components = {\n        'dangerous-area': DangerousArea\n    }\n\n    static computed = {\n        headItems() {\n            return ['地区', '新增本土', '新增本土无症状', '风险地区'];\n        },\n        bodyItems() {\n            const feedItems = this.data.get('feed.items') || [];\n            const isRowHidden = this.data.get('isRowHidden');\n            const tableData = isRowHidden ? feedItems.slice(0, 5) : feedItems;\n            return tableData.map(item => {\n                item.isDanger = item.dangerousAreas\n                    && item.dangerousAreas.subList\n                    && item.dangerousAreas.subList.length > 0;\n                return item;\n            });\n        },\n        isRowHidden() {\n            const bodyItems = this.data.get('feed.items') || [];\n            const reportTableHasShow = this.data.get('feed.reportTableHasShow');\n            if (bodyItems.length > 5 && !reportTableHasShow) {\n                return true;\n            }\n            return false;\n        },\n        showArea() {\n            const bodyItems = this.data.get('bodyItems');\n            const showAreaCityIndex = bodyItems.findIndex(item => !!item.isDanger);\n            // 如果没有包含风险地区，或者默认第一位的风险地区被操作收起\n            if (showAreaCityIndex < 0) {\n                return {};\n            }\n            return {\n                [showAreaCityIndex]: true\n            }\n        },\n    };\n\n    initData() {\n        return {\n            s,\n            isRowHidden: false\n        };\n    }\n    areaClick(item, index) {\n        if (!item.isDanger) {\n            return;\n        }\n        const showArea = this.data.get('showArea');\n        const lastStatus = showArea[index];\n        showArea[index] = !lastStatus;\n        this.data.set('showArea', {\n            ...showArea,\n            [index]: !lastStatus\n        });\n\n        this.nextTick(() => {\n            let height = document.getElementById('appContain').scrollHeight;\n            window.ReactNativeWebView && window.ReactNativeWebView.postMessage(height);\n        });\n    }\n\n    handleFirstRowItem(e, index, bodyRow) {\n        e.stopPropagation();\n        const bodyItems = this.data.get('bodyItems');\n        const cmd = bodyItems && bodyItems[index] && bodyItems[index].link;\n        if (cmd) {\n            let schemeHead = 'baiduboxapp://v1/browser/open?url=';\n            let couponUrl = schemeHead + encodeURIComponent(cmd);\n            invokeScheme(couponUrl);\n        }\n        // 点击table每一行表头的打点\n        ubc('event', {\n            page: 'domestic',\n            type: 'clk',\n            value: 'report',\n            ext: {\n                pos: index,\n                headInfo: bodyRow,\n                isShowHidden: false\n            }\n        });\n    }\n\n    onButtonClick() {\n        this.data.set('isRowHidden', false);\n        this.dispatch('report:reportTableHasShow');\n        ubc('event', {\n            page: 'domestic',\n            type: 'clk',\n            value: 'report',\n            ext: {\n                isShowHidden: true\n            }\n        });\n        this.nextTick(() => {\n            // 加一下延时，不然算出来的高度是上一个tab的\n            let height = document.getElementById('appContain').scrollHeight;\n            window.ReactNativeWebView && window.ReactNativeWebView.postMessage(height);\n        });\n    }\n}"]}]}