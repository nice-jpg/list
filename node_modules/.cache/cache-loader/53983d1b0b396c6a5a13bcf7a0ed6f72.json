KAAK,GAAG,KAAKF,IAAL,CAAUC,GAAV,CAAc,OAAd,CAAd;AACA,WAAKG,KAAL,CAAWC,KAAX,CAAiBH,KAAjB,IAA0B,IAA1B;AACH;;;AA1DD;wBA0Ba;AACT,UAAIS,GAAG,GAAG,EAAV;AACA,UAAMC,KAAK,GAAG,KAAKC,GAAL,CAAS,MAAT,CAAd;;AACA,UAAID,KAAJ,EAAW;AACPD,QAAAA,GAAG,CAACG,KAAJ,GAAYF,KAAK,CAACG,WAAlB;AACAJ,QAAAA,GAAG,CAACK,IAAJ,GAAWJ,KAAK,CAACK,UAAjB;AACH;;AAED,aAAON,GAAP;AACH;;;EApC4BO,c;;;8BAAZnB,G;AAEC;;8BAFDA,G,eAQE;AACfU,EAAAA,MAAM,EAAEU,eAAUC,IADH;AAEfC,EAAAA,QAAQ,EAAEF,eAAUC,IAFL;AAGfE,EAAAA,IAAI,EAAEH,eAAUI,MAHD;AAIfpB,EAAAA,IAAI,EAAEgB,eAAUC;AAJD,C;8BARFrB,G,cAeC;AACdyB,EAAAA,QADc,sBACH;AACP,QAAMf,MAAM,GAAG,KAAKT,IAAL,CAAUC,GAAV,CAAc,QAAd,CAAf;AACA,QAAMoB,QAAQ,GAAG,KAAKrB,IAAL,CAAUC,GAAV,CAAc,UAAd,CAAjB;AACA,WAAO,CACHH,SADG,EAEHW,MAAM,cAAOX,SAAP,YAFH,EAGHuB,QAAQ,cAAOvB,SAAP,cAHL,EAIL2B,MAJK,CAIE,UAAAC,CAAC;AAAA,aAAIA,CAAJ;AAAA,KAJH,CAAP;AAKH;AATa,C;;AAvBlB,IAAIC,MAAM,CAACC,GAAX,EAAgB;AACZ,MAAIC,WAAW,GAAGC,OAAO,CAAC,+HAAD,CAAzB;;AACA,MAAIC,aAAa,GAAGD,OAAO,CAAC,gHAAD,CAA3B;;AAEA,MAAIE,iBAAiB,GAAGD,aAAa,CAACE,UAAd,CAAyBN,MAAzB,CAAxB;;AACA,MAAIK,iBAAiB,CAACE,QAAlB,IAA8BF,iBAAiB,CAACG,SAAlB,CAA4BD,QAA9D,EAAwE;AACpEP,IAAAA,MAAM,CAACC,GAAP,CAAWQ,MAAX;;AACAP,IAAAA,WAAW,CAACQ,OAAZ,CAAoBP,OAAO,CAAC,KAAD,CAA3B;;AAEA,QAAIQ,UAAU,GAAG,UAAjB;;AACA,QAAI,CAACX,MAAM,CAACC,GAAP,CAAW5B,IAAhB,EAAsB;AAClB6B,MAAAA,WAAW,CAACU,YAAZ,CAAyBD,UAAzB,EAAqCN,iBAArC;AACH,KAFD,MAGK;AACDH,MAAAA,WAAW,CAACW,SAAZ,CAAsBF,UAAtB,EAAkCN,iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * @file 组件 tab\n * @author baozhixin <baozhixin@baidu.com>\n */\n\nimport {Component, DataTypes} from 'san';\n\nconst prefixCls = 's-tab';\n\nexport default class Tab extends Component {\n    // eslint-disable-next-line\n    static template = /*html*/ `\n        <div class=\"{{tabClass}}\">\n            <span s-ref=\"node\">{{name}}</span>\n        </div>\n    `;\n\n    static dataTypes = {\n        active: DataTypes.bool,\n        disabled: DataTypes.bool,\n        name: DataTypes.string,\n        last: DataTypes.bool\n    };\n\n    static computed = {\n        tabClass() {\n            const active = this.data.get('active');\n            const disabled = this.data.get('disabled');\n            return [\n                prefixCls,\n                active && `${prefixCls}-active`,\n                disabled && `${prefixCls}-disabled`\n            ].filter(c => c);\n        }\n    };\n\n    get offset() {\n        let ret = {};\n        const $node = this.ref('node');\n        if ($node) {\n            ret.width = $node.offsetWidth;\n            ret.left = $node.offsetLeft;\n        }\n\n        return ret;\n    }\n\n    attached() {\n        const {index, last} = this.data.get();\n        const owner = this.owner;\n        owner.$tabs[index] = this;\n\n        // 最后一个tab挂载之后的下一个时钟处理\n        if (last) {\n            this.owner.updateCursorStyle();\n            this.owner.scrollIntoView(true);\n        }\n\n        this.watch('active', active => {\n            if (active) {\n                this.owner.updateCursorStyle(this.offset);\n            }\n        });\n    }\n\n    detached() {\n        const index = this.data.get('index');\n        this.owner.$tabs[index] = null;\n    }\n}\n"]}]}