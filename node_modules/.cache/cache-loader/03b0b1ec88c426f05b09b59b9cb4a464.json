,EAAG;AACfA,MAAAA,CAAC,CAACC,cAAF;AACH;;;EAnFoCC,c;;;8BAApBf,W;AAEC;2HAEID,S,kGAEIA,S,2NAIOA,S,0uBAgBRA,S,sKAIHA,S;8BA9BLC,W,gBAkCG;AAChB,YAAUgB;AADM,C;8BAlCHhB,W,eAsCE;AACfC,EAAAA,OAAO,EAAEgB,eAAUC,KADJ;AAEfhB,EAAAA,SAAS,EAAEe,eAAUE,MAFN;AAGfhB,EAAAA,MAAM,EAAEc,eAAUG,KAAV,CAAgB,CAAC,MAAD,EAAS,MAAT,CAAhB,CAHO;AAIfhB,EAAAA,YAAY,EAAEa,eAAUI,IAJT;AAKfhB,EAAAA,KAAK,EAAEY,eAAUI,IALF;AAMff,EAAAA,IAAI,EAAEW,eAAUI;AAND,C;8BAtCFrB,W,cA+CC;AACdsB,EAAAA,SADc,uBACF;AACR,QAAMjB,KAAK,GAAG,KAAKI,IAAL,CAAUE,GAAV,CAAc,OAAd,CAAd;AACA,WAAO,CACHZ,SADG,EAEHM,KAAK,cAAON,SAAP,WAFF,EAGLwB,MAHK,CAGE,UAAAC,CAAC;AAAA,aAAIA,CAAJ;AAAA,KAHH,CAAP;AAIH;AAPa,C;;AAzDlB,IAAIC,MAAM,CAACC,GAAX,EAAgB;AACZ,MAAIC,WAAW,GAAGC,OAAO,CAAC,+HAAD,CAAzB;;AACA,MAAIC,aAAa,GAAGD,OAAO,CAAC,gHAAD,CAA3B;;AAEA,MAAIE,iBAAiB,GAAGD,aAAa,CAACE,UAAd,CAAyBN,MAAzB,CAAxB;;AACA,MAAIK,iBAAiB,CAACE,QAAlB,IAA8BF,iBAAiB,CAACG,SAAlB,CAA4BD,QAA9D,EAAwE;AACpEP,IAAAA,MAAM,CAACC,GAAP,CAAWQ,MAAX;;AACAP,IAAAA,WAAW,CAACQ,OAAZ,CAAoBP,OAAO,CAAC,KAAD,CAA3B;;AAEA,QAAIQ,UAAU,GAAG,UAAjB;;AACA,QAAI,CAACX,MAAM,CAACC,GAAP,CAAWjB,IAAhB,EAAsB;AAClBkB,MAAAA,WAAW,CAACU,YAAZ,CAAyBD,UAAzB,EAAqCN,iBAArC;AACH,KAFD,MAGK;AACDH,MAAAA,WAAW,CAACW,SAAZ,CAAsBF,UAAtB,EAAkCN,iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * @file 组件 action-sheet 上拉菜单\n * @author baozhixin <baozhixin@baidu.com>\n */\n\nimport './style/index.less';\nimport {Component, DataTypes} from 'san';\nimport Icon from '../icon';\n\nconst prefixCls = 's-action-sheet';\n\nexport default class ActionSheet extends Component {\n    // eslint-disable-next-line\n    static template = /*html*/ `\n        <div s-if=\"show\" class=\"{{wrapClass}}\" on-touchmove=\"handleTouchmove\">\n            <div class=\"${prefixCls}-container\">\n                <slot name=\"header\"/>\n                <div class=\"${prefixCls}-content\">\n                    <slot name=\"content\">\n                        <div\n                            s-for=\"action, index in actions trackby action.id\"\n                            class=\"${prefixCls}-action {{action.class}}\"\n                            style=\"color: {{action.color}};\"\n                            on-click=\"handleAction(action)\"\n                        >\n                            <s-icon\n                                s-if=\"action.icon\"\n                                color=\"{{action.iconColor || action.color}}\"\n                                size=\"{{action.iconSize || 'md'}}\"\n                                type=\"{{action.icon}}\"\n                            />\n                            {{action.text}}\n                        </div>\n                    </slot>\n                </div>\n                <div\n                    s-if=\"closeText\"\n                    class=\"${prefixCls}-footer\"\n                    on-click=\"handleClose\"\n                ><span>{{closeText}}</span></div>\n            </div>\n            <div class=\"${prefixCls}-mask\" on-click=\"handleMaskClick\"/>\n        </div>\n    `;\n\n    static components = {\n        's-icon': Icon\n    };\n\n    static dataTypes = {\n        actions: DataTypes.array,\n        closeText: DataTypes.string,\n        layout: DataTypes.oneOf(['grid', 'list']),\n        maskClosable: DataTypes.bool,\n        round: DataTypes.bool,\n        show: DataTypes.bool\n    };\n\n    static computed = {\n        wrapClass() {\n            const round = this.data.get('round');\n            return [\n                prefixCls,\n                round && `${prefixCls}-round`\n            ].filter(c => c);\n        }\n    };\n\n    initData() {\n        return {\n            actions: [],\n            closeText: '取消',\n            layout: 'list', // TODO: grid\n            maskClosable: true,\n            round: true,\n            show: false\n        };\n    }\n\n    handleAction(action) {\n        this.fire('action', action);\n    }\n\n    handleClose() {\n        this.data.set('show', false);\n    }\n\n    handleMaskClick() {\n        const maskClosable = this.data.get('maskClosable');\n        maskClosable && this.handleClose();\n    }\n\n    handleTouchmove(e) {\n        e.preventDefault();\n    }\n}\n"]}]}