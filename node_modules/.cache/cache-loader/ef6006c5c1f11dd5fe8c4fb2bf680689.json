bc=1')\n *   {\n *       url: 'http://baidu.com',\n *       query: 'abc=1'\n *   }\n */\nexport default (input, parseQueryString = false) => {\n    const [url, query = ''] = input.split('?');\n    return {\n        url,\n        query: parseQueryString ? query2json(query) : query\n    };\n};\n\n"]}]}