A,KAAK,EAAEH,cAAc,KAAK,CAAnB,GAAuB,uBAAvB,GAAiD;AAF/C,OAAb;AAIH;;;EAtFgCI,c;;;8BAAhBlB,O;;AAVjB,IAAImB,MAAM,CAACC,GAAX,EAAgB;AACZ,MAAIC,WAAW,GAAGC,OAAO,CAAC,+HAAD,CAAzB;;AACA,MAAIC,aAAa,GAAGD,OAAO,CAAC,gHAAD,CAA3B;;AAEA,MAAIE,iBAAiB,GAAGD,aAAa,CAACE,UAAd,CAAyBN,MAAzB,CAAxB;;AACA,MAAIK,iBAAiB,CAACE,QAAlB,IAA8BF,iBAAiB,CAACG,SAAlB,CAA4BD,QAA9D,EAAwE;AACpEP,IAAAA,MAAM,CAACC,GAAP,CAAWQ,MAAX;;AACAP,IAAAA,WAAW,CAACQ,OAAZ,CAAoBP,OAAO,CAAC,KAAD,CAA3B;;AAEA,QAAIQ,UAAU,GAAG,UAAjB;;AACA,QAAI,CAACX,MAAM,CAACC,GAAP,CAAWf,IAAhB,EAAsB;AAClBgB,MAAAA,WAAW,CAACU,YAAZ,CAAyBD,UAAzB,EAAqCN,iBAArC;AACH,KAFD,MAGK;AACDH,MAAAA,WAAW,CAACW,SAAZ,CAAsBF,UAAtB,EAAkCN,iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * @file Top城市组件\n */\n\n/* eslint-disable fecs-camelcase */\nimport {Component} from 'san';\nimport s from './index.module.less';\nimport '../../assets/swiper.css';\nimport Swiper from '@/lib/utils/swiper';\nimport invokeScheme from '../../utils/invokeScheme';\nimport ubc from '@baidu/ubc-report-sdk';\nexport default class TopCity extends Component {\n    static template = `\n    <div>\n        <div class=\"{{s['city-container']}}\">\n            <div class=\"swiper {{isIOS ? '' : 'swiper-no-swiping'}} cityEle\">\n                <div\n                    class=\"swiper-wrapper\"\n                    style=\"width: 100%;\"\n                >\n                    <div\n                        s-for=\"pageData, pageIndex in topCityData\"\n                        class=\"swiper-slide\"\n                    >\n                        <div\n                            s-for=\"item, index in pageData\"\n                            class=\"{{s['city-item']}}\"\n                            on-click=\"itemClick(item,index)\"\n                        >\n                            <div\n                                s-if=\"{{index !== pageData.length - 1}}\"\n                                class=\"{{s['horizontal']}}\"\n                            ></div>\n                            <div class=\"{{s['city-area']}}\">\n                                <span\n                                    class=\"{{s['index']}} {{pageIndex === 0 && index < 3 ? s['index'+index]: ''}}\"\n                                    style=\"{{indexColor}}\"\n                                    >{{pageIndex * 5 + index + 1}}</span>\n                                <span>{{item.area}}</span>\n                            </div>\n                            <div class=\"{{s['city-rate']}}\"\n                            >\n                                <div class=\"{{s['compare-container']}}\">\n                                    <span class=\"{{s['num']}}\">{{item.num}}</span>\n                                    <span class=\"{{s['before']}}\">较前日</span>\n                                    <div\n                                        s-if=\"{{item.trend !== 'fair'}}\"\n                                        class=\"{{s['compare-img']}}\n                                        {{item.trend === 'up' ? s['up']: s['down']}}\"\n                                    ></div>\n                                    <span s-else>-</span>\n                                    <span class=\"{{s['num']}}\">{{item.rate}}</span>\n                                </div>\n                                <div class=\"{{s['linear-container']}}\">\n                                    <div\n                                        class=\"{{s['linear']}} {{subTabSelected === 0 ? s['color0']: s['color1']}}\"\n                                        style=\"width: {{item.precent}};\"\n                                    ></div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <p s-if=\"isIOS\" class=\"{{s['indicator']}}\">\n                <span\n                    s-for=\"item,index in topCityData\"\n                    class=\"{{pageSelected === index ? s['active'] : ''}}\"\n                ></span>\n            </p>\n        </div>\n        <div\n            class=\"{{s['tip']}}\"\n        >以上数据为单位人口搜索比例（所在地域指数/当地人口规模）；数据取最新一天，更新时间为{{updateTime}}</div>\n    </div>\n    `;\n    attached() {\n        this.swiper = new Swiper('.cityEle', {});\n        this.swiper.on('slideChange', e => {\n            this.data.set('pageSelected', e.realIndex);\n        });\n    }\n    initData() {\n        return {\n            s,\n            pageSelected: 0,\n        }\n    }\n    itemClick(item) {\n        const url = 'https://m.baidu.com/s?word=' + item.area + '疫情';\n        invokeScheme(`baiduboxapp://v1/browser/open?url=${encodeURIComponent(url)}`);\n        // 点击打点\n        const subTabSelected = this.data.get('subTabSelected');\n        ubc('event', {\n            type: 'clk',\n            value: subTabSelected === 0 ? 'COVID_search_cityarea' : 'health_ask_cityarea',\n        });\n    }\n}\n"]}]}