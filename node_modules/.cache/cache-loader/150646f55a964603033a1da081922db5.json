M,QAAQ,GAAGD,UAAU,IAAI,CAAC,IAAD,EAAO,CAAP,EAAU,GAAV,EAAe,IAAf,EAAqB,IAArB,CAA7B;AACA,UAAIE,QAAQ,GAAGH,aAAa,IAAI,CAAC,IAAD,EAAO,CAAP,EAAU,IAAV,EAAgB,IAAhB,EAAsB,IAAtB,CAAhC;AACA,UAAII,YAAJ;AACA,UAAIC,cAAJ;AACA,UAAIC,OAAO,GAAG,CAACR,cAAD,GAAkB,EAAlB,GAAuB,EAArC;AACA,UAAIS,KAAJ;AACA,UAAIC,UAAU,GAAG,EAAjB;;AACA,UAAIX,IAAI,KAAK,QAAb,EAAuB;AACnBU,QAAAA,KAAK,GAAGZ,IAAI,GAAGO,QAAQ,CAACH,UAAD,CAAvB;AACAO,QAAAA,OAAO,GAAGR,cAAc,GAAG,CAAjB,GAAsBS,KAAK,GAAGT,cAA9B,GAAgDQ,OAA1D;;AACA,YAAIC,KAAK,IAAI,EAAb,EAAiB;AAAC;AACd,cAAIE,UAAU,GAAGX,cAAc,GAAG,CAAjB,GAAqB,MAAMA,cAAc,GAAG,CAAvB,CAArB,GAAiDQ,OAAlE;AACAA,UAAAA,OAAO,GAAGG,UAAU,GAAGF,KAAvB;AACH;;AACDH,QAAAA,YAAY,GAAGb,IAAI,CAACmB,WAAL,CAAiB,WAAjB,YAAiCH,KAAjC,QAAf;AACAF,QAAAA,cAAc,GAAGd,IAAI,CAACmB,WAAL,CAAiB,aAAjB,YAAmCJ,OAAnC,QAAjB;AACAE,QAAAA,UAAU,GAAGjB,IAAI,CAACmB,WAAL,CAAiB,YAAjB,aAAmC,CAACJ,OAAO,GAAGC,KAAX,IAAoB,CAAvD,QAAb;AACH,OAVD,MAUO;AACHA,QAAAA,KAAK,GAAGZ,IAAI,GAAGQ,QAAQ,CAACJ,UAAD,CAAvB;AACAO,QAAAA,OAAO,GAAGR,cAAc,GAAG,CAAjB,GAAqBS,KAAK,GAAGT,cAA7B,GAA8CQ,OAAxD;;AACA,YAAIC,KAAK,IAAI,EAAb,EAAiB;AAAE;AACf,cAAIE,WAAU,GAAGX,cAAc,GAAG,CAAjB,GAAqB,MAAMA,cAAc,GAAG,CAAvB,CAArB,GAAiDQ,OAAlE;;AACAA,UAAAA,OAAO,GAAGG,WAAU,GAAGF,KAAvB;AACH;;AACDH,QAAAA,YAAY,GAAGb,IAAI,CAACmB,WAAL,CAAiB,WAAjB,YAAiCH,KAAjC,QAAf;AACAF,QAAAA,cAAc,GAAGd,IAAI,CAACmB,WAAL,CAAiB,aAAjB,YAAmCJ,OAAnC,QAAjB;AACAE,QAAAA,UAAU,GAAGjB,IAAI,CAACmB,WAAL,CAAiB,YAAjB,aAAmC,CAACJ,OAAO,GAAGC,KAAX,IAAoB,CAAvD,QAAb;AACH;;AACD,aAAOhB,IAAI,CAACoB,eAAL,CAAqBpB,IAAI,CAACqB,OAAL,CAAa,EAAb,EAAiB,CAACR,YAAD,EAAeC,cAAf,EAA+BG,UAA/B,CAAjB,CAArB,CAAP;AACH,KArCD;AAsCH;AAzCY,CAAjB","sourcesContent":["/**\n * @file 字号四档方法\n * @author changjialu <changjialu@baidu.com>\n * 1.lheight => 根据不同位置不同的line-height，默认是0 非正文之外的字号 21， 1 正文中line-height 33\n * 2.字号调整规则：ios/andriod两套比例\n *  ios比例 = [0.84, 1, 1.1, 1.21, 1.26];\n *  安卓比例 = [0.82, 1, 1.12, 1.24, 1.41];\n */\n\n/* eslint-disable */\n\nmodule.exports = {\n    // fontStatus 由之前的fontStatus = fSize / 行间距   改为 fontStatus = (fSize+行间距)/fSize;\n    install: function (less, pluginManager, functions) {\n        functions.add('applyFont', function (\n            {value: size} = {},\n            {value: type} = {},\n            {value: lineHeightType} = {},\n            {value: fontStatus} = {},\n            {value: andrioPercent} = {},\n            {value: iosPercent} = {},\n        ) {\n            let ratesIos = iosPercent || [0.84, 1, 1.1, 1.21, 1.26];\n            let ratesAnd = andrioPercent || [0.82, 1, 1.12, 1.24, 1.41];\n            let ruleFontSize;\n            let ruleLineHeight;\n            let lheight = !lineHeightType ? 21 : 33;\n            let fSize;\n            let margintTop = '';\n            if (type === 'iphone') {\n                fSize = size * ratesIos[fontStatus];\n                lheight = lineHeightType > 1 ? (fSize * lineHeightType) : lheight;\n                if (fSize >= 69) {// 当字号大于69  行高统一设置为固定值;\n                    let lineheight = lineHeightType > 1 ? 69 * (lineHeightType - 1) : lheight;\n                    lheight = lineheight + fSize;\n                }\n                ruleFontSize = less.declaration('font-size', `${fSize}pr`);\n                ruleLineHeight = less.declaration('line-height', `${lheight}pr`);\n                margintTop = less.declaration('margin-top', `-${(lheight - fSize) / 2}pr`);\n            } else {\n                fSize = size * ratesAnd[fontStatus];\n                lheight = lineHeightType > 1 ? fSize * lineHeightType : lheight;\n                if (fSize >= 69) { // 当字号大于69  行高统一设置为固定值;\n                    let lineheight = lineHeightType > 1 ? 69 * (lineHeightType - 1) : lheight;\n                    lheight = lineheight + fSize;\n                }\n                ruleFontSize = less.declaration('font-size', `${fSize}pr`);\n                ruleLineHeight = less.declaration('line-height', `${lheight}pr`);\n                margintTop = less.declaration('margin-top', `-${(lheight - fSize) / 2}pr`);\n            }\n            return less.detachedruleset(less.ruleset('', [ruleFontSize, ruleLineHeight, margintTop]));\n        });\n    }\n};\n"]}]}