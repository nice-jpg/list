AoB,CACxBD,eAAUE,MADc,EAExBF,eAAUG,MAFc,CAApB;AALO,C;8BAPFd,G,cAkBC;AACdgB,EAAAA,QADc,sBACH;AACP,QAAIC,KAAK,GAAG,EAAZ;AACA,QAAMb,IAAI,GAAG,KAAKA,IAAlB;AACA,QAAMG,MAAM,GAAGH,IAAI,CAACC,GAAL,CAAS,QAAT,CAAf;AACA,QAAMK,IAAI,GAAGN,IAAI,CAACC,GAAL,CAAS,MAAT,CAAb;AACA,QAAMU,MAAM,GAAGX,IAAI,CAACC,GAAL,CAAS,QAAT,CAAf;AACA,QAAMC,MAAM,GAAGF,IAAI,CAACC,GAAL,CAAS,QAAT,CAAf;;AAEA,QAAIE,MAAM,GAAG,CAAb,EAAgB;AACZ,UAAMW,OAAO,aAAM,qBAAOX,MAAM,GAAG,CAAhB,CAAN,QAAb;AACAY,MAAAA,MAAM,CAACC,MAAP,CAAcH,KAAd,EAAqB;AACjB,wBAAgBC,OADC;AAEjB,yBAAiBA;AAFA,OAArB;AAIH;;AAED,QAAI,CAACR,IAAL,EAAW;AACPS,MAAAA,MAAM,CAACC,MAAP,CAAcH,KAAd,EAAqB;AACjB,4BAAoB,CADH;AAEjB,6BAAqB,CAFJ;AAGjB,qBAAa;AAHI,OAArB;AAKH,KAND,MAOK;AACD,UAAMI,KAAK,GAAIX,IAAI,GAAGJ,MAAR,GAAkB,GAAlB,GAAwB,GAAtC;AACAa,MAAAA,MAAM,CAACC,MAAP,CAAcH,KAAd,EAAqB;AACjB,4BAAoB,CADH;AAEjB,6BAAqB,CAFJ;AAGjB,qBAAa,CAHI;AAIjB,8BAAsBI,KAJL;AAKjB,sBAAcA,KALG;AAMjB,qBAAaA;AANI,OAArB;AAQH;;AAED,QAAIN,MAAM,GAAG,CAAT,IAAcA,MAAM,GAAG,CAACT,MAA5B,EAAoC;AAChC,UAAMgB,IAAI,GAAIP,MAAM,GAAG,CAACT,MAAX,GAAqB,GAArB,GAA2B,GAAxC;AACAa,MAAAA,MAAM,CAACC,MAAP,CAAcH,KAAd,EAAqB;AACjB,uBAAeK;AADE,OAArB;AAGH;;AAED,WAAOL,KAAP;AACH;AA5Ca,C;;AA7BlB,IAAIM,MAAM,CAACC,GAAX,EAAgB;AACZ,MAAIC,WAAW,GAAGC,OAAO,CAAC,+HAAD,CAAzB;;AACA,MAAIC,aAAa,GAAGD,OAAO,CAAC,gHAAD,CAA3B;;AAEA,MAAIE,iBAAiB,GAAGD,aAAa,CAACE,UAAd,CAAyBN,MAAzB,CAAxB;;AACA,MAAIK,iBAAiB,CAACE,QAAlB,IAA8BF,iBAAiB,CAACG,SAAlB,CAA4BD,QAA9D,EAAwE;AACpEP,IAAAA,MAAM,CAACC,GAAP,CAAWQ,MAAX;;AACAP,IAAAA,WAAW,CAACQ,OAAZ,CAAoBP,OAAO,CAAC,KAAD,CAA3B;;AAEA,QAAIQ,UAAU,GAAG,UAAjB;;AACA,QAAI,CAACX,MAAM,CAACC,GAAP,CAAWpB,IAAhB,EAAsB;AAClBqB,MAAAA,WAAW,CAACU,YAAZ,CAAyBD,UAAzB,EAAqCN,iBAArC;AACH,KAFD,MAGK;AACDH,MAAAA,WAAW,CAACW,SAAZ,CAAsBF,UAAtB,EAAkCN,iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * @file 组件 WujiGrid/col 栅格/列\n * @author baozhixin <baozhixin@baidu.com>\n *\n * Mark: 未监听父组件的属性变化，暂时没必要\n */\n\nimport {Component, DataTypes} from 'san';\nimport pr2rem from '../utils/pr2rem';\n\nconst prefixCls = 'w-col';\n\nexport default class Col extends Component {\n    static template = /* html */ `\n        <div class=\"${prefixCls}\" style=\"{{colStyle}}\">\n            <slot/>\n        </div>\n    `;\n\n    static dataTypes = {\n        span: DataTypes.oneOfType([\n            DataTypes.string,\n            DataTypes.number,\n        ]),\n        offset: DataTypes.oneOfType([\n            DataTypes.string,\n            DataTypes.number,\n        ]),\n    };\n\n    static computed = {\n        colStyle() {\n            let style = {};\n            const data = this.data;\n            const gutter = data.get('gutter');\n            const span = data.get('span');\n            const offset = data.get('offset');\n            const column = data.get('column');\n\n            if (gutter > 0) {\n                const padding = `${pr2rem(gutter / 2)}rem`;\n                Object.assign(style, {\n                    'padding-left': padding,\n                    'padding-right': padding,\n                });\n            }\n\n            if (!span) {\n                Object.assign(style, {\n                    '-webkit-box-flex': 1,\n                    '-webkit-flex-grow': 1,\n                    'flex-grow': 1,\n                });\n            }\n            else {\n                const width = (span / column) * 100 + '%';\n                Object.assign(style, {\n                    '-webkit-box-flex': 0,\n                    '-webkit-flex-grow': 0,\n                    'flex-grow': 0,\n                    '-webkit-flex-basis': width,\n                    'flex-basis': width,\n                    'max-width': width,\n                });\n            }\n\n            if (offset > 0 && offset < +column) {\n                const left = (offset / +column) * 100 + '%';\n                Object.assign(style, {\n                    'margin-left': left,\n                });\n            }\n\n            return style;\n        },\n    };\n\n    inited() {\n        const $parent = this.parentComponent;\n\n        if ($parent && $parent.isGridRow) {\n            const {column, gutter} = $parent.data.get();\n            this.data.set('column', column);\n            this.data.set('gutter', gutter);\n        }\n    }\n}\n"]}]}