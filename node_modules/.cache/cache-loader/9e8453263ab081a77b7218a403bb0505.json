AAGE,IAAI,CAACC,KAAL,CAAWZ,IAAI,CAACC,GAAL,CAAS,QAAT,CAAX,CAAf;AACA,WAAO,CACHL,SADG,EAEFQ,IAAI,GAAG,CAAP,IAAYA,IAAI,IAAI,EAArB,cAA+BR,SAA/B,cAA4CQ,IAA5C,CAFG,EAGFK,MAAM,GAAG,CAAT,IAAcA,MAAM,GAAG,EAAxB,qBAAyCA,MAAzC,CAHG,EAILI,MAJK,CAIE,UAAAC,CAAC;AAAA,aAAIA,CAAJ;AAAA,KAJH,CAAP;AAKH,GAVa;AAYdC,EAAAA,QAZc,sBAYH;AACP,QAAMjB,MAAM,GAAG,KAAKE,IAAL,CAAUC,GAAV,CAAc,QAAd,CAAf;AACA,QAAMe,OAAO,aAAM,qBAAOlB,MAAM,GAAG,CAAhB,CAAN,QAAb;;AACA,QAAIA,MAAM,GAAG,CAAb,EAAgB;AACZ,aAAO;AACH,wBAAgBkB,OADb;AAEH,yBAAiBA;AAFd,OAAP;AAIH;AACJ;AArBa,C;;AAtBlB,IAAIC,MAAM,CAACC,GAAX,EAAgB;AACZ,MAAIC,WAAW,GAAGC,OAAO,CAAC,+HAAD,CAAzB;;AACA,MAAIC,aAAa,GAAGD,OAAO,CAAC,gHAAD,CAA3B;;AAEA,MAAIE,iBAAiB,GAAGD,aAAa,CAACE,UAAd,CAAyBN,MAAzB,CAAxB;;AACA,MAAIK,iBAAiB,CAACE,QAAlB,IAA8BF,iBAAiB,CAACG,SAAlB,CAA4BD,QAA9D,EAAwE;AACpEP,IAAAA,MAAM,CAACC,GAAP,CAAWQ,MAAX;;AACAP,IAAAA,WAAW,CAACQ,OAAZ,CAAoBP,OAAO,CAAC,KAAD,CAA3B;;AAEA,QAAIQ,UAAU,GAAG,UAAjB;;AACA,QAAI,CAACX,MAAM,CAACC,GAAP,CAAWlB,IAAhB,EAAsB;AAClBmB,MAAAA,WAAW,CAACU,YAAZ,CAAyBD,UAAzB,EAAqCN,iBAArC;AACH,KAFD,MAGK;AACDH,MAAAA,WAAW,CAACW,SAAZ,CAAsBF,UAAtB,EAAkCN,iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * @file 组件 grid 网格布局\n * @author baozhixin <baozhixin@baidu.com>\n */\n\nimport {Component, DataTypes} from 'san';\nimport pr2rem from '../utils/pr2rem';\n\nconst prefixCls = 's-col';\n\nexport default class Col extends Component {\n    // eslint-disable-next-line\n    static template = /*html*/ `\n        <div class=\"{{colClass}}\" style=\"{{colStyle}}\">\n            <slot/>\n        </div>\n    `;\n\n    static dataTypes = {\n        span: DataTypes.oneOfType([DataTypes.string, DataTypes.number]),\n        offset: DataTypes.oneOfType([DataTypes.string, DataTypes.number])\n    };\n\n    static computed = {\n        colClass() {\n            const data = this.data;\n            const span = Math.floor(data.get('span'));\n            const offset = Math.floor(data.get('offset'));\n            return [\n                prefixCls,\n                (span > 0 && span <= 12) && `${prefixCls}-${span}`,\n                (offset > 0 && offset < 12) && `offset-${offset}`\n            ].filter(c => c);\n        },\n\n        colStyle() {\n            const gutter = this.data.get('gutter');\n            const padding = `${pr2rem(gutter / 2)}rem`;\n            if (gutter > 0) {\n                return {\n                    'padding-left': padding,\n                    'padding-right': padding\n                };\n            }\n        }\n    };\n\n    inited() {\n        const gutter = this.parentComponent.data.get('gutter') || 0;\n        this.data.set('gutter', gutter);\n    }\n}\n"]}]}