B;AACpB,aAAOC,OAAO,CAACC,KAAR,CAAc,CAAd,EAAiB,CAAjB,CAAP;AACH;;AACD,WAAOD,OAAP;AACH,GAjBa;AAkBdE,EAAAA,aAlBc,2BAkBE;AACZ,QAAM9C,SAAS,GAAG,KAAKC,IAAL,CAAUC,GAAV,CAAc,YAAd,CAAlB;AACA,WAAOF,SAAS,CAACI,MAAV,IAAoBJ,SAAS,CAACI,MAAV,CAAiB2C,kBAA5C;AACH;AArBa,C;;AA1ClB,IAAIC,MAAM,CAACC,GAAX,EAAgB;AACZ,MAAIC,WAAW,GAAGC,OAAO,CAAC,+HAAD,CAAzB;;AACA,MAAIC,aAAa,GAAGD,OAAO,CAAC,gHAAD,CAA3B;;AAEA,MAAIE,iBAAiB,GAAGD,aAAa,CAACE,UAAd,CAAyBN,MAAzB,CAAxB;;AACA,MAAIK,iBAAiB,CAACE,QAAlB,IAA8BF,iBAAiB,CAACG,SAAlB,CAA4BD,QAA9D,EAAwE;AACpEP,IAAAA,MAAM,CAACC,GAAP,CAAWQ,MAAX;;AACAP,IAAAA,WAAW,CAACQ,OAAZ,CAAoBP,OAAO,CAAC,KAAD,CAA3B;;AAEA,QAAIQ,UAAU,GAAG,UAAjB;;AACA,QAAI,CAACX,MAAM,CAACC,GAAP,CAAWhD,IAAhB,EAAsB;AAClBiD,MAAAA,WAAW,CAACU,YAAZ,CAAyBD,UAAzB,EAAqCN,iBAArC;AACH,KAFD,MAGK;AACDH,MAAAA,WAAW,CAACW,SAAZ,CAAsBF,UAAtB,EAAkCN,iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * @file 周边疫情组件\n * @author liuyuxin06@baidu.com\n */\n/* eslint-disable fecs-camelcase */\nimport {Component} from 'san';\nimport ubc from '@baidu/ubc-report-sdk';\nimport invokeScheme from '../../utils/invokeScheme';\nimport s from './surround.module.less';\n\nconst BaiduMapUrl = 'https://ugc.map.baidu.com/cube/ncp/homepage?ncpfrom=dasousecondtab';\n\nexport default class SurroundEpid extends Component {\n    /* eslint-disable max-len */\n    static template = `\n        <div class=\"{{s['surround']}}\" s-if=\"showSurround\">\n            <div class=\"{{s['surround-totalInfo']}}\">\n                <div class=\"{{s['surround-totalInfo-title']}}\">周边疫情</div>\n                <div class=\"{{s['surround-totalInfo-more']}}\" on-click=\"moreClick\">\n                    <span>共{{patientNumber}}个小区</span>\n                    <span class=\"{{s['surround-totalInfo-moreImg']}}\"></span>\n                </div>\n            </div>\n            <div>\n                <div s-for=\"item, index in poiList\"\n                    class=\"{{s['surround-poilist']}}\"\n                    on-click=\"poiListClick(item, index)\">\n                    <div class=\"{{s['surround-poilist-name']}}\">{{item.poi_name}}</div>\n                    <div class=\"{{s['surround-poilist-bottom']}}\">\n                        <span class=\"{{s['surround-poilist-address']}}\">{{item.address}}</span>\n                        <span class=\"{{s['surround-poilist-dis']}}\">{{item.distance}}</span>\n                    </div>\n                </div>\n            </div>\n            <div s-if=\"update_time\" class=\"{{s['surround-totalInfo-remark']}}\">*更新至{{update_time}} 来自百度地图</div>\n        </div>\n    `;\n    /* eslint-enable max-len */\n    static components = {\n    };\n    static filters = {\n    }\n\n    static computed = {\n        showSurround() {\n            const feedItems = this.data.get('feed.items');\n            if (feedItems.result\n                && feedItems.result.poi_list\n                && feedItems.result.poi_list.length) {\n                return true;\n            }\n            return false;\n        },\n        poiList() {\n            const feedItems = this.data.get('feed.items');\n            let poiList = feedItems.result && feedItems.result.poi_list;\n            if (poiList.length > 3) {\n                return poiList.slice(0, 3);\n            }\n            return poiList;\n        },\n        patientNumber() {\n            const feedItems = this.data.get('feed.items');\n            return feedItems.result && feedItems.result.patient_poi_number;\n        }\n    };\n\n    attached() {\n        const feedItems = this.data.get('feed.items');\n        const time = feedItems.result && feedItems.result.update_time;\n        if (time) {\n            this.data.set('update_time', this.formatDate(time));\n        }\n    }\n    initData() {\n        return {\n            s,\n            update_time: ''\n        };\n    }\n    skipToBaiduMap() {\n        // 跳转到百度地图\n        let schemeHead = 'baiduboxapp://v1/browser/open?url=';\n        let couponUrl = schemeHead + encodeURIComponent(BaiduMapUrl);\n        invokeScheme(couponUrl);\n    }\n    poiListClick(item, index) {\n        this.skipToBaiduMap();\n        ubc('event', {\n            page: 'local',\n            type: 'clk',\n            value: 'nearby',\n            ext: {\n                pos: index,\n                clickPlace: item.poi_name || '',\n                isMore: false\n            }\n        });\n    }\n    moreClick() {\n        this.skipToBaiduMap();\n        ubc('event', {\n            page: 'local',\n            type: 'clk',\n            value: 'nearby',\n            ext: {\n                isMore: true\n            }\n        });\n    }\n    formatDate(timestamp) {\n        const time = new Date(parseInt(timestamp, 10) * 1000);\n        const year = time.getFullYear();\n        const month = time.getMonth() + 1;\n        const date = time.getDate();\n        const hour = time.getHours();\n        const minute = time.getMinutes();\n        const fillZero = number => `${number < 10 ? '0' : ''}${number}`;\n        const formatRes = `${year}.${fillZero(month)}.${fillZero(date)} ${fillZero(hour)}:${fillZero(minute)}`;\n        return formatRes.indexOf('NaN') !== -1 ? '' : formatRes;\n    }\n}"]}]}