,IAAAA,UAAU,CAACT,KAAD,CAAV,GAAoBW,MAAM,CAACC,MAAP,CAAc,EAAd,EAAkBF,OAAlB,EAA2B;AAC3CG,MAAAA,QAAQ,EAAEH,OAAO,CAACG,QAAR,GAAmB,CAAnB,GAAuB;AADU,KAA3B,CAApB;AAGA,WAAO,0BAAUT,GAAV,CAAcG,GAAd,oBAAuB1B,QAAvB,EAAP;AACH,GArEE,2CAsEF,QAtEE,gCAsE8B;AAAA,QAArBiC,MAAqB,SAArBA,MAAqB;AAAA,QAAXR,QAAW,SAAXA,QAAW;;AAAA,oBACXA,QAAQ,CAAC,KAAD,CADG;AAAA,QACtBS,OADsB,aACtBA,OADsB;;AAE7B,QAAIA,OAAJ,EAAa;AACT,UAAI,8BAAenC,SAAIoC,UAAnB,EAA+B,MAA/B,IAAyC,CAA7C,EAAgD;AAC5CC,sBAAKC,IAAL,CAAU,oBAAV,EAAgC;AAC5BC,UAAAA,IAAI,EAAE,GADsB;AAE5BC,UAAAA,OAAO,EAAE;AAFmB,SAAhC;;AAIA;AACH;;AACD,4BAAON,MAAP;AACH,KATD,MASO;AACH,kCAAU;AACNO,QAAAA,OAAO,EAAE,aADH;AAENC,QAAAA,MAAM,EAAER;AAFF,OAAV;AAIH;AACJ,GAvFE,2CAwFF,qBAxFE,4CAwFoD;AAAA,QAA9BT,SAA8B,SAA9BA,SAA8B;AAAA,QAAnBrB,IAAmB,SAAnBA,IAAmB;AAAA,QAAXsB,QAAW,SAAXA,QAAW;AACnD,QAAMC,GAAG,sBAAeF,SAAf,MAAT;AACA,QAAMxB,QAAQ,GAAGyB,QAAQ,CAACC,GAAD,CAAR,CAAcvB,IAA/B;;AACA,QAAI,CAACsB,QAAQ,CAACC,GAAD,CAAR,CAAcgB,YAAnB,EAAiC;AAC7B,aAAO,0BAAUnB,GAAV,CAAcG,GAAd,EAAmBI,MAAM,CAACC,MAAP,CAAc,EAAd,kCACnBN,QAAQ,CAACC,GAAD,CADW;AAEtBvB,QAAAA,IAAI,mCAAMA,IAAI,CAACA,IAAX,CAFkB;AAGtBiB,QAAAA,MAAM,EAAEjB,IAAI,CAACiB,MAHS,CAItB;;AAJsB,SAAnB,CAAP;AAMH;;AACD,WAAO,0BAAUG,GAAV,CAAcG,GAAd,EAAmBI,MAAM,CAACC,MAAP,CAAc,EAAd,kCACnBN,QAAQ,CAACC,GAAD,CADW;AAEtBvB,MAAAA,IAAI,6CAAMH,QAAN,oCAAmBG,IAAI,CAACA,IAAxB,EAFkB;AAGtBiB,MAAAA,MAAM,EAAEjB,IAAI,CAACiB;AAHS,OAAnB,CAAP;AAKH,GAxGE,2CAyGF,oBAzGE,6CAyGqD;AAAA,QAAhCI,SAAgC,UAAhCA,SAAgC;AAAA,QAArBmB,MAAqB,UAArBA,MAAqB;AAAA,QAAXlB,QAAW,UAAXA,QAAW;AACpD,QAAMC,GAAG,sBAAeF,SAAf,MAAT;AACA,QAAMxB,QAAQ,GAAGyB,QAAQ,CAACC,GAAD,CAAzB;AACA,WAAO,0BAAUH,GAAV,CAAcG,GAAd,EAAmBI,MAAM,CAACC,MAAP,CAAc,EAAd,kCACnB/B,QADmB;AAEtBsB,MAAAA,OAAO,EAAEqB;AAFa,OAAnB,CAAP;AAIH,GAhHE;AARS,CAAb;;;AA2HPC,aAAMjD,QAAN,CAAeD,IAAI,CAACC,QAApB,EAA8BkD,UAA9B,CAAyCnD,IAAI,CAACQ,OAA9C","sourcesContent":["/**\n * store\n * @file yangmei(yangmei03@baidu.com)\n */\nimport {store} from '@store';\nimport {builder} from 'san-update';\nimport {invokeApp} from '@baidu/ug-invoke-app';\nimport invoke from '@baidu/xbox/na/invoke';\nimport {env} from '../../../lib/utils/env';\nimport boxx from '@baidu/auc-base/boxx';\nimport versionCompare from '@baidu/xbox/version_compare';\n\nexport const main = {\n    initData: {\n        topBg: undefined,\n        majorPrograms: undefined,\n        env,\n        pageData: [],\n        needPageModules: []\n    },\n    actions: {\n        ['main:updatePageInfo'](data, {dispatch}) {\n            const {\n                top_img, title,\n                bottom_color,\n                bottom_img,\n                is_login,\n                ip_type,\n                target,\n                modules,\n                share_abstract,\n                share_img,\n                share_title\n            } = data;\n            // 头图&背景\n            const topBg = {\n                title,\n                top_img,\n                bottom_img,\n                bottom_color,\n                is_login,\n                ip_type,\n                target,\n                share_abstract,\n                share_img,\n                share_title\n            };\n\n            // update 氛围图\n            dispatch('main:updateTopBg', topBg);\n            if (modules.length) {\n                let needPageModules = [];\n                modules.map((item, index) => {\n                    // 需要分页的数据 添加loading参数\n                    if (+item.is_end === 0) {\n                        needPageModules.push(index);\n                        item.loading = false;\n                    }\n                });\n                dispatch('main:updatePageData', modules);\n                dispatch('main:needPageModules', needPageModules);\n            }\n        },\n        ['main:needPageModules'](data) {\n            return builder().set('needPageModules', data);\n        },\n        ['main:updatePageData'](data) {\n            return builder().set('pageData', data);\n        },\n        ['main:updateTopBg'](data) {\n            return builder().set('topBg', data);\n        },\n        ['main:updateOrder']({data, dataIndex} = {}, {getState}) {\n            const key = `pageData[${dataIndex}]`;\n            const result = {\n                ...getState(key),\n                ...data\n            };\n            return builder().set(key, {...result});\n        },\n        ['main:updateFocusFollow']({index, dataIndex} = {}, {getState}) {\n            const key = `pageData[${dataIndex}]`;\n            const pageData = getState(key);\n            const followData = pageData.data;\n            const oldData = followData[index];\n            followData[index] = Object.assign({}, oldData, {\n                followed: oldData.followed ? 0 : 1\n            });\n            return builder().set(key, {...pageData});\n        },\n        ['invoke']({urlCmd}, {getState}) {\n            const {isBDBox} = getState('env');\n            if (isBDBox) {\n                if (versionCompare(env.boxVersion, '12.2') < 0) {\n                    boxx.call('commonUI.showToast', {\n                        type: '1',\n                        message: '当前版本过低'\n                    });\n                    return;\n                }\n                invoke(urlCmd);\n            } else {\n                invokeApp({\n                    appName: 'baiduboxapp',\n                    scheme: urlCmd\n                });\n            }\n        }, // 分页之后数据更新\n        ['main:updateMoreData']({dataIndex, data}, {getState}) {\n            const key = `pageData[${dataIndex}]`;\n            const pageData = getState(key).data;\n            if (+getState(key).can_exchange) {\n                return builder().set(key, Object.assign({}, {\n                    ...getState(key),\n                    data: [...data.data],\n                    is_end: data.is_end,\n                    // can_exchange: data.is_end ? '0' : '1'\n                }));\n            }\n            return builder().set(key, Object.assign({}, {\n                ...getState(key),\n                data: [...pageData, ...data.data],\n                is_end: data.is_end\n            }));\n        }, // 更新分页请求状态\n        ['main:updateLoading']({dataIndex, status}, {getState}) {\n            const key = `pageData[${dataIndex}]`;\n            const pageData = getState(key);\n            return builder().set(key, Object.assign({}, {\n                ...pageData,\n                loading: status\n            }));\n        }\n    }\n};\nstore.initData(main.initData).addActions(main.actions);"]}]}