{"remainingRequest":"/Users/a2012/Desktop/list/node_modules/babel-loader/lib/index.js??ref--1-2!/Users/a2012/Desktop/list/node_modules/@baidu/xbox/is-box.js","dependencies":[{"path":"/Users/a2012/Desktop/list/node_modules/@baidu/xbox/is-box.js","mtime":1696919504000},{"path":"/Users/a2012/Desktop/list/node_modules/san-hot-loader/index.js","mtime":1696919525000},{"path":"/Users/a2012/Desktop/list/node_modules/cache-loader/dist/cjs.js","mtime":1696919513000},{"path":"/Users/a2012/Desktop/list/node_modules/babel-loader/lib/index.js","mtime":1696919524000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["/Users/a2012/Desktop/list/node_modules/@baidu/xbox/is-box.js"],"names":["subName","ua","UA","reg","test","matchRes","match","sign","indexOf"],"mappings":";;;;;;;;;;;;;;;;;AAIA;;AAJA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;eACe,oBAA2B;AAAA,MAA1BA,OAA0B,uEAAhB,EAAgB;AAAA,MAAZC,EAAY,uEAAPC,WAAO;;AACtC,MAAIF,OAAO,KAAK,MAAhB,EAAwB;AACpBA,IAAAA,OAAO,GAAG,EAAV;AACH,GAHqC,CAKtC;AACA;;;AACA,MAAMG,GAAG,GAAI,8BAAb;;AACA,MAAIA,GAAG,CAACC,IAAJ,CAASH,EAAT,MAAiB,KAArB,EAA4B;AACxB;AACA,WAAO,KAAP;AACH;;AAED,MAAII,QAAQ,GAAGJ,EAAE,CAACK,KAAH,CAASH,GAAT,CAAf;AACA,MAAII,IAAI,GAAGF,QAAQ,CAAC,CAAD,CAAnB;;AACA,MAAI,CAAC,CAACJ,EAAE,CAACO,OAAH,WAAcR,OAAd,cAAyBO,IAAzB,EAAN,EAAwC;AACpC;AACA,WAAO,KAAP;AACH;;AAED,SAAO,IAAP;AACH,C","sourcesContent":["/**\n * @file Created on Thu Nov 15 2018\n * @author wangyongqing <wangyongqing01@baidu.com>\n */\nimport UA from './ua';\n/**\n * 是否是手百产品，支持矩阵产品\n * @function isBox\n * @param {''|lite|pro|info|mission} [subName=''] - 手百矩阵产品标识\n * @param {string} ua - 传入的 userAgent，如果不传则取 navigator.userAgent\n * @return { boolean} 是否是手百判断结果\n * @example\n * isBoxApp()\n * isBoxApp('lite')\n * isBoxApp('pro', 'fdafafdafaf pro baiduboxapp')\n */\nexport default (subName = '', ua = UA) => {\n    if (subName === 'main') {\n        subName = '';\n    }\n\n    // baiduboxapp: 框的常规UA\n    // biduboxapp:  个别网站会触发内核的风控策略(比如域名以le.com结尾)，UA会被替换\n    const reg =  / (baiduboxapp|biduboxapp)\\//i;\n    if (reg.test(ua) === false) {\n        // 框外\n        return false;\n    }\n\n    let matchRes = ua.match(reg);\n    let sign = matchRes[1];\n    if (!~ua.indexOf(`${subName} ${sign}`)) {\n        // 具体的 app ：主线 / lite / info / pro 等\n        return false;\n    }\n\n    return true;\n};\n"]}]}