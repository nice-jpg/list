K,CAWE,UAAAC,CAAC;AAAA,aAAIA,CAAJ;AAAA,KAXH,CAAP;AAYH,GA1Ba;AA4BdC,EAAAA,SA5Bc,uBA4BF;AACR,QAAIC,GAAG,GAAG,EAAV;AACA,QAAMlB,IAAI,GAAG,KAAKA,IAAlB;AACA,QAAMO,KAAK,GAAGP,IAAI,CAACC,GAAL,CAAS,OAAT,CAAd;AACA,QAAMS,KAAK,GAAGV,IAAI,CAACC,GAAL,CAAS,OAAT,CAAd;AACA,QAAMY,MAAM,GAAGb,IAAI,CAACC,GAAL,CAAS,QAAT,CAAf;;AACA,QAAIM,KAAJ,EAAW;AACPW,MAAAA,GAAG,CAACX,KAAJ,GAAYA,KAAZ;AACH;;AACD,QAAI,wBAAUG,KAAV,CAAJ,EAAsB;AAClBQ,MAAAA,GAAG,CAACR,KAAJ,aAAe,qBAAOA,KAAP,CAAf;AACH;;AACD,QAAI,wBAAUG,MAAV,CAAJ,EAAuB;AACnBK,MAAAA,GAAG,CAACL,MAAJ,aAAgB,qBAAOA,MAAP,CAAhB;AACH;;AACD,WAAOK,GAAP;AACH;AA5Ca,C;;AAjDlB,IAAIC,MAAM,CAACC,GAAX,EAAgB;AACZ,MAAIC,WAAW,GAAGC,OAAO,CAAC,+HAAD,CAAzB;;AACA,MAAIC,aAAa,GAAGD,OAAO,CAAC,gHAAD,CAA3B;;AAEA,MAAIE,iBAAiB,GAAGD,aAAa,CAACE,UAAd,CAAyBN,MAAzB,CAAxB;;AACA,MAAIK,iBAAiB,CAACE,QAAlB,IAA8BF,iBAAiB,CAACG,SAAlB,CAA4BD,QAA9D,EAAwE;AACpEP,IAAAA,MAAM,CAACC,GAAP,CAAWQ,MAAX;;AACAP,IAAAA,WAAW,CAACQ,OAAZ,CAAoBP,OAAO,CAAC,KAAD,CAA3B;;AAEA,QAAIQ,UAAU,GAAG,UAAjB;;AACA,QAAI,CAACX,MAAM,CAACC,GAAP,CAAWpB,IAAhB,EAAsB;AAClBqB,MAAAA,WAAW,CAACU,YAAZ,CAAyBD,UAAzB,EAAqCN,iBAArC;AACH,KAFD,MAGK;AACDH,MAAAA,WAAW,CAACW,SAAZ,CAAsBF,UAAtB,EAAkCN,iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * @file 组件 button 按钮\n * @author baozhixin <baozhixin@baidu.com>\n */\n\nimport './style/index.less';\nimport {Component, DataTypes} from 'san';\nimport isNumeric from '../utils/is-numeric';\nimport pr2rem from '../utils/pr2rem';\nimport Icon from '../icon';\n\nconst prefixCls = 's-button';\n\nexport default class Button extends Component {\n    // eslint-disable-next-line\n    static template = /*html*/ `\n        <div\n            class=\"{{wrapClass}}\"\n            style=\"{{wrapStyle}}\"\n            on-click=\"handleClick\"\n            on-touchstart=\"handleTouchstart\"\n        >\n            <slot>\n                <s-icon s-if=\"icon\" class=\"icon\" type=\"{{icon}}\"/>\n                <div s-else class=\"spinner\"/>\n                <div s-if=\"text\" class=\"text\">{{text}}</div>\n            </slot>\n        </div>\n    `;\n\n    static components = {\n        's-icon': Icon\n    };\n\n    static dataTypes = {\n        block: DataTypes.bool,\n        color: DataTypes.string,\n        disabled: DataTypes.bool,\n        ghost: DataTypes.bool,\n        hairline: DataTypes.bool,\n        icon: DataTypes.string,\n        loading: DataTypes.bool,\n        shape: DataTypes.oneOf(['', 'round', 'square']),\n        size: DataTypes.oneOf(['sm', 'md', 'lg']),\n        text: DataTypes.string,\n        type: DataTypes.oneOf(['primary', 'danger', 'default', 'weak']),\n        width: DataTypes.oneOfType([DataTypes.number, DataTypes.string]),\n        height: DataTypes.oneOfType([DataTypes.number, DataTypes.string])\n    };\n\n    static computed = {\n        wrapClass() {\n            const data = this.data;\n            const block = data.get('block');\n            const color = data.get('color');\n            const disabled = data.get('disabled');\n            const ghost = data.get('ghost');\n            const hairline = data.get('hairline');\n            const loading = data.get('loading');\n            const shape = data.get('shape');\n            const size = data.get('size');\n            const text = data.get('text');\n            const type = color ? 'color' : data.get('type');\n\n            return [\n                prefixCls,\n                `${prefixCls}-${size}`,\n                `${prefixCls}-${type}`,\n                block && `${prefixCls}-block`,\n                disabled && `${prefixCls}-disabled`,\n                ghost && `${prefixCls}-ghost`,\n                hairline && `${prefixCls}-hairline`,\n                loading && `${prefixCls}-loading`,\n                !text && `${prefixCls}-notext`,\n                !!shape && `${prefixCls}-${shape}`\n            ].filter(c => c);\n        },\n\n        wrapStyle() {\n            let ret = {};\n            const data = this.data;\n            const color = data.get('color');\n            const width = data.get('width');\n            const height = data.get('height');\n            if (color) {\n                ret.color = color;\n            }\n            if (isNumeric(width)) {\n                ret.width = `${pr2rem(width)}rem`;\n            }\n            if (isNumeric(height)) {\n                ret.height = `${pr2rem(height)}rem`;\n            }\n            return ret;\n        }\n    };\n\n    initData() {\n        return {\n            block: false,\n            disabled: false,\n            ghost: false,\n            hairline: false,\n            icon: '',\n            loading: false,\n            shape: '',\n            size: 'md',\n            text: '',\n            type: 'default'\n        };\n    }\n\n    handleClick(e) {\n        e.stopPropagation();\n        const context = this.data.get();\n        const {disabled, loading} = context;\n\n        if (!disabled && !loading) {\n            this.fire('click', context);\n        }\n    }\n\n    handleTouchstart(e) {\n        e.stopPropagation();\n        this.fire('touchstart', this.data.get());\n    }\n}\n"]}]}