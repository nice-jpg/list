ChB,eAAaW,iBADG;AAEhB,gBAAcC;AAFE,C;8BAtBHZ,U,eA2BE;AACfa,EAAAA,KAAK,EAAEC,eAAUC,MADF;AAEfb,EAAAA,QAAQ,EAAEY,eAAUE,IAFL;AAGfb,EAAAA,eAAe,EAAEW,eAAUE;AAHZ,C;8BA3BFhB,U,cAiCC;AACdiB,EAAAA,IADc,kBACP;AACH,WAAO,KAAKX,IAAL,CAAUE,GAAV,CAAc,UAAd,IAA4B,iBAA5B,GAAgD,mBAAvD;AACH,GAHa;AAIdU,EAAAA,OAJc,qBAIJ;AAAA;;AACN,QAAMC,OAAO,GAAG,KAAKb,IAAL,CAAUE,GAAV,CAAc,SAAd,KAA4B,EAA5C;;AACA,QAAIW,OAAO,CAACC,MAAZ,EAAoB;AAChB,UAAMC,QAAQ,GAAGF,OAAO,CAACG,SAAR,CAAkB,UAAAC,IAAI,EAAI;AACvC,eAAOA,IAAI,CAACV,KAAL,KAAe,KAAI,CAACP,IAAL,CAAUE,GAAV,CAAc,OAAd,CAAtB;AACH,OAFgB,CAAjB;AAGA,aAAOW,OAAO,CAACE,QAAD,CAAP,CAAkBG,IAAzB;AACH;;AACD,WAAO,KAAKlB,IAAL,CAAUE,GAAV,CAAc,OAAd,CAAP;AACH;AAba,C;;AA/ClB,IAAIiB,MAAM,CAACC,GAAX,EAAgB;AACZ,MAAIC,WAAW,GAAGC,OAAO,CAAC,+HAAD,CAAzB;;AACA,MAAIC,aAAa,GAAGD,OAAO,CAAC,gHAAD,CAA3B;;AAEA,MAAIE,iBAAiB,GAAGD,aAAa,CAACE,UAAd,CAAyBN,MAAzB,CAAxB;;AACA,MAAIK,iBAAiB,CAACE,QAAlB,IAA8BF,iBAAiB,CAACG,SAAlB,CAA4BD,QAA9D,EAAwE;AACpEP,IAAAA,MAAM,CAACC,GAAP,CAAWQ,MAAX;;AACAP,IAAAA,WAAW,CAACQ,OAAZ,CAAoBP,OAAO,CAAC,KAAD,CAA3B;;AAEA,QAAIQ,UAAU,GAAG,UAAjB;;AACA,QAAI,CAACX,MAAM,CAACC,GAAP,CAAWpB,IAAhB,EAAsB;AAClBqB,MAAAA,WAAW,CAACU,YAAZ,CAAyBD,UAAzB,EAAqCN,iBAArC;AACH,KAFD,MAGK;AACDH,MAAAA,WAAW,CAACW,SAAZ,CAAsBF,UAAtB,EAAkCN,iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * @file 组件 WujiSelect 无极-下拉框\n * @author yangmei03 <yangmei03@baidu.com>\n */\n\nimport './style/index.less';\nimport {Component, DataTypes} from 'san';\nimport Icon from '../wuji-icon';\nimport Popup from '../wuji-popup';\n\nimport '@baidu/wuji-icons/svg/arrow-unfold-o-48.svg';\nimport '@baidu/wuji-icons/svg/arrow-fold-o-48.svg';\n\nconst prefixCls = 'w-select';\n\nexport default class WujiSelect extends Component {\n    static template = /* html */ `\n        <div class=\"${prefixCls}-wrap\">\n            <div class=\"${prefixCls}-box\" on-click=\"handleClick\">\n                <span s-if=\"label\" class=\"${prefixCls}-text\">{{label}}</span>\n                <div class=\"${prefixCls}-content\">\n                    <span\n                        s-if=\"!value && showPlaceholder\"\n                        class=\"${prefixCls}-placeholder\"\n                    >{{placeholder}}</span>\n                    <span\n                        s-if=\"value\"\n                        class=\"${prefixCls}-select\"\n                    >{{getText}}</span>\n                </div>\n                <span class=\"${prefixCls}-icon\">\n                    <wuji-icon type=\"{{icon}}\"/>\n                </span>\n            </div>\n        </div>\n    `;\n\n    static components = {\n        'wuji-icon': Icon,\n        'wuji-popup': Popup,\n    };\n\n    static dataTypes = {\n        value: DataTypes.string,\n        showMenu: DataTypes.bool,\n        showPlaceholder: DataTypes.bool,\n    };\n\n    static computed = {\n        icon() {\n            return this.data.get('showMenu') ? 'arrow-fold-o-48' : 'arrow-unfold-o-48';\n        },\n        getText() {\n            const options = this.data.get('options') || [];\n            if (options.length) {\n                const selected = options.findIndex(item => {\n                    return item.value === this.data.get('value');\n                });\n                return options[selected].text;\n            }\n            return this.data.get('value');\n        },\n    };\n\n    initData() {\n        return {\n            selectText: '',\n            showMenu: false,\n            showPlaceholder: true,\n            isOpen: false,\n        };\n    }\n\n    handleClick(event) {\n        this.data.set('showMenu', !this.data.get('showMenu'));\n        this.fire('click', {event});\n    }\n}\n"]}]}