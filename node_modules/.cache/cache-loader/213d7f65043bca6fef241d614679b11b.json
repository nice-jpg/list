   static computed = {\n        iconClass() {\n            const size = this.data.get('size');\n            return [\n                prefixCls,\n                `${prefixCls}-${size}`\n            ];\n        },\n        iconStyle() {\n            let ret = {};\n            const color = this.data.get('color');\n            const size = this.data.get('size');\n            if (color) {\n                ret.color = color;\n            }\n            if (isNumeric(size)) {\n                const width = `${pr2rem(size)}rem`;\n                ret.width = ret.height = width;\n            }\n            return ret;\n        },\n        use() {\n            const type = this.data.get('type');\n            return `<use xlink:href=\"#${type}\"/>`;\n        }\n    };\n\n    initData() {\n        return {\n            size: 'lg'\n        };\n    }\n}\n"]}]}