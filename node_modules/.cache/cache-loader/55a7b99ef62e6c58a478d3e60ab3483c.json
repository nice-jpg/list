,EAAEQ,eAAUE,IAFJ;AAGfT,EAAAA,QAAQ,EAAEO,eAAUE,IAHL;AAIfR,EAAAA,kBAAkB,EAAEM,eAAUE;AAJf,C;8BARFZ,M,cAeC;AACda,EAAAA,SADc,uBACF;AACR,QAAMX,OAAO,GAAG,KAAKG,IAAL,CAAUC,GAAV,CAAc,SAAd,CAAhB;AACA,QAAMH,QAAQ,GAAG,KAAKE,IAAL,CAAUC,GAAV,CAAc,UAAd,CAAjB;AACA,WAAO,CACHP,SADG,EAEHG,OAAO,cAAOH,SAAP,aAFJ,EAGHI,QAAQ,cAAOJ,SAAP,cAHL,EAILe,MAJK,CAIE,UAAAC,CAAC;AAAA,aAAIA,CAAJ;AAAA,KAJH,CAAP;AAKH,GATa;AAUdC,EAAAA,SAVc,uBAUF;AACR,QAAMd,OAAO,GAAG,KAAKG,IAAL,CAAUC,GAAV,CAAc,SAAd,CAAhB;AACA,QAAML,KAAK,GAAG,KAAKI,IAAL,CAAUC,GAAV,CAAc,OAAd,CAAd;AACA,WAAOJ,OAAO,IAAID,KAAX,GAAmB;AACtB,0BAAoBA;AADE,KAAnB,GAEH,EAFJ;AAGH;AAhBa,C;;AAxBlB,IAAIgB,MAAM,CAACC,GAAX,EAAgB;AACZ,MAAIC,WAAW,GAAGC,OAAO,CAAC,+HAAD,CAAzB;;AACA,MAAIC,aAAa,GAAGD,OAAO,CAAC,gHAAD,CAA3B;;AAEA,MAAIE,iBAAiB,GAAGD,aAAa,CAACE,UAAd,CAAyBN,MAAzB,CAAxB;;AACA,MAAIK,iBAAiB,CAACE,QAAlB,IAA8BF,iBAAiB,CAACG,SAAlB,CAA4BD,QAA9D,EAAwE;AACpEP,IAAAA,MAAM,CAACC,GAAP,CAAWQ,MAAX;;AACAP,IAAAA,WAAW,CAACQ,OAAZ,CAAoBP,OAAO,CAAC,KAAD,CAA3B;;AAEA,QAAIQ,UAAU,GAAG,UAAjB;;AACA,QAAI,CAACX,MAAM,CAACC,GAAP,CAAWb,IAAhB,EAAsB;AAClBc,MAAAA,WAAW,CAACU,YAAZ,CAAyBD,UAAzB,EAAqCN,iBAArC;AACH,KAFD,MAGK;AACDH,MAAAA,WAAW,CAACW,SAAZ,CAAsBF,UAAtB,EAAkCN,iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * @file 组件 switch 开关\n * @author baozhixin <baozhixin@baidu.com>\n */\n\nimport './style/index.less';\nimport {Component, DataTypes} from 'san';\n\nconst prefixCls = 's-switch';\n\nexport default class Switch extends Component {\n    // eslint-disable-next-line\n    static template = /*html*/ `\n        <div class=\"{{wrapClass}}\" style=\"{{wrapStyle}}\" on-click=\"handleClick\">\n            <div class=\"${prefixCls}-node\"/>\n        </div>\n    `;\n\n    static dataTypes = {\n        color: DataTypes.string,\n        checked: DataTypes.bool,\n        disabled: DataTypes.bool,\n        disableAutoChecked: DataTypes.bool\n    };\n\n    static computed = {\n        wrapClass() {\n            const checked = this.data.get('checked');\n            const disabled = this.data.get('disabled');\n            return [\n                prefixCls,\n                checked && `${prefixCls}-checked`,\n                disabled && `${prefixCls}-disabled`\n            ].filter(c => c);\n        },\n        wrapStyle() {\n            const checked = this.data.get('checked');\n            const color = this.data.get('color');\n            return checked && color ? {\n                'background-color': color\n            } : {};\n        }\n    };\n\n    initData() {\n        return {\n            color: '',\n            checked: true,\n            disabled: false,\n            disableAutoChecked: false\n        };\n    }\n\n    handleClick() {\n        const {checked, disabled, disableAutoChecked} = this.data.get();\n        if (disabled) {\n            return;\n        }\n        !disableAutoChecked && this.data.set('checked', !checked);\n        this.fire('change', !checked);\n    }\n}\n"]}]}