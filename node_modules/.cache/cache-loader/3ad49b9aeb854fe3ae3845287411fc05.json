,eAAUM;AAND,C;8BARFb,G,cAiBC;AACdc,EAAAA,QADc,sBACH;AACP,QAAMC,IAAI,GAAG,KAAKA,IAAlB;AACA,QAAMC,IAAI,GAAG,WAAWD,IAAI,CAACE,GAAL,CAAS,MAAT,CAAxB;AACA,QAAMd,IAAI,GAAGY,IAAI,CAACE,GAAL,CAAS,MAAT,CAAb;AACA,QAAMX,KAAK,GAAGS,IAAI,CAACE,GAAL,CAAS,OAAT,CAAd;AACA,QAAMR,OAAO,GAAGM,IAAI,CAACE,GAAL,CAAS,SAAT,CAAhB;AACA,QAAMb,IAAI,GAAGW,IAAI,CAACE,GAAL,CAAS,MAAT,CAAb;AAEA,WAAO,CACHlB,SADG,YAEAA,SAFA,cAEaI,IAFb,GAGHa,IAAI,IAAIV,KAAR,cAAoBP,SAApB,oBAAuCO,KAAvC,CAHG,EAIHU,IAAI,IAAIP,OAAR,cAAsBV,SAAtB,sBAA2CU,OAA3C,CAJG,EAKHL,IAAI,cAAOL,SAAP,UALD,EAMLmB,MANK,CAME,UAAAC,CAAC;AAAA,aAAIA,CAAJ;AAAA,KANH,CAAP;AAOH,GAhBa;AAkBdC,EAAAA,QAlBc,sBAkBH;AACP,QAAML,IAAI,GAAG,KAAKA,IAAlB;AACA,QAAMM,UAAU,GAAGN,IAAI,CAACE,GAAL,CAAS,QAAT,CAAnB;AACA,QAAMf,MAAM,GAAGa,IAAI,CAACE,GAAL,CAAS,QAAT,CAAf;AACA,QAAMhB,MAAM,GAAG,qBAAOoB,UAAU,GAAGnB,MAAM,GAAG,CAA7B,CAAf;AACA,QAAMoB,GAAG,GAAGrB,MAAM,GAAG,CAAT,aAAgBA,MAAhB,WAA8B,CAA1C;AACA,WAAO;AACH,qBAAeqB,GADZ;AAEH,sBAAgBA;AAFb,KAAP;AAIH;AA5Ba,C;;AA3BlB,IAAIC,MAAM,CAACC,GAAX,EAAgB;AACZ,MAAIC,WAAW,GAAGC,OAAO,CAAC,+HAAD,CAAzB;;AACA,MAAIC,aAAa,GAAGD,OAAO,CAAC,gHAAD,CAA3B;;AAEA,MAAIE,iBAAiB,GAAGD,aAAa,CAACE,UAAd,CAAyBN,MAAzB,CAAxB;;AACA,MAAIK,iBAAiB,CAACE,QAAlB,IAA8BF,iBAAiB,CAACG,SAAlB,CAA4BD,QAA9D,EAAwE;AACpEP,IAAAA,MAAM,CAACC,GAAP,CAAWQ,MAAX;;AACAP,IAAAA,WAAW,CAACQ,OAAZ,CAAoBP,OAAO,CAAC,KAAD,CAA3B;;AAEA,QAAIQ,UAAU,GAAG,UAAjB;;AACA,QAAI,CAACX,MAAM,CAACC,GAAP,CAAWT,IAAhB,EAAsB;AAClBU,MAAAA,WAAW,CAACU,YAAZ,CAAyBD,UAAzB,EAAqCN,iBAArC;AACH,KAFD,MAGK;AACDH,MAAAA,WAAW,CAACW,SAAZ,CAAsBF,UAAtB,EAAkCN,iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * @file 组件 grid 网格布局\n * @author baozhixin <baozhixin@baidu.com>\n */\n\nimport {Component, DataTypes} from 'san';\n// import classNames from 'classnames';\nimport pr2rem from '../utils/pr2rem';\n\nconst prefixCls = 's-row';\n\nexport default class Row extends Component {\n    // eslint-disable-next-line\n    static template = /*html*/ `\n        <div class=\"{{rowClass}}\" style=\"{{rowStyle}}\">\n            <slot/>\n        </div>\n    `;\n\n    static dataTypes = {\n        align: DataTypes.string,\n        justify: DataTypes.string,\n        margin: DataTypes.oneOfType([DataTypes.string, DataTypes.number]),\n        gutter: DataTypes.oneOfType([DataTypes.string, DataTypes.number]),\n        type: DataTypes.oneOf(['flex', 'float']),\n        wrap: DataTypes.bool\n    };\n\n    static computed = {\n        rowClass() {\n            const data = this.data;\n            const flex = 'flex' === data.get('type');\n            const type = data.get('type');\n            const align = data.get('align');\n            const justify = data.get('justify');\n            const wrap = data.get('wrap');\n\n            return [\n                prefixCls,\n                `${prefixCls}-${type}`,\n                flex && align && `${prefixCls}-align-${align}`,\n                flex && justify && `${prefixCls}-justify-${justify}`,\n                wrap && `${prefixCls}-wrap`\n            ].filter(c => c);\n        },\n\n        rowStyle() {\n            const data = this.data;\n            const baseMargin = data.get('margin');\n            const gutter = data.get('gutter');\n            const margin = pr2rem(baseMargin - gutter / 2);\n            const val = margin > 0 ? `${margin}rem` : 0;\n            return {\n                'margin-left': val,\n                'margin-right': val\n            };\n        }\n    };\n\n    initData() {\n        return {\n            margin: 51,\n            gutter: 24,\n            type: 'flex',\n            wrap: true\n        };\n    }\n}\n"]}]}