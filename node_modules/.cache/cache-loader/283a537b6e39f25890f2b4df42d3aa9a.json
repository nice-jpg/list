ALc,qBAKJ;AACN,QAAMJ,IAAI,GAAG,KAAKL,IAAL,CAAUC,GAAV,CAAc,kBAAd,CAAb;AACA,WAAO,CAACI,IAAI,IAAI,EAAT,EAAaC,MAAb,CAAoB,UAAAC,IAAI;AAAA,aAAIA,IAAI,CAACC,KAAL,KAAe,KAAnB;AAAA,KAAxB,CAAP;AACH,GARa;AASdE,EAAAA,WATc,yBASA;AACV,QAAML,IAAI,8CAAO,KAAKL,IAAL,CAAUC,GAAV,CAAc,UAAd,CAAP,oCAAqC,KAAKD,IAAL,CAAUC,GAAV,CAAc,SAAd,CAArC,EAAV;AACA,WAAO,CAACI,IAAI,IAAI,EAAT,EAAaM,GAAb,CAAiB,UAAAJ,IAAI;AAAA,6CACrBA,IADqB;AAExBK,QAAAA,MAAM,EAAEL,IAAI,CAACC,KAAL,KAAe;AAFC;AAAA,KAArB,EAGHK,KAHG,CAGG,CAHH,EAGM,CAHN,CAAP;AAIH,GAfa;AAgBdC,EAAAA,QAhBc,sBAgBH;AACP,QAAMC,KAAK,GAAG,KAAKf,IAAL,CAAUC,GAAV,CAAc,UAAd,EAA0Be,MAA1B,GAAmC,KAAKhB,IAAL,CAAUC,GAAV,CAAc,SAAd,EAAyBe,MAA1E;AACA,WAAOD,KAAK,GAAG,CAAf;AACH;AAnBa,C;;AA9ClB,IAAIE,MAAM,CAACC,GAAX,EAAgB;AACZ,MAAIC,WAAW,GAAGC,OAAO,CAAC,+HAAD,CAAzB;;AACA,MAAIC,aAAa,GAAGD,OAAO,CAAC,gHAAD,CAA3B;;AAEA,MAAIE,iBAAiB,GAAGD,aAAa,CAACE,UAAd,CAAyBN,MAAzB,CAAxB;;AACA,MAAIK,iBAAiB,CAACE,QAAlB,IAA8BF,iBAAiB,CAACG,SAAlB,CAA4BD,QAA9D,EAAwE;AACpEP,IAAAA,MAAM,CAACC,GAAP,CAAWQ,MAAX;;AACAP,IAAAA,WAAW,CAACQ,OAAZ,CAAoBP,OAAO,CAAC,KAAD,CAA3B;;AAEA,QAAIQ,UAAU,GAAG,UAAjB;;AACA,QAAI,CAACX,MAAM,CAACC,GAAP,CAAWlB,IAAhB,EAAsB;AAClBmB,MAAAA,WAAW,CAACU,YAAZ,CAAyBD,UAAzB,EAAqCN,iBAArC;AACH,KAFD,MAGK;AACDH,MAAAA,WAAW,CAACW,SAAZ,CAAsBF,UAAtB,EAAkCN,iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * @file 风险地区组件\n * @author liuyuxin06@baidu.com\n */\n/* eslint-disable fecs-camelcase */\nimport {Component} from 'san';\nimport styles from './index.module.less';\nimport invokeScheme from '../../utils/invokeScheme';\n\nexport default class DangerousArea extends Component {\n    /* eslint-disable max-len */\n    static template = `\n        <div class=\"areaIncity\">\n            <div class=\"{{styles.areaWrapper}}\">\n                <div class=\"{{styles.areaTitle}}\">\n                    <div s-if=\"{{+areaData.highLevelNum > 0}}\">\n                        <span>高风险地区</span>\n                        <span class=\"{{styles.areaCount}}\">{{areaData.highLevelNum}}</span>\n                        <span>个</span>\n                    </div>\n                    <div s-if=\"{{+areaData.midLevelNum > 0}}\">\n                        <span>{{+areaData.highLevelNum > 0 ? '，' : ''}}中风险地区</span>\n                        <span class=\"{{styles.areaCount}}\">{{areaData.midLevelNum}}</span>\n                        <span>个</span>\n                    </div>\n                </div>\n                <div s-for=\"item, index in contentList\" \n                    class=\"{{styles.subList}}\"\n                >\n                    <div class=\"{{styles.areaTag}} {{item.isHigh ? styles.areaTagHigh : styles.areaTagMid}}\"\n                    >\n                        {{item.level}}\n                    </div>\n                    <div class=\"{{styles.areaName}}\">{{item.area}}</div>\n                    <div s-if=\"item.isNew\"\n                        class=\"{{styles.areaTag}} {{styles.areaTagNew}}\"\n                    >新</div>\n                </div>\n                <div s-if=\"showMore\" class=\"{{styles.areaMore)}}\">\n                    <div class=\"{{styles.areaMoreBtn}}\">\n                        <span on-click=\"handleShowMore\">查看更多</span>\n                        <span class=\"{{styles.areaMoreIcon}}\"></span>\n                    </div>\n                </div>\n            </div>\n        </div>\n    `;\n    static computed = {\n        highList() {\n            const list = this.data.get('areaData.subList');\n            return (list || []).filter(item => item.level === '高风险');\n        },\n        midList() {\n            const list = this.data.get('areaData.subList');\n            return (list || []).filter(item => item.level === '中风险');\n        },\n        contentList() {\n            const list = [...this.data.get('highList'), ...this.data.get('midList')];\n            return (list || []).map(item => ({\n                ...item,\n                isHigh: item.level === '高风险'\n            })).slice(0, 6);\n        },\n        showMore() {\n            const total = this.data.get('highList').length + this.data.get('midList').length;\n            return total > 6;\n        }\n    }\n    initData() {\n        return {\n            styles\n        }\n    }\n    handleShowMore() {\n        /* eslint-disable max-len */\n        const schemeHead = 'baiduboxapp://v1/browser/open?url=';\n        /* eslint-enable max-len */\n        const cmd = this.data.get('areaData.moreUrl');\n        cmd && invokeScheme(schemeHead + encodeURIComponent(cmd));\n    }\n}"]}]}