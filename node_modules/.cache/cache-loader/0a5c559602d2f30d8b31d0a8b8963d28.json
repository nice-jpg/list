iBAAM,KAAI,CAACb,GAAL,CAASc,eAAT,CAAyB,QAAzB,EAAmCP,GAAnC,CAAN;AAAA,SANV;AAOA;AACH;;AACD,UAAMH,KAAK,GAAG,KAAKJ,GAAL,CAASK,gBAAT,EAAd;AACAD,MAAAA,KAAK,KAAKA,KAAK,CAACE,MAAN,GAAeC,GAApB,CAAL;AACH","sourcesContent":["export default class Volume {\n    constructor(api, pluginOptions) {\n        this.api = api;\n        this.pluginOptions = pluginOptions;\n    }\n    /**\n    * volume声音\n    */\n    get() {\n        // 端内统一用option来维护volume值，安卓端video.volume用于更改\n        if (this.api.getMode() === 'na') {\n            return this.api.getPlayerOption('volume');\n        }\n        const video = this.api.getVideoInstance();\n        return video ? video.volume : '';\n    }\n    set(vol) {\n        const {isIOS, invokeP} = this.api.getBoxTools();\n        const containerId = this.api.getPlayerOption('containerId');\n        if (this.api.getMode() === 'na' && isIOS()) {\n            invokeP('video/slrSetSystemVolume', {\n                    params: {\n                        containerId,\n                        volume: vol\n                    }\n                })\n                .then(() => this.api.setPlayerOption('volume', vol));\n            return;\n        }\n        const video = this.api.getVideoInstance();\n        video && (video.volume = vol);\n    }\n}\n"]}]}