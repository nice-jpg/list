QAAQ,GAAGD,UAAU,IAAI,CAAC,IAAD,EAAO,CAAP,EAAU,GAAV,EAAe,IAAf,EAAqB,IAArB,CAA7B;AACA,UAAIE,QAAQ,GAAGH,aAAa,IAAI,CAAC,IAAD,EAAO,CAAP,EAAU,IAAV,EAAgB,IAAhB,EAAsB,IAAtB,CAAhC;AACA,UAAII,YAAJ;AACA,UAAIC,cAAJ;AACA,UAAIC,OAAO,GAAG,CAACT,cAAD,GAAkB,EAAlB,GAAuB,EAArC;AACA,UAAIU,KAAJ;AACA,UAAIC,UAAU,GAAG,EAAjB;;AACA,UAAIZ,IAAI,KAAK,QAAb,EAAuB;AACnBW,QAAAA,KAAK,GAAGb,IAAI,GAAGQ,QAAQ,CAACJ,UAAD,CAAvB;AACH,OAFD,MAEO;AACHS,QAAAA,KAAK,GAAGb,IAAI,GAAGS,QAAQ,CAACL,UAAD,CAAvB;AACH;;AACDQ,MAAAA,OAAO,GAAGT,cAAc,GAAG,CAAjB,GAAqBU,KAAK,GAAGV,cAA7B,GAA8CS,OAAxD;AACAF,MAAAA,YAAY,GAAGd,IAAI,CAACmB,WAAL,CAAiB,WAAjB,YAAiCF,KAAjC,QAAf;AACAF,MAAAA,cAAc,GAAGf,IAAI,CAACmB,WAAL,CAAiB,aAAjB,YAAmCF,KAAK,GAAGD,OAA3C,QAAjB;AACAP,MAAAA,SAAS,KAAKS,UAAU,GAAGlB,IAAI,CAACmB,WAAL,CAAiB,YAAjB,aAAmCH,OAAO,GAAG,CAA7C,QAAlB,CAAT;AACA,aAAOhB,IAAI,CAACoB,eAAL,CAAqBpB,IAAI,CAACqB,OAAL,CAAa,EAAb,EAAiB,CAACP,YAAD,EAAeC,cAAf,EAA+BN,SAAS,IAAIS,UAA5C,CAAjB,CAArB,CAAP;AACH,KA1BD;AA2BH;AA7BY,CAAjB","sourcesContent":["/**\n * @file 字号四档方法\n * @author changjialu <changjialu@baidu.com>\n * 1.lheight => 根据不同位置不同的line-height，默认是0 非正文之外的字号 21， 1 正文中line-height 33\n * 2.字号调整规则：ios/andriod两套比例\n *  ios比例 = [0.84, 1, 1.1, 1.21, 1.26];\n *  安卓比例 = [0.82, 1, 1.12, 1.24, 1.41];\n *  从无极抠出来的，无极现版本间距是倍数，不适应\n */\n\n/* eslint-disable */\n\nmodule.exports = {\n    install: function (less, pluginManager, functions) {\n        functions.add('applyFont', function (\n            {value: size} = {},\n            {value: type} = {},\n            {value: lineHeightType} = {},\n            {value: fontStatus} = {},\n            {value: needMarFu = 1} = {},\n            {value: andrioPercent} = {},\n            {value: iosPercent} = {},\n        ) {\n            let ratesIos = iosPercent || [0.84, 1, 1.1, 1.21, 1.26];\n            let ratesAnd = andrioPercent || [0.82, 1, 1.12, 1.24, 1.41];\n            let ruleFontSize;\n            let ruleLineHeight;\n            let lheight = !lineHeightType ? 21 : 33;\n            let fSize;\n            let margintTop = '';\n            if (type === 'iphone') {\n                fSize = size * ratesIos[fontStatus];\n            } else {\n                fSize = size * ratesAnd[fontStatus];\n            }\n            lheight = lineHeightType > 1 ? fSize / lineHeightType : lheight;\n            ruleFontSize = less.declaration('font-size', `${fSize}pr`);\n            ruleLineHeight = less.declaration('line-height', `${fSize + lheight}pr`);\n            needMarFu && (margintTop = less.declaration('margin-top', `-${lheight / 2}pr`));\n            return less.detachedruleset(less.ruleset('', [ruleFontSize, ruleLineHeight, needMarFu && margintTop]));\n        });\n    }\n};"]}]}