ecs-camelcase */\nimport {Component} from 'san';\nimport s from './trendingList.module.less';\nimport ubc from '@baidu/ubc-report-sdk';\nimport invoke from '@baidu/xbox/na/invoke';\nexport default class TrendingList extends Component {\n    /* eslint-disable max-len */\n    static template = `\n        <div s-if=\"{{itemData.board_list && itemData.board_list.length > 0}}\"\n            class=\"{{s['trendingList']}}\"\n            style=\"{{'bg_image' | getBackgroundImage(theme, itemData)}} {{'bg_color' | getBackgroundColor(theme, itemData)}}\">\n            <div class=\"{{s['trendingList-head']}}\"\n                style=\" {{paddingStyle}} {{'title_image' | getBackgroundImage(theme, itemData)}}\">\n                <div>\n                    <img class=\"{{s['trendingList-head-img']}}\"\n                        src=\"{{'board_icon' | getThemeAttr(theme)}}\" alt=\"\"/>\n                    <span class=\"{{s['trendingList-head-text']}}\"\n                        style=\"{{'board_name_font' | getFontColor(theme, itemData)}}\">{{itemData.board_name}}</span>\n                </div>\n                <div on-click=\"moreClick\" >\n                    <span class=\"{{s['trendingList-head-moretitle']}}\"\n                        style=\"{{'more_text_font' | getFontColor(theme, itemData)}}\">{{itemData.more_text}}</span>\n                    <img class=\"{{s['trendingList-head-moreicon']}}\" \n                        src=\"{{'more_icon' | getThemeAttr(theme)}}\"/>\n                </div>\n            </div>\n            <div class=\"{{s['trendingList-body']}}\">\n                <div class=\"{{s['trendingList-bodyItem']}}\"\n                    s-for=\"item,index in boardList\"\n                    style=\"{{index < boardList.length - 1 ? borderStyle : ''}}\"\n                    on-click=\"itemClick($event, item)\">\n                    <div s-if=\"{{item.data.index && item.data.title}}\" class=\"{{s['trendingList-bodyItem-info']}}\"\n                        style=\"width: {{itemMaxWidth}}px;\">\n                        <div class=\"{{s['trendingList-bodyItem-index']}}\"\n                            style=\"{{'index_bg_color' | getBackgroundColor(theme, item.data)}}\n                                {{'index_font_color' | getIndexColor(theme, item.data)}}\">\n                            <span>{{item.data.index}}</span>\n                        </div>\n                        <span class=\"{{s['trendingList-bodyItem-title']}}\"\n                            style=\"{{titleFontSize}}\">{{item.data.title}}</span>\n                        <div s-if=\"item.data.hot_tag\"\n                            class=\"{{s['trendingList-bodyItem-tag']}}\"\n                            style=\"{{'hot_tag_bg_color' | getBackgroundSpecColor(theme, item.data)}}\n                                {{'hot_tag_font_color' | getFontColor(theme, item.data)}}\">{{item.data.hot_tag}}\n                            <div class=\"{{s['trendingList-bodyItem-hightlight']}}\"></div> \n                        </div>\n                    </div>\n                    <div class=\"{{s['trendingList-bodyItem-hotvalue']}}\">{{item.data.hot_num}}</div>\n                </div>\n            </div>\n        </div>\n    `;\n    /* eslint-enable max-len */\n    static computed = {\n        isFirst() {\n            return this.data.get('pos') === 0;\n        },\n        paddingStyle() {\n            const isFirst = this.data.get('isFirst');\n            return `padding-top: ${isFirst ? 0.5625 : 0.9375}rem; padding-bottom: 0.16rem;`;\n        },\n        itemData() {\n            return this.data.get('feed.data');\n        },\n        boardList() {\n            const listData = this.data.get('itemData').board_list;\n            return listData.slice(0, 5);\n        },\n        borderStyle() {\n            const theme = this.data.get('theme');\n            const itemData = this.data.get('feed.data');\n            let color = '#eeeeee';\n            switch (theme) {\n                case 'light':\n                    color = itemData.border_color || '#eeeeee';\n                    break;\n                case 'night':\n                    color = itemData.border_color_night || '#2f3031';\n                    break;\n                case 'dark':\n                    color = itemData.border_color_dark || '#2d2d2d';\n                    break;\n            }\n            return `border-bottom: .33px solid ${color};`;\n        },\n        titleFontSize() {\n            const fontLevel = +this.data.get('fontLevel');\n            let result = '';\n            switch (fontLevel) {\n                case 0:\n                    result = 'font-size: 0.875rem';\n                    break;\n                case 1:\n                    result = 'font-size: 1.0625rem';\n                    break;\n                case 2:\n                    result = 'font-size: 1.1875rem';\n                    break;\n                case 3:\n                    result = 'font-size: 1.25rem';\n                    break;\n            }\n            return result;\n        },\n        isFirst() {\n            return this.data.get('pos') === 0;\n        },\n        itemMaxWidth() {\n            const width = document.documentElement.clientWidth;\n            return width * 0.73;\n        }\n    }\n    static filters = {\n        tagIndexColor(item) {\n            const theme = this.data.get('theme');\n            const attribute = `index_bg_${theme === 'light' ? '' : theme + '_'}color`;\n            return `color: ${item[attribute]}`;\n        },\n        tagBackgroundColor(item) {\n            const theme = this.data.get('theme');\n            const font = `hot_tag_font_${theme === 'light' ? '' : theme + '_'}color`;\n            const background = `hot_tag_bg_${theme === 'light' ? '' : theme + '_'}color`;\n            return `background-color: ${item[background]};color: ${item[font]}`;\n        },\n        getThemeAttr(attr, theme) {\n            const itemData = this.data.get('itemData');\n            return itemData[`${attr}${theme === 'light' ? '' : '_' + theme}`];\n        },\n        getFontColor(attr, theme, itemData) {\n            if (attr === 'hot_tag_font_color' && theme === 'night') {\n                return 'color: rgb(221,0,0);';\n            }\n            const attribute = attr + `${theme === 'light' ? '' : '_' + theme}`;\n            return itemData[attribute] ? `color: ${itemData[attribute]};` : '';\n        },\n        getIndexColor(attr, theme, itemData) {\n            if (theme === 'night') {\n                return 'color: rgb(221,0,0);';\n            } else {\n                const attribute = attr + `${theme === 'light' ? '' : '_' + theme}`;\n                return itemData[attribute] ? `color: ${itemData[attribute]};` : '';\n            }\n        },\n        getBackgroundColor(attr, theme, itemData) {\n            const attribute = attr + `${theme === 'light' ? '' : '_' + theme}`;\n            return itemData[attribute] ? `background-color: ${itemData[attribute]};` : '';\n        },\n        getBackgroundSpecColor(attr, theme, itemData) {\n            if (theme === 'night') {\n                return 'background-color: #79251F;';\n            } else {\n                const attribute = attr + `${theme === 'light' ? '' : '_' + theme}`;\n                return itemData[attribute] ? `background-color: ${itemData[attribute]};` : '';\n            }\n        },\n        getBackgroundImage(attr, theme, itemData) {\n            const attribute = attr + `${theme === 'light' ? '' : '_' + theme}`;\n            return itemData[attribute] ? `background-image: ${itemData[attribute]};` : '';\n        }\n    }\n    initData() {\n        return {\n            s,\n            theme: 'light',\n            moreIcon: require('./image_arrow_light.png'),\n            feed: {},\n            logFlag: 0\n        };\n    }\n    attached() {\n        const visible = this.data.get('visibility');\n        // 页面一开始就可视的话，进行展现打点\n        visible && this.displayLog();\n        // 监听可视状态变化\n        this.watch('visibility', val => {\n            const logFlag = this.data.get('logFlag');\n            val && !logFlag && this.displayLog();\n        });\n    }\n    displayLog() {\n        const itemData = this.data.get('itemData');\n        ubc('event', {\n            page: this.data.get('feed.serverName'),\n            type: 'show',\n            value: itemData.board_name,\n            ext: {\n                id: this.data.get('tabId'),\n                pos: this.data.get('pos')\n            }\n        });\n        this.data.set('logFlag', 1);\n    }\n    itemClick(e, item) {\n        if (item.data && item.data.cmd) {\n            invoke(item.data.cmd);\n            ubc('event', {\n                page: this.data.get('feed.serverName'),\n                type: 'clk',\n                value: item.data.title,\n                ext: {\n                    id: this.data.get('tabId'),\n                    pos: this.data.get('pos')\n                }\n            });\n        }\n    }\n\n    moreClick() {\n        const cmd = this.data.get('feed.data.more_cmd');\n        if (!cmd) {\n            return;\n        }\n        invoke(cmd);\n        ubc('event', {\n            page: this.data.get('feed.serverName'),\n            type: 'clk',\n            value: 'more',\n            ext: {\n                id: this.data.get('tabId')\n            }\n        });\n    }\n}\n"]}]}