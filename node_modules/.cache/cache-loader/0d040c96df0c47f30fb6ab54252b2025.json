iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * @file 视频大卡\n * @author lizhe21(lizhe21@baidu.com)\n */\nimport {Component} from 'san';\nimport './style.less';\nimport ubc from '@baidu/ubc-report-sdk';\nimport {log} from '@/lib/utils/index';\n\nexport default class BigVideo extends Component {\n    static template = require('./template.html');\n    followBtnClick(index, e) {\n        e.stopPropagation();\n        this.fire('followClick', {index, dataIndex: this.data.get('index')});\n        ubc('cardLog.event', {\n            type: 'bigVideoFollow',\n            value: 'big_video'\n        });\n    }\n\n    showMoreClick(data) {\n        const {guide_word_scheme, jump_type} = data;\n        this.fire('clickInvoke', guide_word_scheme);\n        ubc('cardLog.event', {\n            type: +jump_type ? 'videoArticleClick' : 'videoEncyclopediaClick',\n            value: +jump_type ? 'video_article' : 'video_encyclopedia'\n        });\n    }\n    headInfoClick(data) {\n        const {auhomepagecmd} = data;\n        this.fire('clickInvoke', auhomepagecmd);\n        ubc('cardLog.event', {\n            type: 'bigVideoAuthorClick',\n            value: 'big_video'\n        });\n    }\n    videoClick(data) {\n        const {\n            cmd,\n            id: rid = '',\n            resource_type: resourceType = ''\n        } = data;\n        this.fire('clickInvoke', cmd);\n        ubc('cardLog.event', {\n            type: 'bigVideoClick',\n            value: 'big_video'\n        });\n        // 视频点击分发打点\n        log('clk', {\n            rid,\n            resourceType\n        });\n    }\n    attached() {\n        ubc('cardLog.event', {\n            type: 'bigVideoShow',\n            value: 'big_video'\n        });\n\n        this.data.get('videoBig.data').forEach(item => {\n            const {\n                guide_word_url,\n                jump_type,\n                id: rid = '',\n                resource_type: resourceType = ''\n            } = item;\n            // 词条 展现ubc打点\n            if (guide_word_url) {\n                ubc('cardLog.event', {\n                    type: +jump_type ? 'videoArticleShow' : 'videoEncyclopediaShow',\n                    value: +jump_type ? 'video_article' : 'video_encyclopedia'\n                });\n            }\n            // 视频展现分发打点\n            log('show', {\n                rid,\n                resourceType\n            })\n        });\n    }\n\n    initData() {\n        return {\n            videoBig: {},\n            topBg: {}\n        };\n    }\n\n}"]}]}