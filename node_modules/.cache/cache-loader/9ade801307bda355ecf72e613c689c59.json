AL,CAAUC,GAAV,CAAc,OAAd,CAAd;AACA,QAAMN,IAAI,GAAG,KAAKK,IAAL,CAAUC,GAAV,CAAc,MAAd,CAAb;;AAEA,QAAIX,KAAJ,EAAW;AACPe,MAAAA,KAAK,CAACC,IAAN,GAAahB,KAAb;AACH;;AAED,QAAI,wBAAUK,IAAV,CAAJ,EAAqB;AACjB,UAAMY,KAAK,aAAM,qBAAOZ,IAAP,CAAN,QAAX;AACAU,MAAAA,KAAK,CAACE,KAAN,GAAcF,KAAK,CAACG,MAAN,GAAeD,KAA7B;AACH;;AAED,WAAOF,KAAP;AACH,GA3Ba;AA4BdI,EAAAA,GA5Bc,iBA4BR;AACF,QAAMhB,IAAI,GAAG,KAAKO,IAAL,CAAUC,GAAV,CAAc,MAAd,KAAyB,KAAKD,IAAL,CAAUC,GAAV,CAAc,MAAd,CAAtC;AACA,wCAA4BR,IAA5B;AACH;AA/Ba,C;;AAhClB,IAAIiB,MAAM,CAACC,GAAX,EAAgB;AACZ,MAAIC,WAAW,GAAGC,OAAO,CAAC,2FAAD,CAAzB;;AACA,MAAIC,aAAa,GAAGD,OAAO,CAAC,4EAAD,CAA3B;;AAEA,MAAIE,iBAAiB,GAAGD,aAAa,CAACE,UAAd,CAAyBN,MAAzB,CAAxB;;AACA,MAAIK,iBAAiB,CAACE,QAAlB,IAA8BF,iBAAiB,CAACG,SAAlB,CAA4BD,QAA9D,EAAwE;AACpEP,IAAAA,MAAM,CAACC,GAAP,CAAWQ,MAAX;;AACAP,IAAAA,WAAW,CAACQ,OAAZ,CAAoBP,OAAO,CAAC,KAAD,CAA3B;;AAEA,QAAIQ,UAAU,GAAG,UAAjB;;AACA,QAAI,CAACX,MAAM,CAACC,GAAP,CAAWX,IAAhB,EAAsB;AAClBY,MAAAA,WAAW,CAACU,YAAZ,CAAyBD,UAAzB,EAAqCN,iBAArC;AACH,KAFD,MAGK;AACDH,MAAAA,WAAW,CAACW,SAAZ,CAAsBF,UAAtB,EAAkCN,iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * @file 组件 WujiIcon 无极-图标\n * @author baozhixin <baozhixin@baidu.com>\n */\n\nimport './style/index.less';\nimport {Component, DataTypes} from 'san';\nimport isNumeric from '../utils/is-numeric';\nimport pr2rem from '../utils/pr2rem';\n\nconst prefixCls = 'w-icon';\n\nexport default class WujiIcon extends Component {\n    static template = /* html */ `\n        <svg class=\"{{iconClass}}\" style=\"{{iconStyle}}\" on-click=\"handleClick\">\n            {{use | raw}}\n        </svg>\n    `;\n\n    static dataTypes = {\n        color: DataTypes.string,\n        type: DataTypes.string,\n        name: DataTypes.string,\n        size: DataTypes.oneOfType([\n            DataTypes.number,\n            DataTypes.string,\n            DataTypes.oneOf([\n                'auto',\n                'half',\n            ]),\n        ]),\n    };\n\n    static computed = {\n        iconClass() {\n            const size = this.data.get('size');\n\n            return [\n                prefixCls,\n                size === 'auto' && `${prefixCls}-auto`,\n                size === 'half' && `${prefixCls}-half`,\n            ].filter(\n                c => c\n            );\n        },\n        iconStyle() {\n            let style = {};\n            const color = this.data.get('color');\n            const size = this.data.get('size');\n\n            if (color) {\n                style.fill = color;\n            }\n\n            if (isNumeric(size)) {\n                const width = `${pr2rem(size)}rem`;\n                style.width = style.height = width;\n            }\n\n            return style;\n        },\n        use() {\n            const type = this.data.get('type') || this.data.get('name');\n            return `<use xlink:href=\"#${type}\"/>`;\n        },\n    };\n\n    handleClick(event) {\n        this.fire('click', {event});\n    }\n}\n"]}]}