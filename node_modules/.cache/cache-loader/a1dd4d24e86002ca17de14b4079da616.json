,CAAWnC,IAAhB,EAAsB;AAClBoC,MAAAA,WAAW,CAACU,YAAZ,CAAyBD,UAAzB,EAAqCN,iBAArC;AACH,KAFD,MAGK;AACDH,MAAAA,WAAW,CAACW,SAAZ,CAAsBF,UAAtB,EAAkCN,iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * @file 柱状图组件\n * @author liuyuxin06@baidu.com\n */\n/* eslint-disable fecs-camelcase */\nimport {Component} from 'san';\nimport styles from './barChart.module.less';\nimport * as echarts from 'echarts/lib/echarts';\nimport 'echarts/lib/chart/bar';\nimport 'echarts/lib/component/title';\nimport 'echarts/lib/component/legend';\nimport 'echarts/lib/component/tooltip';\nimport 'echarts/lib/component/grid';\nimport 'echarts/lib/component/visualMap';\n\nexport default class BarChart extends Component {\n    /* eslint-disable max-len */\n    static template = `\n        <div class=\"{{styles['barChart']}}\"\n            style=\"padding: .9375rem .75rem;\">\n            <div class=\"{{styles['barChart-title']}}\">昨日新增国家TOP10</div>\n            <div id=\"echarts-container-bar\" style=\"width: {{chartsWidth}}px; height: 300px;\"></div>\n            <div class=\"{{styles['barChart-bottom']}}\">\n                <span>*趋势图与国家卫健委官网发布一致，每日更新一次</span>\n            </div>\n        </div>\n    `;\n    /* eslint-enable max-len */\n\n    static computed = {\n        chartsWidth() {\n            return this.data.get('underTalosTab')\n                ? document.documentElement.clientWidth - 26\n                : document.documentElement.clientWidth - 52;\n        }\n    };\n    getBaseOptions(feedItem) {\n        let maxValue = 0;\n        let minValue = 0;\n        let source = [['score', 'value', 'name']];\n        if (feedItem) {\n            feedItem.map(item => {\n                source.push([item.value, item.value, item.name]);\n            });\n            maxValue = feedItem[0].value;\n            minValue = feedItem[feedItem.length - 1].value;\n        }\n        // https://www.echartsjs.com/examples/zh/index.html#chart-type-bar\n        const defaultOptions =  {\n            animationDuration: 0,\n            title: {\n                text: '单位: 例',\n                textStyle: {\n                    color: '#848691',\n                    fontSize: 12,\n                    fontWeight: 'normal'\n                },\n                textAlign: 'left',\n                left: '-5'\n            },\n            tooltip: {\n                confine: true,\n                axisPointer: {\n                    lineStyle: {\n                        type: 'dotted',\n                        color: 'rgba(77, 80, 84, .4)'\n                    }\n                },\n                formatter(params) {\n                    let html = [\n                        `<span>${params.name}</span>`,\n                        `<span style=\"display: inline-block;transform: scale(0.7);margin-left: -3px;\">\n                            ${echarts.format.getTooltipMarker({color: params.color})}\n                         </span>`\n                        + `<span>新增: ${params.value[1]}</span>`\n                    ];\n\n                    return html.join('<br/>');\n                },\n                backgroundColor: '#FFF',\n                borderColor: 'rgba(77, 80, 84, .2)',\n                borderWidth: 1,\n                padding: [5, 8, 5, 8],\n                extraCssText: 'text-align: left;box-shadow: 0 5px 15px 0 rgba(0, 0, 0, 0.1);border-radius: 5px;',\n                textStyle: {\n                    color: '#333333',\n                    fontSize: 12,\n                    lineHeight: 18\n                }\n            },\n            xAxis: {\n                axisTick: {\n                    show: false\n                },\n                axisLine: {\n                    show: false,\n                    lineStyle: {\n                        color: '#F8F8F8'\n                    }\n                },\n                axisLabel: {\n                    fontSize: 10,\n                    color: '#848691',\n                    rotate: 45\n                },\n                type: 'category'\n            },\n            yAxis: {\n                // y轴\n                axisLine: {\n                    show: false\n                },\n                // y轴刻度线\n                axisTick: {\n                    show: false\n                },\n                axisLabel: {\n                    fontSize: 10,\n                    color: '#848691',\n                    formatter: value => {\n                        return value;\n                    }\n                },\n                splitLine: {\n                    lineStyle: {\n                        color: '#F8F8F8'\n                    }\n                },\n                type: 'value'\n            },\n            visualMap: {\n                show: false,\n                min: minValue,\n                max: maxValue,\n                dimension: 0,\n                inRange: {\n                    color: ['#FAC575', '#D92121']\n                }\n            },\n            grid: {\n                top: 31,\n                left: 2,\n                right: '1%',\n                bottom: '3%',\n                width: '100%',\n                containLabel: true,\n                show: true,\n                borderWidth: 0\n            },\n            emphasis: {\n                itemStyle: {\n                    // 高亮时点的颜色。\n                    // color: '#10AEB5'\n                    opacity: 0.5\n                }\n            },\n            dataset: {\n                source: source\n            },\n            series: [{\n                type: 'bar',\n                barWidth: 10,\n                encode: {\n                    // 将 \"name\" 列映射到 X 轴。\n                    x: 'name',\n                    // 将 \"value\" 列映射到 Y 轴。\n                    y: 'value'\n                },\n                itemStyle: {\n                    opacity: 1\n                }\n            }]\n        };\n\n        return defaultOptions;\n    }\n\n    changeEchartsStyle(option, setTheme) {\n        let theme = setTheme ? setTheme : this.data.get('theme');\n        if (theme === 'kangyi-night') {\n            option.xAxis.axisLabel.color = '#444';\n            option.yAxis.axisLabel.color = '#444';\n            option.yAxis.splitLine.lineStyle.color = '#303030';\n            option.title.textStyle.color = '#444';\n            option.tooltip.backgroundColor = '#121212';\n            option.tooltip.textStyle.color = '#666';\n            option.series.forEach(item => {\n                item.itemStyle.opacity = 0.5;\n            });\n        } else if (theme === 'kangyi-dark') {\n            option.xAxis.axisLabel.color = 'rgba(255,255,255,0.5)';\n            option.yAxis.axisLabel.color = 'rgba(255,255,255,0.5)';\n            option.yAxis.splitLine.lineStyle.color = '#222';\n            option.title.textStyle.color = 'rgba(255,255,255,0.5)';\n            option.tooltip.backgroundColor = '#1e1f24';\n            option.tooltip.textStyle.color = 'rgba(255,255,255,0.8)';\n            option.series.forEach(item => {\n                item.itemStyle.opacity = 1;\n            });\n        } else {\n            option.xAxis.axisLabel.color = '#848691';\n            option.yAxis.axisLabel.color = '#848691';\n            option.yAxis.splitLine.lineStyle.color = '#F8F8F8';\n            option.title.textStyle.color = '#848691';\n            option.tooltip.backgroundColor = '#fff';\n            option.tooltip.textStyle.color = '#333';\n            option.series.forEach(item => {\n                item.itemStyle.opacity = 1;\n            });\n        }\n        return option;\n    }\n\n    drawBarChart(newItems, setTheme = '') {\n        let barChart = echarts.getInstanceByDom(document.getElementById('echarts-container-bar'));\n        if (barChart == null) { // 如果不存在，就进行初始化。\n            barChart = echarts.init(document.getElementById('echarts-container-bar'));\n        }\n        let theme = setTheme ? setTheme : this.data.get('theme');\n        let baseOption = this.getBaseOptions(newItems);\n        baseOption = this.changeEchartsStyle(baseOption, theme);\n        barChart.setOption(baseOption);\n    }\n    attached() {\n        // 柱状图配置\n        const barChart = echarts.init(document.getElementById('echarts-container-bar'));\n        const feedItem = this.data.get('feed.items');\n        let baseOption = this.getBaseOptions(feedItem);\n        baseOption = this.changeEchartsStyle(baseOption);\n        barChart.setOption(baseOption);\n\n        this.watch('feed.items', newItems => {\n            setTimeout(() => {\n                this.drawBarChart(newItems);\n            }, 0);\n        });\n\n        this.watch('theme', val => {\n            const feedItems = this.data.get('feedItems');\n            this.drawBarChart(feedItems, val);\n        });\n        window.addEventListener('resize', this.resizeChat.bind(this));\n    }\n    resizeChat() {\n        let newChartsWidth = this.data.get('underTalosTab')\n                ? document.documentElement.clientWidth - 26\n                : document.documentElement.clientWidth - 52;\n        if (this.data.get('chartsWidth') === newChartsWidth) {\n            return;\n        }\n        if (this.resizeTimer) {\n            clearTimeout(this.resizeTimer);\n        }\n        this.data.set('chartsWidth', newChartsWidth);\n        let myChart = echarts.getInstanceByDom(document.getElementById('echarts-container-bar'));\n        this.resizeTimer = setTimeout(() => {\n            this.resizeTimer = null;\n            myChart && myChart.resize();\n        }, 300);\n\n    }\n\n    initData() {\n        return {\n            styles,\n            chartsHeight: (document.documentElement.clientWidth - 52) * 0.67\n        };\n    }\n}\n\n"]}]}