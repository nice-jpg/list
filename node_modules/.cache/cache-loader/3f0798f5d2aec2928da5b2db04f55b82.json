ined,\n        focus: undefined,\n        majorPrograms: undefined,\n        jumpChannel: undefined,\n        introduce: undefined\n    },\n    actions: {\n        ['main:updatePageInfo'](data, {dispatch, getState}) {\n            const {\n                modules\n            } = data;\n            if (modules.length) {\n                modules.map(item => {\n                    dispatch(`main:${item.type}Data`, item);\n                });\n            }\n            dispatch('main:updatePageData', data);\n        },\n        ['main:orderData'](data) {\n            return builder().set('order', data);\n        },\n        ['main:focusData'](data) {\n            return builder().set('focus', data);\n        },\n        ['main:major_programsData'](data) {\n            return builder().set('majorPrograms', data);\n        },\n        ['main:jump_channelData'](data) {\n            return builder().set('jumpChannel', data);\n\n        },\n        ['main:introduceData'](data) {\n            return builder().set('introduce', data);\n        },\n        ['main:updatePageData'](data) {\n            return builder().set('pageData', data);\n        },\n        ['main:updateFocusFollow']({index}, {getState}) {\n            const focus = getState('focus');\n            const followData = focus.data;\n            const oldData = followData[index];\n            followData[index] = Object.assign({}, oldData, {\n                followed: oldData.followed ? 0 : 1\n            });\n            return builder().set('focus.data', {...followData});\n        },\n        ['main:updateOrder']({data} = {}, {getState}) {\n            const result = {\n                ...getState('order'),\n                ...data\n            };\n            return builder().set('order', {...result});\n        },\n        ['invoke']({urlCmd}, {getState}) {\n            const {isBDBox} = getState('env');\n            if (isBDBox) {\n                invoke(urlCmd);\n            } else {\n                invokeApp({\n                    appName: 'baiduboxapp',\n                    scheme: urlCmd\n                });\n            }\n        }\n    }\n};\n\nstore.initData(main.initData).addActions(main.actions);"]}]}