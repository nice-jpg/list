Button from '../../wuji-button';\n/*\n * type字段枚举值说明:\n * 0--单个按钮\n * 1--文字+按钮\n * 2--价格+按钮\n * 3--价格\n * 4--双按钮\n */\n\nexport default class TextButton extends Component {\n    static template = /* html */ `\n        <div class=\"text-button\">\n            <wuji-row column=\"24\" class=\"gridContainer\" gutter=\"24\" margin=\"0\" align=\"center\">\n                <wuji-col s-if=\"type !== 0\">\n                    <div s-if=\"type === 1\" class=\"text\">\n                        <span class=\"text1\">{{text1}}</span>\n                        <span class=\"text2\">{{text2}}</span>\n                    </div>\n                    <div s-if=\"type === 2 || type === 3\" class=\"text\">\n                        <span class=\"text1-price\">{{text1}}</span>\n                        <span class=\"text2-unit\">{{text2}}</span>\n                    </div>\n                    <wuji-button\n                        s-if=\"type === 4\"\n                        theme=\"outline\"\n                        text=\"{{btnText1}}\"\n                        height=\"84\"\n                        color=\"{{btnColor1}}\"\n                    />\n                </wuji-col>\n                <wuji-col\n                    s-if=\"type === 0 || type === 1 || type === 2 || type === 4\"\n                    span=\"{{span}}\"\n                >\n                    <wuji-button theme=\"outline\" text=\"{{btnText2}}\" height=\"84\" color=\"{{btnColor2}}\"/>\n                </wuji-col>\n            </wuji-row>\n        </div>\n    `;\n    static components = {\n        'wuji-row': WujiGrid.Row,\n        'wuji-col': WujiGrid.Col,\n        'wuji-button': WujiButton,\n    };\n}\n"]}]}