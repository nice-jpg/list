,CAAP;AACA;AACH;;AACD,YAAIxD,SAAIW,WAAJ,IAAmBX,SAAIa,QAA3B,EAAqC;AACjC,cAAIb,SAAIc,KAAR,EAAe;AACX,iCAAQ,8BAAR,EAAwCX,IAAxC,CAA6C,UAAAC,GAAG,EAAI;AAChDoD,cAAAA,OAAO,CAAC,CAACpD,GAAG,CAACgD,MAAN,CAAP;AACH,aAFD,EAEG,YAAM;AACLI,cAAAA,OAAO,CAAC,MAAI,CAACjD,IAAL,CAAU6B,GAAV,CAAc,wBAAd,CAAD,CAAP;AACH,aAJD;AAKH,WAND,MAMO;AACHoB,YAAAA,OAAO,CAAC,MAAI,CAACjD,IAAL,CAAU6B,GAAV,CAAc,wBAAd,CAAD,CAAP;AACH;AACJ;AACJ,OAhBM,CAAP;AAiBH;;;+BAEU;AACP,aAAO;AACH4B,QAAAA,IAAI,EAAE,EADH;AAEHC,QAAAA,MAAM,EAANA,kBAFG;AAGHC,QAAAA,eAAe,EAAE,KAHd;AAIHC,QAAAA,WAAW,EAAE,KAJV;AAKHC,QAAAA,sBAAsB,EAAE,EALrB;AAKyB;AAC5BC,QAAAA,eAAe,EAAE,CANd;AAMiB;AACpBC,QAAAA,qBAAqB,EAAE,EAPpB;AAOwB;AAC3BC,QAAAA,OAAO,EAAE;AARN,OAAP;AAUH;;;EA/S4BC,sB;;;8BAAZ9E,G,gBAEG;AAChB,eAAa+E,iBADG;AAEhB,eAAaC,iBAFG;AAGhB,kBAAgBC;AAHA,C;8BAFHjF,G;8BAAAA,G,YAkDD,E;8BAlDCA,G,cAoDC;AACdkF,EAAAA,cADc,4BACG;AACb,QAAIlD,IAAI,GAAG,KAAKnB,IAAL,CAAU6B,GAAV,CAAc,WAAd,CAAX;AACA,QAAIyC,SAAS,GAAG,KAAKtE,IAAL,CAAU6B,GAAV,CAAc,gBAAd,KAAmC,EAAnD;AACA,QAAI0C,GAAG,GAAG,EAAV;;AACA,QAAIpD,IAAI,KAAK,UAAb,EAAyB;AACrBoD,MAAAA,GAAG,GAAGD,SAAS,CAACE,IAAhB;AACH,KAFD,MAEO;AACHD,MAAAA,GAAG,GAAGD,SAAS,CAACG,GAAhB;AACH;;AACD,WAAOF,GAAP;AACH,GAXa;AAYdG,EAAAA,YAZc,0BAYC;AACX,QAAI,CAACjF,SAAIW,WAAJ,IAAmBX,SAAIY,cAAvB,IAAyCZ,SAAIa,QAA9C,KAA2Db,SAAIkF,YAAnE,EAAiF;AAC7E,aAAO,KAAK3E,IAAL,CAAU6B,GAAV,CAAc,iBAAd,CAAP;AACH,KAFD,MAEO;AACH,aAAO,KAAP;AACH;AACJ,GAlBa;AAmBd+C,EAAAA,UAnBc,wBAmBD;AACT,WAAOnF,SAAIW,WAAX;AACH,GArBa;AAsBdyE,EAAAA,kBAtBc,gCAsBO;AACjB;AACA,WAAO,KAAK7E,IAAL,CAAU6B,GAAV,CAAc,aAAd,IACD5C,OAAO,CAAC,CAAC,KAAKe,IAAL,CAAU6B,GAAV,CAAc,iBAAd,CAAD,GAAoC,CAArC,CADN,GAED,CAFN;AAGH;AA3Ba,C","sourcesContent":["/**\n * @file 容器组件\n * @author leizhihao <leizhihao@baidu.com>\n */\n/* eslint-disable fecs-camelcase */\nimport ubc from '@baidu/ubc-report-sdk';\nimport axios from 'axios';\nimport {env} from '@baidu/auc-base/env';\nimport styles from './app.module.less';\nimport SpyClient from 'spy-client/dist/spy-client';\nimport throttle from 'lodash.throttle';\nimport ListItem from '../components/listItem';\nimport axiosJsonp from 'axios-jsonp';\nimport AucTitlebar from '@baidu/auc-titlebar';\nimport {getPageMode, registerModeChange, setPageMode} from \"@/lib/utils/pageMode\";\nimport {startFontSize} from \"@/lib/utils/fontSize\";\nimport AucComponent from \"@baidu/auc-base/AucComponent\";\nimport {WujiIcon} from '@baidu/wuji-san';\nimport boxx from \"@baidu/auc-base/boxx\";\nimport {setShareInfo} from \"@/lib/utils/setShareInfo\";\nimport {getQueryString} from \"@/lib/utils/getQueryString\";\nimport {invokeP} from \"@baidu/xbox/na/invoke\";\n\nimport '@baidu/wuji-icons/svg/d20-titlebar-more-o-66.svg';\n\nconst pxToRem = value => value / 100 + 'rem';\n\nexport default class App extends AucComponent {\n\n    static components = {\n        'list-item': ListItem,\n        'wuji-icon': WujiIcon,\n        'auc-titlebar': AucTitlebar,\n    };\n    /* eslint-disable max-len */\n    static template = `\n        <div id=\"appContainer\">\n            <div\n                class=\"{{styles.headContainer}}\"\n                s-ref=\"headContainer\"\n                style=\"background-image: url({{headBackground}});\"\n            >\n                <div\n                    class=\"{{styles.menuIcon}}\"\n                    s-if=\"isShowMenu\"\n                    on-click=\"clickMenu\"\n                    style=\"top: {{statusBarHeightRem}};\"\n                >\n                    <wuji-icon type=\"d20-titlebar-more-o-66\" size=\"66\"></wuji-icon>\n                </div>\n            </div>\n            <div\n                s-if=\"{{showTitleBar && isImmersive}}\"\n                style=\"height: {{statusBarHeightRem}};\"\n                class=\"{{styles.titleImmersive}}\">\n            </div>\n            <auc-titlebar\n                title=\"百度热议榜\"\n                style=\"top: {{statusBarHeightRem}};\"\n                s-if=\"showTitleBar\"\n                on-click-set=\"clickMenu\"\n                s-ref=\"titleBar\"\n            ></auc-titlebar>\n            <div class=\"{{styles.contentTop}}\">\n            </div><div class=\"{{styles.listContainer}}\">\n                <list-item\n                    s-for=\"item,index in main.itemList\"\n                    index=\"{{index + 1}}\"\n                    data=\"{{item}}\"\n                    class=\"{{styles.listItem}}\"\n                >\n                </list-item>\n            </div>\n        </div>\n    `;\n    /* eslint-enable max-len */\n\n    static filter = {}\n\n    static computed = {\n        headBackground() {\n            let mode = this.data.get('main.mode');\n            let headImage = this.data.get('main.headImage') || {};\n            let src = '';\n            if (mode === 'darkmode') {\n                src = headImage.dark;\n            } else {\n                src = headImage.day;\n            }\n            return src;\n        },\n        showTitleBar() {\n            if ((env.isBoxSeries || env.isXiaoMiBaiPai || env.isMatrix) && env.isLightFrame) {\n                return this.data.get('appShowTitleBar');\n            } else {\n                return false;\n            }\n        },\n        isShowMenu() {\n            return env.isBoxSeries;\n        },\n        statusBarHeightRem() {\n            // 手动加高点，iOS可能会出现间隙\n            return this.data.get('isImmersive')\n                ? pxToRem(+this.data.get('statusBarHeight') + 7)\n                : 0;\n        },\n    }\n\n    clickMenu() {\n        boxx.call('easyBrowser.showMenu');\n    }\n\n    attached() {\n        this.pageInit();\n    }\n\n    pageInit() {\n\n        let isTomas = /tomas/i.test(env.ua);\n\n        // 获取数据\n        this.fetchAllData().then(res => {\n            // 检查是否显示titlebar(可能页面已经滑动了一部分)\n            this.checkShowTitleBar();\n\n            // 滚动相关事件监听\n            this.scrollListen();\n\n            this.data.set('loadEnd', true);\n        });\n\n        // 是否是沉浸式模式\n        this.data.set(\n            'isImmersive',\n            getQueryString('isimmersive') === 'immersive'\n        );\n\n        // 状态栏高度\n        this.titleBarOffsetSet().then(value => {\n            this.data.set('statusBarHeight', +value);\n        });\n\n        // 暗黑模式/夜间模式监听\n        this.pageModeInit();\n\n        // 字体大小变化监听\n        (env.isBoxSeries || env.isXiaoMiBaiPai || env.isMatrix) && !isTomas && env.isIOS && startFontSize();\n\n        // 设置分享数据\n        this.setShareData();\n\n        // 设置收藏数据\n        this.setFavoriteInfo();\n    }\n\n    setShareData() {\n        setShareInfo({\n            title: '百度热议榜',\n            description: '最热最新话题，尽在百度app~',\n            link: location.href,\n            icon: 'https://b.bdstatic.com/searchbox/icms/searchbox/img/baidu.png'\n        });\n    }\n\n    setFavoriteInfo() {\n        (env.isBoxSeries || env.isMatrix) && boxx.call('feed.updateFavorInfo', {\n            title: '百度热议榜',\n            ukey: 'baiduapp_rebang://rebang',\n            url: location.href,\n            cmd: {\n                mode: '2',\n                url: 'baiduboxapp://easybrowse?opentype=1&sfrom=feed&openurl=%7B%22switches%22%3A%224%22%2C%22showtitlebar%22%3A%220%22%2C%22showtoolbar%22%3A%221%22%2C%22toolbaricons%22%3A%7B%22toolids%22%3A%5B%223%22%5D%7D%2C%22menuitem%22%3A%7B%7D%7D&isla=0&rbtnstyle=0&toolbaricons=2&menumode=%!s(MISSING)'\n            }\n        });\n    }\n\n    async pageModeInit() {\n        if (env.isBoxSeries || env.isXiaoMiBaiPai || env.isMatrix) {\n            const modeMap = {\n                lightmode: 'lightmode',\n                nightmode: 'nightmode',\n                darkmode: 'darkmode'\n            };\n\n            setPageMode();\n            const pageMode = await getPageMode();\n            this.data.set('pageMode', modeMap[pageMode]);\n            registerModeChange((mode) => {\n                this.data.set('pageMode', modeMap[mode]);\n            });\n        }\n    }\n\n    getData(page, pagesize = 20) {\n        const HOST = 'https://ext.baidu.com';\n        return axios.get(`${HOST}/api/hotrank/v1/common/get`, {\n            params: {\n                page,\n                pagesize\n            },\n            adapter: axiosJsonp,\n            callbackParamName: 'callback'\n        }).then(res => {\n            // res.data = {\n            //     \"errno\": 0, \"request_id\": \"1606298372\", \"errmsg\": \"\", \"data\": {\n            //         \"total\": 3,\n            //         \"conf\": {\n            //             \"head_image\": {\n            //                 \"day\": \"https://b.bdstatic.com/searchbox/image/gcp/20220915/3500642166.png\",\n            //                 \"night\": \"https://b.bdstatic.com/searchbox/image/gcp/20220915/3713084190.png\",\n            //                 \"dark\": \"https://b.bdstatic.com/searchbox/image/gcp/20220915/3500642166.png\"\n            //             }\n            //         },\n            //         \"itemlist\": [{\n            //             \"rank_num\": 2,\n            //             \"id\": \"sv_2831215558757860542\",\n            //             \"layout\": \"image1\",\n            //             \"data\": {\n            //                 \"mode\": \"video\",\n            //                 \"title\": \"俄媒：赫尔松决定就加入俄罗斯举行全民公投 俄罗斯乌克兰局势 点击关注\",\n            //                 \"source\": \"每日经济新闻\",\n            //                 \"comment_num\": 147,\n            //                 \"cmd\": {\"url\": \"\"},\n            //                 \"image\": \"http://vd3.bdstatic.com/mda-nijiqagbtj2vk5ea/1663679558/mda-nijiqagbtj2vk5ea00000000.jpg\",\n            //                 \"comment_bar\": [],\n            //                 \"duration\": \"00:00:15\"\n            //             }\n            //         }, {\n            //             \"rank_num\": 3,\n            //             \"id\": \"sv_4798762803129369267\",\n            //             \"layout\": \"image1\",\n            //             \"data\": {\n            //                 \"mode\": \"video\",\n            //                 \"title\": \"小伙上初中时借住在伯父伯母家，工作后每年都会来探望，堂姐：每次送礼都跟进货似的（乐行观察）\",\n            //                 \"source\": \"法制云播报\",\n            //                 \"comment_num\": 3280,\n            //                 \"cmd\": {\"url\": \"\"},\n            //                 \"image\": \"http://pic.rmb.bdstatic.com/bjh/video/48ea24a25d3f2b258b0cbe01fb28de25.jpeg?for=bg\",\n            //                 \"comment_bar\": {\"content\": \"懂得感恩的孩子懂得感恩的孩子懂得感恩的孩子懂得感恩的孩子懂得感恩的孩子懂得感恩的孩子懂得感恩的孩子[赞同]\", \"is_god_comment\": 0, \"scheme\": \"\"},\n            //                 \"duration\": \"00:00:11\"\n            //             }\n            //         }]\n            //     }\n            // };\n            if (+res.data.errno === 0) {\n                return res.data.data;\n            } else {\n                return Promise.reject('request errno');\n            }\n        });\n    }\n\n    scrollListen() {\n        window.addEventListener('scroll', throttle(() => {\n            // titlebar的动态显示\n            this.checkShowTitleBar();\n        }, 100));\n    }\n\n    checkShowTitleBar() {\n        const headContainer = this.ref('headContainer');\n        const rect = headContainer.getBoundingClientRect();\n        this.data.set(\n            'appShowTitleBar',\n            // 滑动过头图区域简单按照头图2/3区域判断\n            window.scrollY > headContainer.offsetTop + rect.height * 2 / 3\n        );\n    }\n\n    fetchAllData() {\n        let index = 1;\n        const pageSize = 20;\n\n        const loop = (resolve) => {\n            return this.fetchOnePage(index, pageSize).then(data => {\n                if (index * pageSize < data.total) {\n                    index++;\n                    loop(resolve);\n                } else {\n                    return resolve();\n                }\n            });\n        }\n\n        return new Promise((resolve) => {\n            loop(resolve);\n        });\n    }\n\n    fetchOnePage(page, pageSize = 20) {\n        return this.getData(page, pageSize).then(data => {\n            this.storeDispatch('main:updateHeadImage', data.conf.head_image);\n            this.storeDispatch('main:updateItemList', data.itemlist);\n            return data;\n        });\n    }\n\n    // 当是沉浸式时，获取状态栏高度\n    titleBarOffsetSet() {\n        return new Promise((resolve, reject) => {\n            if (!this.data.get('isImmersive')) {\n                resolve(0);\n                return;\n            }\n            if (env.isBoxSeries || env.isMatrix) {\n                if (env.isIOS) {\n                    invokeP('v16/utils/getStatusBarHeight').then(res => {\n                        resolve(+res.height);\n                    }, () => {\n                        resolve(this.data.get('defaultStatusBarHeight'));\n                    });\n                } else {\n                    resolve(this.data.get('defaultStatusBarHeight'));\n                }\n            }\n        });\n    }\n\n    initData() {\n        return {\n            list: [],\n            styles,\n            appShowTitleBar: false,\n            isImmersive: false,\n            defaultStatusBarHeight: 35, // 当获取不到状态栏高度时的默认值\n            statusBarHeight: 0, // 手机状态栏高度\n            defaultTitleBarHeight: 44, // titlebar高度\n            loadEnd: false,\n        };\n    }\n}\n\n"]}]}