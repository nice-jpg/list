n            style=\"{{wrapStyle}}\"\n        >\n            <input\n                class=\"${prefixCls}-input\"\n                type=\"radio\"\n                checked=\"{{checked}}\"\n                on-click=\"handleClick\"\n            />\n            <span class=\"${prefixCls}\">\n                <svg\n                    class=\"${prefixCls}-outer\"\n                    viewBox=\"0 0 60 60\"\n                    role=\"presentation\"\n                ><path d=\"${svgOuter}\"/></svg>\n                <svg\n                    class=\"${prefixCls}-inner\"\n                    viewBox=\"0 0 60 60\"\n                    role=\"presentation\"\n                ><path d=\"${svgInner}\"/></svg>\n            </span>\n            <span class=\"${prefixCls}-label\">{{label}}</span>\n        </label>\n    `;\n\n    static dataTypes = {\n        color: DataTypes.string,\n        checked: DataTypes.bool,\n        disabled: DataTypes.bool\n    };\n\n    static computed = {\n        wrapClass() {\n            const checked = this.data.get('checked');\n            const disabled = this.data.get('disabled');\n            return [\n                `${prefixCls}-wrap`,\n                checked && `${prefixCls}-checked`,\n                disabled && `${prefixCls}-disabled`\n            ].filter(c => c);\n        },\n        wrapStyle() {\n            const color = this.data.get('color');\n            return color ? {\n                'color': color\n            } : {};\n        }\n    };\n\n    initData() {\n        return {\n            color: '',\n            checked: false,\n            disabled: false,\n            toggle: false\n        };\n    }\n\n    handleClick() {\n        const {checked, disabled, toggle} = this.data.get();\n        if (disabled) {\n            return;\n        }\n        let newChecked = toggle ? !checked : checked;\n        this.data.set('checked', newChecked);\n        this.fire('click', newChecked);\n    }\n}\n"]}]}