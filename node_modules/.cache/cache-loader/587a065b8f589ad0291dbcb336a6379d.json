CAAc,OAAd,EAAuBZ,KAAK,GAAG,CAA/B;AACH;;;EAhGiCa,c;;;8BAAjBT,Q;AACC;uJAIGP,S,4EAECA,S,4HAGGA,S,+aAU0BA,S,kMAM9BA,S;8BA1BJO,Q,gBA+BG;AAChB,eAAaU;AADG,C;8BA/BHV,Q,eAmCE;AACfH,EAAAA,SAAS,EAAEc,eAAUC,IADN;AAEfX,EAAAA,KAAK,EAAEU,eAAUE,MAFF;AAGfX,EAAAA,QAAQ,EAAES,eAAUC,IAHL;AAIfjB,EAAAA,KAAK,EAAEgB,eAAUG,SAAV,CAAoB,CACvBH,eAAUE,MADa,EAEvBF,eAAUI,MAFa,CAApB,CAJQ;AAQfZ,EAAAA,IAAI,EAAEQ,eAAUK,KAAV,CAAgB,CAClB,IADkB,EAElB,IAFkB,CAAhB,CARS;AAYfZ,EAAAA,IAAI,EAAEO,eAAUI,MAZD;AAafV,EAAAA,YAAY,EAAEM,eAAUK,KAAV,CAAgB,CAC1B,MAD0B,EAE1B,OAF0B,CAAhB;AAbC,C;8BAnCFhB,Q,cAsDC;AACdiB,EAAAA,IADc,kBACP;AACH,QAAMX,IAAI,GAAG,KAAKA,IAAlB;AACA,QAAML,KAAK,GAAGK,IAAI,CAACC,GAAL,CAAS,OAAT,CAAd;AACA,QAAMZ,KAAK,GAAGW,IAAI,CAACC,GAAL,CAAS,OAAT,CAAd;AACA,QAAMV,SAAS,GAAGS,IAAI,CAACC,GAAL,CAAS,WAAT,CAAlB;AAEA,QAAMU,IAAI,GAAG,EAAb;;AACA,SAAK,IAAIC,CAAC,GAAG,CAAb,EAAgBA,CAAC,IAAIjB,KAArB,EAA4BiB,CAAC,EAA7B,EAAiC;AAC7BD,MAAAA,IAAI,CAACE,IAAL,CAAUzB,aAAa,CAACC,KAAD,EAAQuB,CAAR,EAAWrB,SAAX,CAAvB;AACH;;AAED,WAAOoB,IAAP;AACH,GAba;AAcdG,EAAAA,SAdc,uBAcF;AACR,QAAMjB,IAAI,GAAG,KAAKG,IAAL,CAAUC,GAAV,CAAc,MAAd,CAAb;AACA,WAAO,WACAd,SADA,sBAEAA,SAFA,cAEaU,IAFb,EAAP;AAIH;AApBa,C;;AAnFlB,IAAIkB,MAAM,CAACC,GAAX,EAAgB;AACZ,MAAIC,WAAW,GAAGC,OAAO,CAAC,2FAAD,CAAzB;;AACA,MAAIC,aAAa,GAAGD,OAAO,CAAC,4EAAD,CAA3B;;AAEA,MAAIE,iBAAiB,GAAGD,aAAa,CAACE,UAAd,CAAyBN,MAAzB,CAAxB;;AACA,MAAIK,iBAAiB,CAACE,QAAlB,IAA8BF,iBAAiB,CAACG,SAAlB,CAA4BD,QAA9D,EAAwE;AACpEP,IAAAA,MAAM,CAACC,GAAP,CAAWQ,MAAX;;AACAP,IAAAA,WAAW,CAACQ,OAAZ,CAAoBP,OAAO,CAAC,KAAD,CAA3B;;AAEA,QAAIQ,UAAU,GAAG,UAAjB;;AACA,QAAI,CAACX,MAAM,CAACC,GAAP,CAAWhB,IAAhB,EAAsB;AAClBiB,MAAAA,WAAW,CAACU,YAAZ,CAAyBD,UAAzB,EAAqCN,iBAArC;AACH,KAFD,MAGK;AACDH,MAAAA,WAAW,CAACW,SAAZ,CAAsBF,UAAtB,EAAkCN,iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * @file 组件 WujiRate 无极-评分\n * @author baozhixin <baozhixin@baidu.com>\n */\n\nimport './style/index.less';\nimport {Component, DataTypes} from 'san';\nimport Icon from '../wuji-icon';\n\nimport '@baidu/wuji-icons/svg/score-star-f-30.svg';\nimport '@baidu/wuji-icons/svg/score-star-o-30.svg';\n\nconst prefixCls = 'w-rate';\n\nfunction getStarStatus(score, index, allowHalf) {\n    if (+score >= index) {\n        return 'full';\n    }\n\n    if (Math.round(score) >= index && !allowHalf) {\n        return 'full';\n    }\n\n    if (+score + 0.5 >= index && allowHalf) {\n        return 'half';\n    }\n\n    return 'void';\n}\n\nexport default class WujiRate extends Component {\n    static template = /* html */ `\n        <div class=\"{{wrapClass}}\">\n            <div\n                s-if=\"text && textPosition === 'left'\"\n                class=\"${prefixCls}-text\"\n            >{{text}}</div>\n            <div class=\"${prefixCls}-list\">\n                <div\n                    s-for=\"star, index in list\"\n                    class=\"${prefixCls}-item\"\n                    on-click=\"select(index)\"\n                >\n                    <wuji-icon\n                        s-if=\"star !== 'full'\"\n                        type=\"score-star-o-30\"\n                    />\n                    <wuji-icon\n                        s-if=\"star !== 'void'\"\n                        type=\"score-star-f-30\"\n                        class=\"{{star === 'half' && '${prefixCls}-half'}}\"\n                    />\n                </div>\n            </div>\n            <div\n                s-if=\"text && textPosition === 'right'\"\n                class=\"${prefixCls}-text\"\n            >{{text}}</div>\n        </div>\n    `;\n\n    static components = {\n        'wuji-icon': Icon,\n    };\n\n    static dataTypes = {\n        allowHalf: DataTypes.bool,\n        count: DataTypes.number,\n        disabled: DataTypes.bool,\n        score: DataTypes.oneOfType([\n            DataTypes.number,\n            DataTypes.string,\n        ]),\n        size: DataTypes.oneOf([\n            'sm',\n            'md',\n        ]),\n        text: DataTypes.string,\n        textPosition: DataTypes.oneOf([\n            'left',\n            'right',\n        ]),\n    };\n\n    static computed = {\n        list() {\n            const data = this.data;\n            const count = data.get('count');\n            const score = data.get('score');\n            const allowHalf = data.get('allowHalf');\n\n            const list = [];\n            for (let i = 1; i <= count; i++) {\n                list.push(getStarStatus(score, i, allowHalf));\n            }\n\n            return list;\n        },\n        wrapClass() {\n            const size = this.data.get('size');\n            return [\n                `${prefixCls}-wrap`,\n                `${prefixCls}-${size}`,\n            ];\n        },\n    };\n\n    initData() {\n        return {\n            allowHalf: true,\n            count: 5,\n            disabled: false,\n            size: 'sm',\n            score: 0,\n            text: '',\n            textPosition: 'right',\n        };\n    }\n\n    select(index) {\n        const disabled = this.data.get('disabled');\n        if (disabled) {\n            return;\n        }\n\n        this.data.set('score', index + 1);\n    }\n}\n"]}]}