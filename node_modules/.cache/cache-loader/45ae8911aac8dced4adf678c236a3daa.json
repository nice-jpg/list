F,YAAT,GAAwB,CAAjD;;AACA,YAAA,KAAI,CAACM,IAAL,CAAUC,GAAV,CAAc,eAAd,EAA+BN,SAAS,GAAGI,WAA3C;AACH,WAND;AAOH;AACJ,OAVD;AAWH;;;0BAEKG,K,EAAO;AACT,WAAKF,IAAL,CAAUC,GAAV,CAAc,QAAd,EAAwB,KAAxB;AACA,WAAKE,IAAL,CAAU,OAAV,EAAmB;AAACD,QAAAA,KAAK,EAALA;AAAD,OAAnB;AACH;;;uCAE4B;AAAA,UAAfA,KAAe,QAAfA,KAAe;AAAA,UAARf,KAAQ,QAARA,KAAQ;AACzB,WAAKa,IAAL,CAAUC,GAAV,CAAc,OAAd,EAAuBd,KAAvB;AACA,WAAKgB,IAAL,CAAU,QAAV,EAAoB;AAACD,QAAAA,KAAK,EAALA,KAAD;AAAQf,QAAAA,KAAK,EAALA;AAAR,OAApB;AACH;;;6CAE0B;AAAA,UAARe,KAAQ,SAARA,KAAQ;AACvB,WAAKE,KAAL,CAAWF,KAAX;AACH;;;EAnFkCG,c;;;8BAAlBnB,S;AACC;iCACAD,S,gDACIA,S,oFACAA,S,kBAAiBA,S,6FACMA,S,yDACfA,S,mWAOAA,S,6EACAA,S,qRAKJA,S;8BAnBTC,S,gBAwCG;AAChB,eAAaoB,iBADG;AAEhB,eAAaC;AAFG,C;8BAxCHrB,S,eA6CE;AACfU,EAAAA,KAAK,EAAEY,eAAUC,MADF;AAEfC,EAAAA,IAAI,EAAEF,eAAUC;AAFD,C;;AA5DnB,IAAIE,MAAM,CAACC,GAAX,EAAgB;AACZ,MAAIC,WAAW,GAAGC,OAAO,CAAC,+HAAD,CAAzB;;AACA,MAAIC,aAAa,GAAGD,OAAO,CAAC,gHAAD,CAA3B;;AAEA,MAAIE,iBAAiB,GAAGD,aAAa,CAACE,UAAd,CAAyBN,MAAzB,CAAxB;;AACA,MAAIK,iBAAiB,CAACE,QAAlB,IAA8BF,iBAAiB,CAACG,SAAlB,CAA4BD,QAA9D,EAAwE;AACpEP,IAAAA,MAAM,CAACC,GAAP,CAAWQ,MAAX;;AACAP,IAAAA,WAAW,CAACQ,OAAZ,CAAoBP,OAAO,CAAC,KAAD,CAA3B;;AAEA,QAAIQ,UAAU,GAAG,UAAjB;;AACA,QAAI,CAACX,MAAM,CAACC,GAAP,CAAWZ,IAAhB,EAAsB;AAClBa,MAAAA,WAAW,CAACU,YAAZ,CAAyBD,UAAzB,EAAqCN,iBAArC;AACH,KAFD,MAGK;AACDH,MAAAA,WAAW,CAACW,SAAZ,CAAsBF,UAAtB,EAAkCN,iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * @file 组件 WujiPopup 无极-弹窗\n * @author yangmei03 <yangmei03@baidu.com>\n *\n * TODO: 代码风格和一些实现细节都需要优化\n */\n\nimport './style/index.less';\nimport {Component, DataTypes} from 'san';\nimport Icon from '../wuji-icon';\nimport List from './list';\nimport '@baidu/wuji-icons/svg/popupmenu-close-o-48.svg';\nimport '@baidu/wuji-icons/svg/arrow-back-o-48.svg';\n\nconst prefixCls = 'w-popup';\n\nexport default class WujiPopup extends Component {\n    static template = /* html */ `\n        <div class=\"${prefixCls}-wrap\">\n            <div class=\"${prefixCls}-mask\" on-click=\"close\" s-if=\"isOpen\"/>\n            <div class=\"${prefixCls}-box ${prefixCls}-{{type}}\" s-if=\"isOpen\">\n                <div s-if=\"showToolbar\" class=\"${prefixCls}-title\">\n                    <div class=\"${prefixCls}-left\" on-click=\"close\">\n                        <wuji-icon\n                            s-if=\"type === 'answer'\"\n                            type=\"arrow-back-o-48\"\n                        />\n                        <div s-if=\"type === 'calendar'\">取消</div>\n                    </div>\n                    <div class=\"${prefixCls}-title-text\">{{title}}</div>\n                    <div class=\"${prefixCls}-right\" on-click=\"close\">\n                        <div s-if=\"type === 'calendar'\">完成</div>\n                        <wuji-icon s-else type=\"popupmenu-close-o-48\"/>\n                    </div>\n                </div>\n                <div class=\"${prefixCls}-content\" style=\"max-height: {{contentHeight}}px\">\n                    <span s-if=\"type === 'text'\">{{content}}</span>\n                    <img\n                        s-else-if=\"type === 'image'\"\n                        src=\"{{imageUrl}}\"\n                        class=\"image-box\"\n                    />\n                    <wuji-list\n                        s-else\n                        options=\"{{options}}\"\n                        value=\"{{value}}\"\n                        type=\"{{type}}\"\n                        buttonText=\"{{buttonText}}\"\n                        on-select=\"handleSelect\"\n                        on-button-click=\"handleButtonClick\"\n                    />\n                </div>\n            </div>\n        </div>\n    `;\n\n    static components = {\n        'wuji-icon': Icon,\n        'wuji-list': List,\n    };\n\n    static dataTypes = {\n        title: DataTypes.string,\n        type: DataTypes.string,\n    };\n\n    initData() {\n        return {\n            value: '',\n            content: '',\n        };\n    }\n\n    attached() {\n        this.watch('isOpen', value => {\n            if (value) {\n                this.nextTick(() => {\n                    const windowHeight = document.documentElement.offsetHeight;\n                    const boxHeight = windowHeight * 0.7;\n                    const title = this.el.querySelector(`.${prefixCls}-title`);\n                    const titleHeight = title ? title.offsetHeight : 0;\n                    this.data.set('contentHeight', boxHeight - titleHeight);\n                });\n            }\n        });\n    }\n\n    close(event) {\n        this.data.set('isOpen', false);\n        this.fire('close', {event});\n    }\n\n    handleSelect({event, value}) {\n        this.data.set('value', value);\n        this.fire('select', {event, value});\n    }\n\n    handleButtonClick({event}) {\n        this.close(event);\n    }\n}\n"]}]}