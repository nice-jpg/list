urcesContent":["/**\n * @file 组件 WujiSingleHead 单头像\n * @author raowenjuan <raowenjuan@baidu.com>\n */\n\nimport './style/index.less';\nimport {Component} from 'san';\nimport WujiAvatar from '../wuji-avatar';\n\nconst prefixCls = 'w-single-head';\n\nexport default class WujiSingleHead extends Component {\n    static template = /* html */ `\n        <div class=\"${prefixCls}\" on-click=\"handleClick\">\n            <div class=\"${prefixCls}-wrap\">\n                <wuji-avatar class=\"avatar-wrap\" src=\"{{icon}}\" size=\"{{size}}\"/>\n                <div class=\"${prefixCls}-title-wrap\">\n                    <span class=\"${prefixCls}-title\">{{title}}</span>\n                </div>\n                <div class=\"${prefixCls}-text-wrap\">\n                    <span s-if=\"text1\" class=\"${prefixCls}-text1 {{text1.color}}\">{{text1.text}}</span>\n                    <span\n                        s-if=\"text2 && text2.text\"\n                        class=\"${prefixCls}-text2 {{text2.color}}\"\n                    >{{text2.text}}</span>\n                </div>\n            </div>\n            <span class=\"${prefixCls}-button\">{{btnText}}</span>\n        </div>\n    `;\n\n    static components = {\n        'wuji-avatar': WujiAvatar,\n    };\n\n    initData() {\n        return {\n            size: 66,\n        };\n    }\n\n    handleClick() {\n        console.log('dom clicked!'); // eslint-disable-line\n    }\n}\n"]}]}