AAO,CAAC,gHAAD,CAA3B;;AAEA,MAAIE,iBAAiB,GAAGD,aAAa,CAACE,UAAd,CAAyBN,MAAzB,CAAxB;;AACA,MAAIK,iBAAiB,CAACE,QAAlB,IAA8BF,iBAAiB,CAACG,SAAlB,CAA4BD,QAA9D,EAAwE;AACpEP,IAAAA,MAAM,CAACC,GAAP,CAAWQ,MAAX;;AACAP,IAAAA,WAAW,CAACQ,OAAZ,CAAoBP,OAAO,CAAC,KAAD,CAA3B;;AAEA,QAAIQ,UAAU,GAAG,UAAjB;;AACA,QAAI,CAACX,MAAM,CAACC,GAAP,CAAW5F,IAAhB,EAAsB;AAClB6F,MAAAA,WAAW,CAACU,YAAZ,CAAyBD,UAAzB,EAAqCN,iBAArC;AACH,KAFD,MAGK;AACDH,MAAAA,WAAW,CAACW,SAAZ,CAAsBF,UAAtB,EAAkCN,iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * @file 疫情卡组件\n * @author liuyuxin06@baidu.com\n */\n/* eslint-disable fecs-camelcase */\nimport {Component} from 'san';\nimport {env} from '@baidu/auc-base/env';\nimport invoke from '@baidu/xbox/na/invoke';\nimport s from './numeral.module.less';\nimport invokeScheme from '../../utils/invokeScheme';\n\nexport default class Numeral extends Component {\n    /* eslint-disable max-len */\n    static template = `\n        <div class=\"{{s['numeral']}}\">\n            <div class=\"{{s['numeral-container']}}\">\n                <template s-for=\"item, index in feed.items\">\n                    <div s-if=\"{{index === 8}}\" class=\"{{s['numeral-divider']}}\"></div>\n                    <div style=\"{{index | getNumeralItemStyle(tabType, itemCount)}}\"\n                        class=\"{{s['numeral-item']}}\"\n                        on-click=\"onTabItemClick($event, tabItem, index)\">\n                        <span class=\"{{s['numeral-item-text']}}\">{{item.text}}</span>\n                        <span class=\"{{s['numeral-item-value']}}\"\n                            style=\"color: {{index | getValueColor(theme, item.text, item.color, item.color_night)}};\n                                {{valueFlag | getNewFontSize}} {{extraStyle}} {{IS_IOS? 'margin-top: .1875rem;': ''}}\"\n                        >{{item.value || 0}}</span>\n                        <div s-if=\"{{canShowAddValue && (item.addText || item.addValue)}}\"\n                            class=\"{{s['numeral-item-addinfo']}}\"\n                            style=\"{{addValueFlag | getAddValueFontSize}}\">\n                            <span s-if=\"{{item.addText}}\" class=\"{{s['numeral-item-addText']}}\">{{item.addText}}</span>\n                            <span s-if=\"{{item.addText && item.addValue}}\" class=\"{{s['numeral-item-addValue']}}\"\n                                style=\"color: {{index | getValueColor(theme, item.text, item.color, item.color_night)}}\"\n                            >{{item.addValue}}</span>\n                        </div>\n                    </div>\n                </template>\n            </div>\n            <div class=\"{{s['numeral-bottom']}}\">\n                <div class=\"{{s['numeral-bottom-extra']}}\">\n                    <span class=\"{{s['numeral-bottom-extra-updatetime']}}\">{{tipInfo}}</span>\n                    <span s-if=\"explain\" class=\"{{s['numeral-bottom-extra-explain']}}\">{{explain}}</span>\n                </div>\n            </div>\n        </div>\n    `;\n    /* eslint-enable max-len */\n    static components = {\n    };\n    static filters = {\n        getValueColor(index, theme, text, customColor, customColorNight) {\n            if (customColorNight && 'kangyi-night' === theme) {\n                return customColorNight;\n            }\n            if (customColor) {\n                return customColor;\n            }\n            let color = '#10AEB5';\n            switch (+index) {\n                case 0:\n                    color = 'night' !== theme ? '#FF6A57' : '#7F342B';\n                    break;\n                case 1:\n                    color = 'night' !== theme ? '#E86D48' : '#79251F';\n                    break;\n                case 2:\n                    color = 'night' !== theme ? '#EC9217' : '#75480B';\n                    break;\n                case 3:\n                    color = 'night' !== theme ? '#10AEB5' : '#07565A';\n                    break;\n                case 4:\n                    color = 'night' !== theme ? '#476DA0' : '#23364F';\n                    break;\n                case 5:\n                    color = 'night' !== theme ? '#4D5054' : '#3C4045';\n                    break;\n                default:\n                    break;\n            }\n            return color;\n        },\n        getNewFontSize(valueFlag) {\n            const IS_IOS = this.data.get('IS_IOS');\n            const hasHundredMillion = this.data.get('hasHundredMillion');\n            const smallScreen = this.data.get('smallScreen');\n            switch (+valueFlag) {\n                case 1:\n                    return !smallScreen\n                        ? 'font-size:1.3125rem;'\n                        : 'font-size:1.18125rem;';\n                case 2:\n                    return !smallScreen\n                        ? 'font-size:1.125rem;'\n                        : 'font-size:1.0125rem;';\n                case 3: {\n                    let styleForHM = 'line-height:1rem;padding: 1.6px 0;';\n                    let smallStyleForHM = 'line-height:.9375rem;padding: 1.7px 0;';\n                    return !smallScreen\n                        ? `font-size:1rem;${!IS_IOS && hasHundredMillion ? styleForHM : ''}`\n                        : `font-size:.9375rem;${!IS_IOS && hasHundredMillion ? smallStyleForHM : ''}`;\n                }\n                default:\n                    break;\n            }\n            return '';\n        },\n        getAddValueFontSize(addValueFlag) {\n            const smallScreen = this.data.get('smallScreen');\n            switch (+addValueFlag) {\n                case 0:\n                    return !smallScreen\n                        ? 'font-size: 0.875rem;line-height:0.875rem;'\n                        : 'font-size: 0.7875rem;line-height:0.7875rem;';\n                case 1:\n                    return 'font-size: 0.75rem;line-height:0.75rem;';\n                case 2:\n                    return 'font-size: 0.6875rem;line-height:0.6875rem;';\n                case 3:\n                    const IS_IOS = this.data.get('IS_IOS');\n                    const screenWidth = document.documentElement.clientWidth;\n                    const itemCount = this.data.get('itemCount');\n                    // 由于昨日+ 文案调整，变为 较昨日+，ios端375px屏幕下会出现四列时数字折行的情况\n                    // 所以此处做下兼容\n                    return IS_IOS && screenWidth <= 375 && itemCount === 4\n                        ? 'font-size: 0.61875rem;line-height:0.61875rem;'\n                        : 'font-size: 0.6875rem;line-height:0.6875rem;';\n                default:\n                    break;\n            }\n            return '';\n        },\n        getNumeralItemStyle(index, tabType, itemCount) {\n            let style = '';\n            const itemLen = this.data.get('feed.items').length;\n            const containerWidth = this.data.get('containerWidth');\n            const rowNum = Math.ceil(itemLen / itemCount);// item行数\n            if (+tabType === 1 && rowNum >= 3) {\n                // 国内疫情下，优化后最后一行增加底部间距，倒数第二行增加顶部间距，并在中间增加分割线\n                if (index >= itemCount && index < (rowNum - 1) * itemCount) {\n                    style += 'margin-top:1.0625rem;margin-bottom: .75rem;';\n                } else if (index >= (rowNum - 1) * itemCount) {\n                    style += `padding-top: .8125rem;`;\n                } else {\n                    style += 'margin-top:1.125rem;';\n                }\n            } else {\n                style += `margin-top:${index < itemCount ? '1.125rem' : '1.0625rem'};`;\n            }\n            if ((+tabType === 1 || +tabType === 2) && itemCount === 4) {\n                const pureIndex = index % itemCount;\n                // 本地和国外tab下div比例调整\n                let ratio = 0.25;\n                switch (pureIndex) {\n                    case 0:\n                        ratio = 0.26;\n                        break;\n                    case 1:\n                        ratio = 0.30;\n                        break;\n                    case 2:\n                        ratio = 0.20;\n                        break;\n                    case 3:\n                        ratio = 0.24;\n                        break;\n                }\n                style += `width: ${Math.floor(containerWidth * ratio)}px;`\n            } else {\n                const IS_IOS = this.data.get('IS_IOS');\n                const paddding = IS_IOS ? 12 : 0.75 * 14.4;\n                style += `width: ${(containerWidth - paddding * 2) / itemCount}px;`;\n            }\n            return style;\n        }\n    }\n\n    static computed = {\n        smallScreen() {\n            const IS_IOS = this.data.get('IS_IOS');\n            let smallScreen = false;\n            const screenWidth = document.documentElement.clientWidth;\n            if (IS_IOS) {\n                screenWidth <= 320 && (smallScreen = true);\n            } else {\n                screenWidth < 360 && (smallScreen = true);\n            }\n            return smallScreen;\n        },\n        itemCount() {\n            let count = 3;\n            const itemArr = this.data.get('feed.items') || [];\n            const itemsLen = itemArr.length;\n            if (itemsLen % 4 === 0\n                || (itemsLen > 3 && (itemsLen % 3 === 1 || itemsLen % 4 >= 3 || itemsLen % 3 === itemsLen % 4))) {\n                count = 4;\n            }\n            return count;\n        },\n        canShowAddValue() {\n            const tabType = this.data.get('tabType');\n            const tabTitle = this.data.get('tabTitle');\n            return +tabType !== 2 || /^(香港|澳门|台湾)疫情/.test(tabTitle);\n        },\n        flexStyle() {\n            const itemCount = this.data.get('itemCount');\n            let percent = parseInt(100 / itemCount, 10);\n            return `flex: 1 1 ${percent}%;`;\n        },\n        containerWidth() {\n            let space = env.isIOS ? 15 : 12;\n            return document.documentElement.clientWidth - space * 2;\n        },\n        hasHundredMillion() {\n            const itemArr = this.data.get('feed.items') || [];\n            const itemsLength = itemArr.length;\n            for (let i = 0; i < itemsLength; i++) {\n                const value = itemArr[i].value;\n                if (value.indexOf('亿') !== -1) {\n                    return true;\n                }\n            }\n            return false;\n        },\n        hasHundredMillionNum() {\n            const itemArr = this.data.get('feed.items') || [];\n            const itemsLength = itemArr.length;\n            for (let i = 0; i < itemsLength; i++) {\n                const initValue = itemArr[i].value || '';\n                const value = Number(initValue.replace(/\\,/g, '')) || 0;\n                if (value >= 1000000000 && initValue.indexOf(',') !== -1) {\n                    return true;\n                }\n            }\n            return false;\n        },\n        valueFlag() {\n            let flag = 0;\n            const itemArr = this.data.get('feed.items') || [];\n            const itemsLength = itemArr.length;\n            if (itemsLength > 0) {\n                let isFlag1 = 0;\n                let isFlag2 = 0;\n                let isFlag3 = 0;\n                for (let i = 0; i < itemsLength; i++) {\n                    const initValue = itemArr[i].value || '';\n                    const value = Number(initValue.replace(/\\,/g, '')) || 0;\n                    if (value >= 10000000) { // 千万级\n                        isFlag3 = 1;\n                    }\n                    else if (value >= 1000000) { // 百万级\n                        isFlag2 = 1;\n                    }\n                    else if (value >= 100000) { // 十万级\n                        isFlag1 = 1;\n                    }\n                    if (i === itemsLength - 1) {\n                        if (isFlag1 === 1) {\n                            flag = 1;\n                        }\n                        if (isFlag2 === 1) {\n                            flag = 2;\n                        }\n                        if (isFlag3 === 1) {\n                            flag = 3;\n                        }\n                    }\n                }\n            }\n            return flag;\n        },\n        addValueFlag() {\n            let addFlag = 0; // addFlag: 0表示三位数及以下，1表示四位数，2表示5位数, 3表示六位数及以上\n            const addItemArr = this.data.get('feed.items') || [];\n            if (addItemArr.length > 0) {\n                let isThousand = 0;\n                let overThousand = 0;\n                let overHundredThousand = 0;\n                for (let i = 0; i < addItemArr.length; i++) {\n                    const addVal = (addItemArr[i].addValue && addItemArr[i].addValue.toString()) || '';\n                    let plusIndex = addVal && addVal.trim().indexOf('+');\n                    let minusIndex = addVal && addVal.trim().indexOf('-');\n                    let addValNum = '';\n                    plusIndex === 0 && (addValNum = addVal.slice(1));\n                    minusIndex === 0 && (addValNum = addVal.slice(1));\n                    const value = Number(addValNum.replace(/\\,/g, '')) || 0;\n                    if (value >= 100000) {// 大于十万级\n                        overHundredThousand = 1;\n                    } else if (value >= 10000) { // 大于万级\n                        overThousand = 1;\n                    } else if (value >= 1000) { // 大于千级\n                        isThousand = 1;\n                    }\n                }\n                if (isThousand === 1) {\n                    addFlag = 1;\n                }\n                if (overThousand === 1) {\n                    addFlag = 2;\n                }\n                if (overHundredThousand === 1) {\n                    addFlag = 3;\n                }\n            }\n            return addFlag;\n        },\n        extraStyle() {\n            // 如果后端获取治愈率死亡率失败，那么会出现一行四个且包含亿级数据的情况\n            // 针对该情况做下字号的适配\n            const smallScreen = this.data.get('smallScreen');\n            const itemCount = this.data.get('itemCount');\n            const hasHundredMillionNum = this.data.get('hasHundredMillionNum');\n            return itemCount === 4 && hasHundredMillionNum && env.isIOS\n                ? `font-size: ${smallScreen ? 12 : 14}px;` : '';\n        },\n        updateTime() {\n            const useDate = new Date();\n            // 展示昨日的日期\n            const lastDate = new Date(useDate.getTime() - 24 * 60 * 60 * 1000);\n            const month = lastDate.getMonth();\n            const day = lastDate.getDate();\n            const monthFormat = (month + 1).toString().padStart(2, '0');\n            const dayFormat = day.toString().padStart(2, '0');\n            return `${monthFormat}.${dayFormat}`;\n        },\n        tipInfo() {\n            // 国内、本地tab下文案更新。国外tab文案维持线上\n            return +this.data.get('topTabSelectedId') === 2\n                ? `* 数据更新至${this.data.get('timeInfo')}`\n                : `新增数据统计周期为昨日${this.data.get('updateTime')} 0-24时`;\n        }\n    };\n\n    onTabItemClick(e, tabItem, index) {\n        e.stopPropagation();\n        /* eslint-disable max-len */\n        let schemeHead = 'baiduboxapp://v1/browser/open?url=';\n        /* eslint-enable max-len */\n        let cmd = this.data.get('feed.cmd');\n        cmd && invokeScheme(schemeHead + encodeURIComponent(cmd));\n    }\n    initData() {\n        return {\n            IS_IOS: env.isIOS,\n            s,\n            feed: {},\n            timeInfo: ''\n        };\n    }\n}\n\n"]}]}