CG,SAAlB,CAA4BD,QAA9D,EAAwE;AACpEP,IAAAA,MAAM,CAACC,GAAP,CAAWQ,MAAX;;AACAP,IAAAA,WAAW,CAACQ,OAAZ,CAAoBP,OAAO,CAAC,KAAD,CAA3B;;AAEA,QAAIQ,UAAU,GAAG,UAAjB;;AACA,QAAI,CAACX,MAAM,CAACC,GAAP,CAAWtD,IAAhB,EAAsB;AAClBuD,MAAAA,WAAW,CAACU,YAAZ,CAAyBD,UAAzB,EAAqCN,iBAArC;AACH,KAFD,MAGK;AACDH,MAAAA,WAAW,CAACW,SAAZ,CAAsBF,UAAtB,EAAkCN,iBAAlC;AACH;AACJ;AACJ","sourcesContent":["/**\n * 新闻栏目页面\n * @file yangmei(yangmei03@baidu.com)\n */\nimport '../store';\nimport './app.less';\nimport {Component} from 'san';\nimport {connect} from '@store';\nimport ubc from '@baidu/ubc-report-sdk';\nimport {env} from '@baidu/auc-base/env';\nimport concern from '@baidu/xbox-concern';\nimport {order} from '../service';\nimport versionCompare from '@baidu/xbox/version_compare';\nimport {getQueryString} from '@/lib/utils/getQueryString';\nimport {setShareInfo} from '../../../lib/utils/setShareInfo';\n// components\nimport TopCarousels from '../components/top-carousels';\nimport MiddleOrder from '../components/middle-order';\nimport MainIntroduce from '../components/main-introduction';\nimport Actor from '../components/actor-intro';\nimport GuideBottom from '../components/guide-bottom';\nimport boxx from '@baidu/auc-base/boxx';\n\n// 尝试关闭加载态\ntry {\n    env.isBoxSeries && boxx.call('commonUI.closeLoading');\n} catch (e) {\n    console.error(e);\n}\n\nexport default class App extends Component {\n    constructor(opt) {\n        super(opt);\n    }\n    static template = `\n        <div class=\"wrapper\" style=\"\">\n            <div class=\"top-wrapper\"></div>\n            <div class=\"maincontent-wrapper\">\n                <ui-carousels majorPrograms=\"{{majorPrograms}}\" on-clickInvoke=\"clickInvoke\"/>\n                <ui-order order=\"{{order}}\" on-orderClick=\"orderClick\"/>\n                <ui-actor focus=\"{{focus}}\" on-followClick=\"followClick\"/>\n                <ui-introduction introduce=\"{{introduce}}\"/>\n            </div>\n            <div class=\"bottom-wrapper\">\n                <ui-guide-bottom on-clickInvoke=\"clickInvoke\" jumpChannel=\"{{jumpChannel}}\"/>\n            </div>\n        </div>\n    `;\n    static components = {\n        'ui-carousels': TopCarousels,\n        'ui-order': MiddleOrder,\n        'ui-introduction': MainIntroduce,\n        'ui-actor': Actor,\n        'ui-guide-bottom': GuideBottom\n    };\n    attached() {\n        this.initConcern();\n        // 页面打点\n        ubc('pageLog.event', {\n            type: 'pageShow',\n            ext: {\n            }\n        });\n\n        setShareInfo({\n            title: '高燃来袭！百度自制烧脑综艺上线',\n            description: '在《决胜21天》中看懂博弈思维',\n            icon: 'https://b.bdstatic.com/searchbox/icms/searchbox/img/8003-logo.jpg',\n            link: window.location.href\n        });\n    }\n    initData() {\n        return {\n            initConcern: undefined\n\n        };\n    }\n    initLog() {\n        // 页面打点\n        ubc('pageLog.init', '13908', {\n            from: getQueryString('id') || '',\n            source: getQueryString('source') || 'other'\n        });\n    }\n    inited() {\n        this.initLog();\n    }\n    clickInvoke(urlCmd) {\n        this.actions.invoke({urlCmd});\n    }\n    initConcern() {\n        const initConcern = concern({\n            id: 'news-notice',\n            ops: {\n                type: 'stock',\n                isAladin: 0,\n                onLine: true\n            }\n        });\n        this.data.set('initConcern', initConcern);\n    }\n    followClick({index, item}) {\n        const initConcern = this.data.get('initConcern');\n        initConcern.subscribe({\n            type: item.followtype,\n            op_type: item.followed ? 'cancel' : 'add',\n            third_id: item.aumrid,\n            sfrom: 'sbox',\n            source: item.followtype\n        }, () => {\n            this.actions.updateFocusFollow({\n                index\n            });\n        }, (err) => {\n            console.error(JSON.stringify(err));\n        }, false);\n    }\n    orderClick() {\n        const pageData = this.data.get('pageData');\n        const orderData = this.data.get('order');\n        const orderStatus = orderData.subscribe_state;\n        if (!env.isBoxSeries) {\n            this.clickInvoke(pageData.target);\n            return;\n        }\n        if (!pageData.is_login) {\n            if (versionCompare(env.boxVersion, '12.2') < 0) {\n                boxx.call('commonUI.showToast', {\n                    type: '1',\n                    message: '登陆后才可预约，版本过低当前页面无法登陆'\n                });\n                return;\n            }\n            boxx.call('account.login', {\n                loginType: 'username',\n                showThirdLogin: '1',\n                loginSource: 'pay_landing',\n                normalizeAccount: '1',\n                success: data => {\n                    location.reload();\n                    pageData.is_login = 1;\n                }\n            });\n        } else {\n            order({\n                paUid: orderData.pa_uid,\n                topicId: orderData.topic_id,\n                subscribe: orderStatus ? 0 : 1\n            }).then(({errno, data}) => {\n                if (+errno) {\n                    boxx.call('commonUI.showToast', {\n                        type: '1',\n                        message: '操作失败，请重试'\n                    });\n                    return;\n                }\n                // 更新预约状态\n                this.actions.updateOrder({\n                    data: {\n                        subscribe_state: orderStatus ? 0 : 1,\n                        real_num: data.order_num\n                    }\n                });\n                // 预约提示toast\n                const message = !orderStatus\n                    ? '更新后订阅消息提醒已开启' : '已取消预约';\n                boxx.call('commonUI.showToast', {\n                    type: '1',\n                    message,\n                });\n            });\n        }\n    }\n}\nconnect.san({\n    pageData: 'pageData',\n    order: 'order',\n    focus: 'focus',\n    majorPrograms: 'majorPrograms',\n    jumpChannel: 'jumpChannel',\n    introduce: 'introduce'\n}, {\n    updateFocusFollow: 'main:updateFocusFollow',\n    invoke: 'invoke',\n    updateOrder: 'main:updateOrder'\n})(App);"]}]}