    </div>\n    `;\n    attached() {\n        this.swiper = new Swiper('.tabBar', {\n                direction: 'vertical',\n                autoplay: {\n                    delay: 4000\n                },\n                loop: true,\n                disableOnInteraction: false\n            });\n    }\n    initData() {\n        return {\n            styles\n        }\n    }\n\n    handleClick(item) {\n        /* eslint-disable max-len */\n        const schemeHead = 'baiduboxapp://v1/browser/open?url=';\n        /* eslint-enable max-len */\n        const cmd = item && item.url;\n        cmd && invokeScheme(schemeHead + encodeURIComponent(cmd));\n    }\n}\n\nexport default class Notice extends Component {\n    static template = `\n        <div class=\"{{styles.noticeMargin}}\">\n            <swiper-comp s-if=\"{{titleList.length > 1}}\"\n                titleList=\"{{titleList}}\">\n            </swiper-comp>\n            <div s-else class=\"{{styles.container}}\"\n                style=\"display: flex; flex-direction: row;\">\n                <div class=\"{{styles.pureImg}}\"></div>\n                <div class=\"{{styles.content}} {{styles.singleWrap}}\"\n                    on-click=\"handleClick\">\n                    <span class=\"{{styles.title}}\">{{titleList[0].title}}</span>\n                </div>\n            </div>\n        </div>\n    `;\n    static components = {\n        'swiper-comp': SwiperComponent\n    }\n    static computed = {\n        titleList() {\n            const feed = this.data.get('feed');\n            return (feed && feed.items) || [];\n        },\n    }\n    initData() {\n        return {\n            styles\n        }\n    }\n    handleClick() {\n        /* eslint-disable max-len */\n        const schemeHead = 'baiduboxapp://v1/browser/open?url=';\n        /* eslint-enable max-len */\n        const titleList = this.data.get('titleList');\n        const cmd = titleList[0] && titleList[0].url;\n        cmd && invokeScheme(schemeHead + encodeURIComponent(cmd));\n    }\n}"]}]}