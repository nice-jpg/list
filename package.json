{
  "name": "multi-pages",
  "version": "1.0.0",
  "private": true,
  "description": "A San project",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:client": "cross-env NODE_ENV=development node --max-old-space-size=4096 ./node_modules/.bin/san serve --config build/san.client.config.js",
    "start:ssr": "cross-env NODE_ENV=development SSR=1 node --max-old-space-size=4096 ./node_modules/.bin/san serve --config build/san.client.config.js",
    "build": "rm -rf output/ && npm run build:ssr && npm run build:client",
    "build:client": "cross-env NODE_ENV=production node --max-old-space-size=4096 ./node_modules/.bin/san build --config build/san.client.config.js",
    "build:client:debug": "cross-env NODE_ENV=production DEBUG=1 node --max-old-space-size=4096 ./node_modules/.bin/san build --config build/san.client.config.js",
    "build:ssr": "cross-env NODE_ENV=production node --max-old-space-size=4096 ./node_modules/.bin/san build --config build/ssr.prod.config.js",
    "build:ssr:debug": "cross-env NODE_ENV=production DEBUG=1 node --max-old-space-size=4096 ./node_modules/.bin/san build --config build/ssr.prod.config.js",
    "analyzer:client": "cross-env NODE_ENV=production node --max-old-space-size=4096 ./node_modules/.bin/san build --config build/san.client.config.js --analyze --profile",
    "analyzer:ssr": "cross-env NODE_ENV=production node --max-old-space-size=4096 ./node_modules/.bin/san build --config build/ssr.prod.config.js --analyze"
  },
